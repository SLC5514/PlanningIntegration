                            {if $sGender eq 1}
                			<img class="hover-pic" src="{STATIC_URL2}/global/images/man-content.jpg" alt="pic" />
                            {elseif $sGender eq 2}
                            <img class="hover-pic" src="{STATIC_URL2}/global/images/woman-content.jpg" alt="pic" />
                            {else}
                            <img class="hover-pic" src="{STATIC_URL2}/global/images/child-content.jpg" alt="pic" />

						<a data-id="index_{$topic.iSubTopicId}" onclick="javascript:document.getElementById('index_{$topic.iSubTopicId}').scrollIntoView()" class="clear" title="{$topic.sTitle}">
							<i></i><span>{$topic.sTitle}</span>
						</a>
					</li>
					{/foreach}
				</ul>
				{else}
				<div class="cntsub-null">
					{foreach $res.subTopic as $key => $topic}
					<div class="clear">
						<i></i><span>{$topic.sTitle}</span>
					</div>
					{/foreach}
				</div>
				{/if}
			</div>
			{/if}
			<div class="shop-serve" style="display: none;">
				<div class="serve-title">
					<i></i>商务服务
				</div>
				<ul>
				</ul>
			</div>
		</div>
	</div>

        <div class="con_width">
            <div class="swiper-container-banner">
                <ul class="roundabout-holder js-swiper-wrapper-banner">
                    {foreach $banner as $value}
                    <li class="roundabout-moveable-item">
                        <a href="{$value.sUrl}" target="_blank" title="{$value.sTitle}" data-growing-title="{$value.sTitle}">
                            <img src="{STATIC_URL1}{$value.sImagePath}"/>
                        </a>
                    </li>
                    {/foreach}
                    {if count($banner)==1}
                    <li class="roundabout-moveable-item">
                        <a href="/styles/" target="_blank" data-growing-title="海量款式" title="海量款式">
                            <img src="{STATIC_URL1}/global/images/fashion_home/banner1.jpg"/>
                        </a>
                    </li>
                    <li class="roundabout-moveable-item">
                        <a href="/trends/" target="_blank" data-growing-title="趋势流行报告" title="趋势流行报告">
                            <img src="{STATIC_URL1}/global/images/fashion_home/banner2.jpg"/>
                        </a>
                    </li>
                    {/if}
                    {if count($banner)==2}
                    <li class="roundabout-moveable-item">
                        <a href="/styles/" target="_blank" data-growing-title="海量款式" title="海量款式">
                            <img src="{STATIC_URL1}/global/images/fashion_home/banner1.jpg"/>
                        </a>
                    </li>
                    {/if}
                </ul>
                <a class="swiper-btn-switch swiper-prev js-swiper-prev"></a>

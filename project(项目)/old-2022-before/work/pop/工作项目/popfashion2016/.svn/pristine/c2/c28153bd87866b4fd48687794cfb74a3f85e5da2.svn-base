{if in_array($column_id, ['all', 1, 2])}
<div class="item">
    {foreach $matchs as $id => $name}
        <span class="{if $filter.match_id == $id}action{/if} js-jump" data-name="match" data-id="{$id}">{$name}</span>
    {/foreach}
</div>
{/if}

<div class="select_items">
    {if $tips}
        {if $tips.gen.sub}
            <div class="select_item {if $filter.gen_id}action_item{/if}">
                <span data-id="{$filter.gen_id}">性别 ：{$filter.gen}</span>
                <i></i>
                <div class="list">
                    <div class="contentHolder">
                        <ul class="content">
                            {foreach $tips.gen.sub as $item}
                            <li class="js-jump" data-id="{$item.key}" data-name="gen">{$item.value}</li>
                            {/foreach}
                        </ul>
                    </div>
                </div>
            </div>
            {if in_array($column_id, [82,120])}
                <span class="{if $column_id == 120}filter{/if} js-jump"   data-id="{if $column_id == 120}82{else}120{/if}" data-name="col">只看大牌花型</span>
            {/if}
        {/if}
        {if $tips.ind.sub}
        <div class="select_item {if $filter.ind_id}action_item{/if}">
            <span data-id="{$filter.ind_id}">行业 ：{$filter.ind}</span>
            <i></i>
            <div class="list">
                <div class="contentHolder">
                    <ul class="content">
                        {foreach $tips.ind.sub as $item}
                        <li class="js-jump" data-id="{$item.key}" data-name="ind">{$item.value}</li>
                        {/foreach}
                    </ul>
                </div>
            </div>
        </div>
        {/if}
    {/if}
    {if $filter.sea_id == ''}
        {if $iSeason}
        <div class="select_item sea">
            <span data-id="{$filter.sea_id}">季节 ：{$filter.sea}</span>
            <i></i>
            <div class="list">
                <div class="contentHolder">
                    <ul class="content"> 
                        {foreach $iSeason as $key => $val}
                        <li class="js-jump" data-id="{$val.id}" data-name="sea" data-url="{$val.link}">{$val.name}</li>
                        {/foreach}
                    </ul>
                </div>
            </div>
        </div>
        {/if}
    {else}
        {if !in_array($column_id, [82,120])}
        <div class="select_item sea active1">
            <span class="js-jump" data-name="sea" data-id="all">季节 ：{$filter.sea}</span>
            <i class="js-jump" data-name="sea" data-id="all"></i>
        </div>
        {/if}
    {/if}
</div>

<!-- 下拉 -->
<div class="head_slide js-head-slide">
    <div class="slide_main">
        <div class="fl">
            <a href="/"><img src="{STATIC_URL2}/global/images/lists/nav_logo.png?{STATIC_CHANGE_TIME}"></a>
        </div>
        <div class="item_box fl">
            <span>{$filter.column_name}</span>
            <span data-id="{$column_id}" class="js-all-num"></span>
            <i></i>
            <div class="serach_box">
                <ul>
                    {foreach $columnArr as $id =>$name}
                    <li class="js-jump" data-name="col" data-id="{$id}">{$name}</li>
                    {/foreach}
                </ul>
            </div>
        </div>
        {if $tips}
            {if $tips.gen.sub}
            <div class="item_list {if $filter.gen_id}action{/if}">
                <span data-id="{$filter.gen_id}">性别 ：<label>{$filter.gen}</label></span>
                <i></i>
                <div class="drop">
                    <div class="contentHolder">
                        <ul class="content">
                            {foreach $tips.gen.sub as $item}
                            <li class="js-jump" data-id="{$item.key}" data-name="gen">{$item.value}</li>
                            {/foreach}
                        </ul>
                    </div>
                </div>
            </div>
            {/if}

            {if $tips.ind.sub}
            <div class="seg"></div>
                <div class="item_list {if $filter.ind_id}action{/if}">
                    <span data-id="{$filter.ind_id}">行业 ：<label>{$filter.ind}</label></span>
                    <i></i>
                    <div class="drop">
                        <div class="contentHolder">
                            <ul class="content">
                                {foreach $tips.ind.sub as $item}
                                <li class="js-jump" data-id="{$item.key}" data-name="ind">{$item.value}</li>
                                {/foreach}
                            </ul>
                        </div>
                    </div>
                </div>
            {/if}
        {/if}
        {if $filter.sea_id == ''}
            {if $iSeason}
            <div class="seg"></div>
            <div class="item_list">
                <span data-id="{$filter.sea_id}">季节 ：<label>{$filter.sea}</label></span>
                <i></i>
                <div class="drop">
                    <div class="contentHolder">
                        <ul class="content">
                            {foreach $iSeason as $key => $val}
                            <li class="js-jump" data-id="{$val.id}" data-name="sea" data-url="{$val.link}">{$val.name}</li>
                            {/foreach}
                        </ul>
                    </div>
                </div>
            </div>
            {/if}
        {else}
            {if !in_array($column_id, [82,120])}
            <div class="seg"></div>
            <div class="item_list item_active">
                <span class="js-jump" data-name="sea" data-id="all">季节 ：<label>{$filter.sea}</label></span>
                <i class="js-jump" data-name="sea" data-id="all"></i>
            </div>
            {/if}
        {/if}
        <div class="serach_text">
            <input id="search_box1" class="js-search-ipt" value="{$keyword}" growing-track="true" placeholder="搜索你想要的" type="text" οnfοcus="this.placeholder=''"
                οnblur="this.placeholder='搜索你想要的'" autocomplete='off'>
            <div class="search_listDown search_page_listDown">
                <div class="menu"></div>
                <div class="hot-menu">
                    <div class="menu-hot-list">
                        <div class="label js-history-label">历史记录<div class="fr"><span class="operat js-clear-operat">清除记录</span></div></div>
                        <ul class="history-list"></ul>
                        <div class="label">热门搜索</div>
                        <ul class="hot-list"></ul>
                    </div>
                </div>
            </div>
            <!-- 上传图片 -->
            <div class="files files-sea js-figure-btn1"></div>
            <div class="btn_icon js-search-btn"></div>
        </div>
    </div>
</div>
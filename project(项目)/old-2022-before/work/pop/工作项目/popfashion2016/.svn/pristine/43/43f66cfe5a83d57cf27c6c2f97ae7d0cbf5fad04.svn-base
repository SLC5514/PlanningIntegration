/* 轮播 */
.sp-box{width: 872px;height: 240px;position: relative;box-sizing: border-box;-webkit-user-select: none;-moz-user-select: none;-ms-user-select: none;user-select: none;overflow: hidden;float: right;margin-right: 31px;border-radius: 5px;}
.sp-box.on{height: 240px;}
.sp-box .sp-cube {-webkit-transform: translateZ(0);-moz-transform: translateZ(0);-ms-transform: translateZ(0);-o-transform: translateZ(0);transform: translateZ(0);-webkit-backface-visibility: hidden;-moz-backface-visibility: hidden;-ms-backface-visibility: hidden;backface-visibility: hidden;-webkit-perspective: 1000;-moz-perspective: 1000;-ms-perspective: 1000;perspective: 1000;}
.sp-box .sp-wp{width: 100%;display: none;}
.sp-box .sp-wp.ts {-o-transition: margin-left 0.3s ease-out;-ms-transition: margin-left 0.3s ease-out;-moz-transition: margin-left 0.3s ease-out;-webkit-transition: margin-left 0.3s ease-out;transition: margin-left 0.3s ease-out;}
.sp-box .sp-wp.ts .sp-s{-o-transition: width height margin 0.3s ease-out;-ms-transition: width height margin 0.3s ease-out;-moz-transition: width height margin 0.3s ease-out;-webkit-transition: width height margin 0.3s ease-out;transition: width height margin 0.3s ease-out;}
.sp-box .sp-s{float: left;width: 0;height: 0;box-sizing: border-box;position: relative;}
.sp-box .sp-gb{width: 100%;height: 100%;position: relative;overflow: hidden;}
.sp-box .sp-gb>img{opacity: 0.1;}
.sp-box .sp-gb>.gb-blur {opacity: 0.6;position: absolute;left: 0;top: 0;-webkit-transform: translateZ(0);transform: translateZ(0);filter: url(blur-7px.svg#blur);-webkit-filter: blur(7px);-moz-filter: blur(7px);filter: blur(7px);filter: progid:DXImageTransform.Microsoft.Blur(PixelRadius=7, MakeShadow=false);*left: -7px;*top: -7px;left: -7px\0;top: -7px\0;}
.sp-box :root .gb-blur {left: -7px\0;top: -7px\0;}
.sp-box .gb-blur image {-o-transition: all 0.3s ease-out;-ms-transition: all 0.3s ease-out;-moz-transition: all 0.3s ease-out;-webkit-transition: all 0.3s ease-out;transition: all 0.3s ease-out;}
.sp-box .sp-cp {position: absolute;overflow: hidden;background: #232F3C;-o-transition: width height top right 0.3s ease-out;-ms-transition: width height top right 0.3s ease-out;-moz-transition: width height top right 0.3s ease-out;-webkit-transition: width height top right 0.3s ease-out;transition: width height top right 0.3s ease-out;}
.sp-box .sp-msg {position: absolute;background: url(../../../images/new_common/txt_bg.png) no-repeat center center / cover;color: #1C232B;-o-transition: width height left top font-size 0.3s ease-out;-ms-transition: width height left top font-size 0.3s ease-out;-moz-transition: width height left top font-size 0.3s ease-out;-webkit-transition: width height left top font-size 0.3s ease-out;transition: width height left top font-size 0.3s ease-out;}
.sp-box .sp-msg span {width: 78%;overflow: hidden;word-break: break-all;word-wrap:break-word;display: inline-block;vertical-align: middle;letter-spacing: 2px;text-align: justify;margin-left: 11%;font-weight: bold;}
.sp-box .sp-sd-l, .sp-sd-r{width: 0;height: 100%;background-color: #141C26;opacity: 0.8;filter: alpha(opacity=80);position: absolute;top: 0;display: none;}
.sp-box .sp-sd-l{left: 0;}
.sp-box .sp-sd-r{right: 0;}
.sp-switch-box{position: absolute;right: 0;bottom: 0;border: 1px solid #eee;border-bottom: none;width: 20px;display: none;}
.sp-switch-box .sp-prev,.sp-switch-box .sp-next{width: 20px;height: 21px;cursor: pointer;border-bottom: 1px solid #eee;}
.sp-switch-box .sp-prev{background: url(../../../images/new_common/banner_l.png) no-repeat center;}
.sp-switch-box .sp-prev:hover{background: url(../../../images/new_common/banner_h_l.png) no-repeat center;}
.sp-switch-box .sp-next{background: url(../../../images/new_common/banner_r.png) no-repeat center;}
.sp-switch-box .sp-next:hover{background: url(../../../images/new_common/banner_h_r.png) no-repeat center;}
.sp-anit-time{width: 2px;height: 160px;background:rgba(0,0,0,0.1);border-radius: 1px;position: absolute;right: 10px;top: 27px;display: none;}
.sp-anit-time>span{width: 100%;height: 0;background:rgba(0,0,0,0.14);position: absolute;left: 0;top: 0;border-radius: 1px;}


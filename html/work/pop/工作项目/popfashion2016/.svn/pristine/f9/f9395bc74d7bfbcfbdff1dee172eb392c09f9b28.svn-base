.p4{padding: 145px 0 150px;}
.p4>h2{transition: all 0.4s linear;-ms-transition: all 0.4s linear;-moz-transition: all 0.4s linear;opacity: 0; background: url(../images/h3.png) center top no-repeat;width: 1000px;height: 180px;margin: 0 auto;position: relative;top: -350px; margin-bottom: 70px;}
.bott-msg{transition: all 0.9s ease-in-out 0.7s;-ms-transition: all 0.9s ease-in-out 0.7s;-moz-transition: all 0.9s ease-in-out 0.7s;opacity: 0;position: relative; width: 1060px;height: 514px;background: url(../images/bott.jpg) center top no-repeat;margin: 0 auto;}
.bott-msg>.bott-txt{font-size: 18px;color: #000;line-height: 40px;position: absolute;z-index: 2;bottom: 0;left: 0;width: 100%;text-align: center;padding-bottom: 32px;}

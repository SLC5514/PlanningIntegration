.drag-box{width: 100%;height: 34px;position: relative;text-align:center;line-height:34px;position: relative;background-color:#ccc;color:#333;}
.drag-box>span{}
.bg{width: 0%;height: 100%;background-color: #6fb147;font-size:12px;overflow: hidden;position: absolute;left:0;top:0;color:#fff;}
.drag-box>p{z-index: 10;position: absolute;left: 0px;top: 0px;width: 50px;height: 100%;display: inline-block;border-style: none;background:url(../../../images/user/d-drag.jpg) no-repeat;cursor: pointer;}
.drag-success>p{background:url(../../../images/user/succ.jpg) no-repeat;}

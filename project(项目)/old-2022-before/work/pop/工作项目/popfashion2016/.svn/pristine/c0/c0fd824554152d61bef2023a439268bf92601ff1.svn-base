							{include file="t_trend_report_layer.html"}
						{elseif $res.table=='specialtopic'}
							{include file="specialtopic_layer.html"}
						{elseif $res.table=='specialtopic_graphic'}
							{include file="specialtopic_graphic_layer.html"}
						{else}
							{include file="fashion_shows_layer.html"}
						{/if}

						{if !empty($res.vendorads)}

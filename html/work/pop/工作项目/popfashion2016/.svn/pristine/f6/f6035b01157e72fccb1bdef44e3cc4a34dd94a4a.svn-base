.section4>.con-rg>p{margin-left:115px;width:360px;font-size:16px;color:#333;margin-top:30px;line-height:27px;}
.section4>.con-rg>a{width:198px;height:40px;background-color:#505050;font-size:18px;color:#fff;display:inline-block;margin-left:115px;line-height:40px;text-align:center;margin-top:45px;position: relative;}
.section4>.con-rg>a>em{width:0;height:100%;position: absolute;left:0;top:0;background-color:#7592fe;transition:all 0.3s ease;}
.section4>.con-rg>a>span{position: relative;z-index:1;}
.section4>.con-rg>a>span>i{background:url(../images/arrow.png) no-repeat;width:21px;height:5px;display:inline-block;vertical-align:middle;margin-left:10px;margin-top:-2px;}
/* section4动画 */
.anit-section4>.con-lf{opacity:1;filter:alpha(opacity=100);transform:translateY(0);-webkit-transform:translateY(0);}
.anit-section4>.con-rg{opacity:1;filter:alpha(opacity=100);transform:translateY(0);-webkit-transform:translateY(0);}
.anit-section4>.con-lf>div{-webkit-transform: scale(1);transform: scale(1);opacity:1;filter:alpha(opacity=100);}
.anit-section4-next>.con-lf>div{-webkit-transform: scale(0);transform: scale(0);opacity:0;filter:alpha(opacity=0);-webkit-transition:all 0.5s ease;transition:all 0.5s ease;}
.anit-section4>.con-lf>ul>li.li1{opacity:1;filter:alpha(opacity=100);-webkit-transform: scale(1);transform: scale(1);}
.anit-section4>.con-lf>ul>li.li2{opacity:1;filter:alpha(opacity=100);-webkit-transform: scale(1);transform: scale(1);}
.anit-section4>.con-lf>ul>li.li3{opacity:1;filter:alpha(opacity=100);-webkit-transform: scale(1);transform: scale(1);}
.anit-section4>.con-lf>ul>li.li4{opacity:1;filter:alpha(opacity=100);-webkit-transform: scale(1);transform: scale(1);}
.section4>.con-rg>a:hover em{width:100%;}

/* section-add */
.section-add{padding-top:150px;padding-bottom:100px;}
.section-add>div>.con-lf{transform:translateY(120px);-webkit-transform:translateY(120px);opacity:0;filter:alpha(opacity=0);transition:all 0.5s ease;-webkit-transition:all 0.5s ease;}

input::-moz-placeholder{font-size: 14px;color: #ccc;}
input::-webkit-input-placeholder{font-size: 14px;color: #ccc;}
input::placeholder{font-size: 14px;color: #ccc;}
.toast{position: absolute;top: 215px;left: 50%;transform: translateX(-50%);background-color: rgba(0,0,0,0.7);padding: 0 20px;height: 50px;line-height: 50px;border-radius: 4px;font-size: 14px;color: #fff;z-index: 1;display: none;}

.contant{height: 1026px;background: url(../../images/user/vip_try_bg.jpg) no-repeat center / cover;padding-top: 60px;box-sizing: border-box;}
.main{width: 1200px;height: 905px;margin: 0 auto;background-color: #fefefe;border-radius: 15px;padding-top: 80px;box-sizing: border-box;}
.main>h1{font-size: 36px;line-height: 36px;letter-spacing: 2px;color: #232F3C;text-align: center;}
.main>.hline{width: 43px;height: 4px;background: #999;margin: 30px auto 0;}
.main>.advisory-btns{margin-top: 60px;text-align: center;font-size: 16px;}
.main>.advisory-btns>div{display: inline-block;width: 300px;height: 50px;line-height: 50px;border-radius: 4px;margin: 0 10px;}
.main>.advisory-btns>.tel-btn{border: 1px solid #232F3C;color: #333;}
.main>.advisory-btns>.tel-btn>span{color: #232F3C;font-weight: bold;}
.main>.advisory-btns>.vip-btn{background: #232F3C;color: #fff;cursor: pointer;}
.main>.advisory-btns>.vip-btn:hover{background: #4F5963;}
.main>.fromBox{width: 640px;margin: 55px auto 0;}
.main>.fromBox>ul>li{position: relative;margin-bottom: 45px;}
.main>.fromBox>ul>li label{font-size: 16px;color: #333;}
.main>.fromBox>ul>li label>i{color: #fb9e78;vertical-align: middle;}
.main>.fromBox>ul>li .iptBox{width: 100%;border-bottom: 1px solid #ccc;position: relative;}
.main>.fromBox>ul>li .iptBox>.ipt{width: 100%;height: 45px;line-height: 45px;padding-left: 10px;box-sizing: border-box;border: none;}
.main>.fromBox>ul>li .iptBox>.ipt.selt{font-size: 14px;color: #ccc;cursor: pointer;}
.main>.fromBox>ul>li .iptBox>.ipt.selt.on{color: #333;}
.main>.fromBox>ul>li .iptBox>.arrow{position: absolute;width: 35px;height: 45px;right: 0;bottom: 0;text-align: center;line-height: 45px;cursor: pointer;transition: transform 0.2s ease-out;}
.main>.fromBox>ul>li .iptBox>.arrow>i{display: inline-block;width: 15px;height: 9px;background: url(../../images/user/icon_down.jpg) no-repeat center / cover;}
.main>.fromBox>ul>li .iptBox>.arrow.on{transform: rotate(-180deg);}
.main>.fromBox>ul>li .options{position: absolute;left: 0;top: 70px;right: 0;z-index: 1;color: #999;box-shadow: 0px 0px 10px rgba(0,0,0,0.2);background: #fff;display: none;}
.main>.fromBox>ul>li .options>li{height: 34px;padding: 0 10px;line-height: 34px;cursor: pointer;font-size: 14px;}
.main>.fromBox>ul>li .options>li:hover{background: #eee;color: #666;}
.main>.fromBox>ul>li .info{font-size: 12px;line-height: 22px;color: #fb9e78;position: absolute;left: 10px;bottom: -22px;display: none;}
.main>.fromBox>ul>li .btn{width: 640px;height: 50px;line-height: 50px;text-align: center;background: #232f3c;border-radius: 4px;font-size: 16px;color: #fff;cursor: pointer;}
.main>.fromBox>ul>li .btn:hover{background: #4F5963;}
.advisory-alert-bg{position: fixed;top: 0;left: 0;bottom: 0;right: 0;background: rgba(0,0,0,0.5);display: none;}
.advisory-alert{position: fixed;top: 0;left: 0;bottom: 0;right: 0;display: none;}
.advisory-alert>.alert-content{width: 550px;height: 610px;position: absolute;top: 50%;left: 50%;margin-left: -275px;margin-top: -305px;border-radius: 15px;}
.advisory-alert>.alert-content>.close{width: 30px;height: 30px;position: absolute;right: -40px;top: -30px;background: url(../../images/user/icon-close.png) no-repeat center / cover;border-radius: 50%;cursor: pointer;}
.advisory-alert>.alert-content>.title{height: 90px;line-height: 90px;text-align: center;background-color: #232f3c;font-size: 30px;color: #f6f6f6;border-top-left-radius: 15px;border-top-right-radius: 15px;}
.advisory-alert>.alert-content>.cot{height: 520px;background: #fff;border-bottom-left-radius: 15px;border-bottom-right-radius: 15px;}
.advisory-alert>.alert-content>.cot>ul{padding-top: 30px;}
.advisory-alert>.alert-content>.cot>ul>li{width: 430px;height: 50px;line-height: 50px;text-align: center;margin: 20px auto 0;font-size: 16px;color: #232F3C;border: 1px solid transparent;border-radius: 4px;cursor: pointer;position: relative;background: #f1f1f1;}
.advisory-alert>.alert-content>.cot>ul>li:hover{border: 1px solid #232F3C;}
.advisory-alert>.alert-content>.cot>ul>li.on{border: 1px solid #232F3C;}
.advisory-alert>.alert-content>.cot>ul>li.on>i{position: absolute;right: 17px;top: 50%;width: 16px;height: 16px;margin-top: -8px;background: url(../../images/member/icon_comfirm.png) no-repeat center / cover;border-radius: 50%;}
.advisory-alert>.alert-content>.cot .btn{width: 430px;height: 50px;line-height: 50px;margin: 40px auto 0;background: #232F3C;font-size: 16px;color: #fff;text-align: center;border-radius: 4px;cursor: pointer;}
.advisory-alert>.alert-content>.cot .btn:hover{background: #4F5963;}
.advisory-alert>.alert-content>.cot .qqbtn{display: none;}

.h5-box{display: none;overflow: hidden;}
@media (min-width: 320px){html{font-size: 42.6667px;} }
@media (min-width: 375px){html{font-size: 50px;} }
@media (min-width: 425px){html{font-size: 56.6667px;} }
@media (min-width: 750px){html{font-size: 100px;} }
@media only screen and (max-width: 750px){
  a,div,ul,li{
    -webkit-tap-highlight-color:rgba(0,0,0,0);
    -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
    -webkit-user-select: none;
    -moz-user-focus: none;
    -moz-user-select: none;
    user-select: none;
  }
  html,body{min-width: 100%;}
  .pc-box{display: none;}
  .h5-box{/* display: block; */padding-top: 0.55rem;}
  .contant{height: auto;background: #fff;padding-top: 0;position: fixed;top: 0;bottom: 0;}
  .h5-box>.info{font-size: 0.28rem;color: #999;text-align: left;margin-bottom: 0.6rem;padding-left: 0.4rem;}
  .h5-box>.btns{text-align: center;white-space: nowrap;margin-bottom: 0.6rem;}
  .h5-box>.btns>.btn{width: 3.2rem;height: 0.8rem;line-height: 0.8rem;margin: 0 0.15rem;display: inline-block;cursor: pointer;background-image: linear-gradient(90deg, #FF6F7E 0%, #F9C374 86%, #FBDE5A 100%);box-shadow: 0 0.06rem 0.2rem 0.01rem rgba(253,133,123,0.40);border-radius: 2rem;}
  .h5-box>.btns>.btn>span{vertical-align: middle;font-size: 0.28rem;color: #fff;}
  .h5-box>.btns>.tel-btn>i{display: inline-block;vertical-align: middle;width: 0.4rem;height: 0.4rem;background: url(../../images/member/icon_tel_white.png) no-repeat center / cover;margin-right: 0.1rem;}
  .h5-box>.btns>.qq-btn>i{display: inline-block;vertical-align: middle;width: 0.4rem;height: 0.4rem;background: url(../../images/member/icon_qq_white.png) no-repeat center / cover;margin-right: 0.1rem;}
  .h5-box>.hline{width: 100%;height: 0.2rem;background: #F1F1F1;margin-bottom: 0.4rem;}
  .h5-box>.from-list{padding: 0 0.4rem;box-sizing: border-box;}
  .h5-box>.from-list>li{border-bottom: 0.01rem solid #F1F1F1;white-space: nowrap;margin-bottom: 0.3rem;line-height: 1rem;}
  .h5-box>.from-list>li>label{font-size: 0.28rem;color: #333;display: inline-block;width: 20%;margin-right: 0.36rem;line-height: 1rem;white-space: nowrap;}
  .h5-box>.from-list>li>.ipt-box{display: inline-block;position: relative;width: 74%;}
  .h5-box>.from-list>li>.ipt-box>.ipt{width: 100%;font-size: 0.26rem;color: #999;border: none;}
  .h5-box>.from-list>li>.ipt-box>.ipt.on{color: #333;}
  .h5-box>.from-list>li>.ipt-box>.ipt::placeholder{font-size: 0.26rem;color: #999;}
  .h5-box>.from-list>li>.ipt-box>.ipt::-moz-placeholder{font-size: 0.26rem;color: #999;}
  .h5-box>.from-list>li>.ipt-box>.ipt::-webkit-input-placeholder{font-size: 0.26rem;color: #999;}
  .h5-box>.from-list>li>.ipt-box>.arrow{position: absolute;width: 0.3rem;height: 0.3rem;right: 0;bottom: 0.4rem;background: url(../../images/user/icon_right.png) no-repeat center / cover;}
  .h5-box>.from-list>li.noborder{border: none;}
  .h5-box>.from-list>li>.btn{width: 100%;height: 0.8rem;line-height: 0.8rem;background: #F1F1F1;font-size: 0.28rem;color: #666;margin-top: 1.2rem;text-align: center;border-radius: 2rem;}
  .h5-box>.from-list>li>.btn.on{background-image: linear-gradient(90deg, #FF6F7E 0%, #F9C374 86%, #FBDE5A 100%);box-shadow: 0 0.06rem 0.2rem 0.01rem rgba(253,133,123,0.40);color: #fff;}
  .h5-set-qqwpa-box{position: fixed;top: 0;left: 0;right: 0;bottom: 0;background: rgba(0,0,0,0.6);display: none;}
  .h5-set-qqwpa-box>.content{width: 5.9rem;height: 9.1rem;position: absolute;left: 50%;top: 50%;margin-left: -2.95rem;margin-top: -4.55rem;background: #fff;border-radius: 0.2rem;padding: 0.4rem 0.4rem 0.6rem;box-sizing: border-box;text-align: center;}
  .h5-set-qqwpa-box>.content>.close{position: absolute;width: 0.32rem;height: 0.32rem;top: 0.2rem;right: 0.2rem;background: url(../../images/user/close1.png) no-repeat center / cover;}
  .h5-set-qqwpa-box>.content>.title{font-size: 0.36rem;color: #333;}
  .h5-set-qqwpa-box>.content>ul{margin-top: 0.6rem;}
  .h5-set-qqwpa-box>.content>ul>li{width: 100%;height: 0.8rem;line-height: 0.8rem;background: #F7F7F7;border-radius: 0.4rem;font-size: 0.28rem;color: #333;margin-bottom: 0.4rem;}
  .h5-set-qqwpa-box>.content>ul>li.on{background-image: linear-gradient(-90deg, #FBDE5A 0%, #F9C374 10%, #FF6F7E 100%);box-shadow: 0 0.04rem 0.12rem 0 rgba(249,184,138,0.50);border-radius: 2rem;color: #fff;}
  .h5-set-qqwpa-box>.content .btn{width: 100%;height: 0.8rem;line-height: 0.8rem;text-align: center;font-size: 0.28rem;color: #fff;background-image: linear-gradient(-90deg, #FBDE5A 0%, #F9C374 10%, #FF6F7E 100%);box-shadow: 0 0.04rem 0.12rem 0 rgba(249,184,138,0.50);border-radius: 2rem;margin-top: 0.6rem;}
  .h5-set-qqwpa-box>.content .btn.qqbtn{display: none;}
  .h5-set-category-bg{position: fixed;top: 0;right: 0;left: 0;bottom: 0;background: rgba(0,0,0,0.4);display: none;}
  .h5-set-category>.content{width: 100%;height: 5.2rem;position: absolute;left: 0;bottom: -100%;background: #fff;border-radius: 0.2rem 0.2rem 0 0;transition: bottom 0.3s ease-out;}
  .h5-set-category.on>.content{bottom: 0;}
  .h5-set-category>.content>.top{height: 1.32rem;line-height: 1.32rem;text-align: center;}
  .h5-set-category>.content>.top>.title{display: inline-block;font-size: 0.36rem;color: #333;}
  .h5-set-category>.content>.top>.cancel{margin-left: 0.6rem;font-size: 0.32rem;color: #FB9E78;}
  .h5-set-category>.content>.top>.comfirm{margin-right: 0.6rem;font-size: 0.32rem;color: #FB9E78;}
  .h5-set-category>.content>.btm{height: 3.94rem;overflow: hidden;position: relative;}
  .h5-set-category>.content>.btm>.bg-top{position: absolute;z-index: 1;left: 0;top: 0;width: 100%;height: 1.36rem;background-image: linear-gradient(180deg, #FFFFFF 4%, rgba(255,255,255,0.5) 100%);}
  .h5-set-category>.content>.btm>.bg-btm{position: absolute;z-index: 1;left: 0;bottom: 0;width: 100%;height: 1.36rem;background-image: linear-gradient(360deg, #FFFFFF 0%, rgba(255,255,255,0.5) 100%);}
  .h5-set-category>.content>.btm>.list-box{height: 1.2rem;margin-top: 1.37rem;position: relative;}
  .h5-set-category>.content>.btm>.list-box>ul{position: absolute;top: 0;width: 100%;border-top: 0.01rem solid #F1F1F1;}
  .h5-set-category>.content>.btm>.list-box>ul>li{height: 1.2rem;line-height: 1.2rem;text-align: center;font-size: 0.32rem;color: #333;border-bottom: 0.01rem solid #F1F1F1;box-sizing: border-box;}
  .h5-comfirm-alert{position: fixed;top: 0;left: 0;right: 0;bottom: 0;background: rgba(0,0,0,0.5);display: none;}
  .h5-comfirm-alert>.content{position: absolute;top: 50%;left: 50%;width: 5.7rem;height: 3.4rem;margin-left: -2.85rem;margin-top: -1.7rem;background: #fff;border-radius: 0.2rem;overflow: hidden;text-align: center;}
  .h5-comfirm-alert>.content>.title{font-size: 0.36rem;color: #333;padding-top: 0.4rem;margin-bottom: 0.54rem;}
  .h5-comfirm-alert>.content>p{font-size: 0.28rem;color: #999;}
  .h5-comfirm-alert>.content>.btn{width: 100%;height: 1rem;line-height: 1rem;font-size: 0.32rem;color: #FB9E78;position: absolute;left: 0;bottom: 0;border-top: 0.01rem solid #F1F1F1;}
}

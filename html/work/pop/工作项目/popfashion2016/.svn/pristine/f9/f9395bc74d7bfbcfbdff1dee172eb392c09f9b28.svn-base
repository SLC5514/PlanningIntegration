body{background: #fff;}
.clear:after{display: block;content: "";visibility: hidden;clear: both;}
.clear{zoom: 1;}
.season-section{background: #fff; font-family: "微软雅黑";min-width: 1200px;margin: 0 auto; position:relative;}


.footerFixed,.nav_fixed{display: none !important;}

.season-section>div{position: relative;overflow: hidden;}

.p1{height: 753px; background: url(../images/bg1.jpg) center no-repeat;}
.p1>.p1-t{height: 458px;width: 800px;margin: 0 auto;position: relative;overflow: hidden;}
.p1-t>.ani1{transition: all 0.6s linear;-ms-transition: all 0.6s linear;-moz-transition: all 0.6s linear;  opacity: 0; width: 184px;height: 378px;position: absolute;z-index: 3;top: -400px;left: 308px;background: url(../images/t1.png) center no-repeat;}
.p1-t>.ani2{transition: all 0.6s linear;-ms-transition: all 0.6s linear;-moz-transition: all 0.6s linear;  opacity: 0; width: 569px;height: 183px;position: absolute;z-index: 2;top: 500px;left: 115px;background: url(../images/t2.png) center no-repeat;}
.set1>.p1-t>.ani1{opacity: 1;top: 60px;}
.set1>.p1-t>.ani2{opacity: 1;top: 158px;}



.p1>.p1-txt{transition: all 0.5s linear 0.3s;-ms-transition: all 0.5s linear 0.3s;-moz-transition: all 0.5s linear 0.3s;opacity: 0; position: relative;overflow: hidden; left: 80%;margin: 0 auto;height: 40px;width: 520px;padding: 0 20px; background: #e8a135;}
.p1>.p1-txt>p{float: left;width: 52px;height: 40px;position: relative;top: -40px;opacity: 0;}
.p1>.p1-txt>.txt1{background: url(../images/txt1.png) center no-repeat;transition: all 0.15s linear 0.8s;}
.p1>.p1-txt>.txt2{background: url(../images/txt2.png) center no-repeat;transition: all 0.15s linear 0.95s;}
.p1>.p1-txt>.txt3{background: url(../images/txt3.png) center no-repeat;transition: all 0.15s linear 1.1s;}
.p1>.p1-txt>.txt4{background: url(../images/txt4.png) center no-repeat;transition: all 0.15s linear 1.25s;}
.p1>.p1-txt>.txt5{background: url(../images/txt5.png) center no-repeat;transition: all 0.15s linear 1.4s;}
.p1>.p1-txt>.txt6{background: url(../images/txt6.png) center no-repeat;transition: all 0.15s linear 1.55s;}
.p1>.p1-txt>.txt7{background: url(../images/txt7.png) center no-repeat;transition: all 0.15s linear 1.7s;}
.p1>.p1-txt>.txt8{background: url(../images/txt8.png) center no-repeat;transition: all 0.15s linear 1.85s;}
.p1>.p1-txt>.txt9{background: url(../images/txt9.png) center no-repeat;transition: all 0.15s linear 2s;}
.p1>.p1-txt>.txt10{background: url(../images/txt10.png) center no-repeat;transition: all 0.15s linear 2.15s;}

.set1>.p1-txt{opacity: 1;left: 0;}
.set1>.p1-txt>p{opacity: 1;top: 0;}


.p1>.top-m{transition: all 0.6s linear 0.3s;text-align: center;font-size: 0;padding-top: 12px;opacity: 0;}
.p1>.top-m>div{display:inline-block;*display: inline;*zoom: 1; color: #000;vertical-align: middle;}
.top-m>.m-msg{font-size: 16px;line-height: 32px;text-align: right; padding-right: 18px;}
.top-m>.m-link{*width: 246px;height: 110px; color: #000000;font-size: 14px;line-height: 24px;padding-left: 20px;border-left:1px solid #d0d0cf ;}
.top-m>.m-link>img{float: left;margin-right: 20px;}
.top-m>.m-link>div{float: left;padding-top: 3px;*width: 114px;}
.top-m>.m-link>div>p{font-size: 16px;height: 26px; line-height: 26px;font-weight: bold;}
.top-m>.m-link>div>a{cursor: pointer; display: block;font-size: 14px; margin: 14px auto 0;color: #fff;font-weight: bold;height: 35px;line-height: 35px;width: 110px;border-radius: 18px;text-align: center;background: #e8a135;}
.top-m>.m-link>div>a:hover{background: #ffb03b;}
.top-m>.m-link>div>a>i{display:inline-block;*display: inline;*zoom: 1; vertical-align: -3px;*vertical-align: middle; margin-right: 2px; width: 18px;height: 18px;background: url(../images/isions.png) -105px -30px no-repeat;}
.set1>.top-m{opacity: 1;}


.p2{padding-top: 150px;}
.p2>h2{transition: all 0.4s linear;opacity: 0;top: -300px; width: 900px;height: 84px;background: url(../images/h1.png) center no-repeat;margin: 0 auto;position: relative;}
.p2>.p2-list{padding-top: 100px; text-align: center;font-size: 14px;color: #000;width: 865px;margin: 0 auto;position: relative;overflow: hidden;}
.p2>.p2-list>li{opacity: 0; float: left;width: 288px;position: relative;top: 360px;}
.p2-list .list-hv{border-radius: 100px;overflow: hidden; margin: 0px auto;width: 188px;height: 188px;background: url(../images/i7.png) center no-repeat;text-align: center;line-height: 188px;}
.p2-list .list-hv>i{ display:block; width: 86px;height: 188px;margin: 0 auto;}
.p2-list .list-hv:hover i{animation: frames1 0.6s;-webkit-animation: frames1 0.6s;-ms-animation: frames1 0.6s;-moz-animation: frames1 0.6s;}


.p2-list .list-index{width: 150px;height: 65px;margin: 18px auto 6px;}
.p2>.p2-list>li>p{line-height: 30px;}

.p2-list>.list-1{transition: all 0.4s linear 0.2s;}
.p2-list>.list-2{transition: all 0.4s linear 0.6s;}
.p2-list>.list-3{transition: all 0.4s linear 1s;}

.set2>h2{opacity: 1;top: 0;}
.set2>.p2-list>li{opacity: 1;top: 0;}

.list-hv1>i{background: url(../images/i8.png) center no-repeat;}
.list-hv2>i{background: url(../images/i9.png) center no-repeat;}
.list-hv3>i{background: url(../images/i10.png) center no-repeat;}
.p2-list .list-index1{background: url(../images/txt11.png) center no-repeat;}
.p2-list .list-index2{background: url(../images/txt12.png) center no-repeat;}
.p2-list .list-index3{background: url(../images/txt13.png) center no-repeat;}


.p3{height: 938px;padding-top: 294px; background: url(../images/bg2.jpg) center top no-repeat;}
.p3>h2{transition: all 0.4s linear;opacity: 0;top: -350px; width: 900px;height: 84px;background: url(../images/h2.png) center no-repeat;margin: 0 auto;position: relative;}
.isions-contain{position: relative;overflow: hidden;height: 700px;}
.isions-cont{transition: all 0.4s linear;opacity: 0; top: 720px;padding-top: 100px;position: relative;overflow: hidden;width: 840px;height: 600px;margin: 0 auto;}
.isions-list{width: 11000px;}
.isions-list>li{float: left;width: 840px;height: 500px;position: relative;overflow: hidden;}
.isions-list>li>div{float: left;}
.item-msg{width: 300px;margin-right: 40px;background: #fff;padding-top: 20px;height: 480px;}
.item-msg>.msg-t{width: 80px;height: 80px;margin: 0 auto;}
.item-msg>h3{text-align: center;font-size: 24px;font-weight: bold;line-height: 34px;padding: 15px 0;color: #000;}
.item-msg>p{text-align: center;font-size: 14px;line-height: 24px;color: #000;}

.item-msg ul{padding-top: 32px;width: 240px;margin: 0 auto;}
.item-msg li{padding: 0 0 12px 40px;background: url(../images/i11.png) 0 0 no-repeat;}
.item-msg li>h4{color: #e8a135;font-size: 18px;font-weight: bold; line-height: 20px;padding-bottom: 4px;}
.item-msg li>p{color: #666;font-size: 14px;line-height: 24px;}

.isions-list>li>.imgs{height: 500px;width: 500px;float: left;}
.isions-list>li>.imgs>div{float: left;width: 230px;height: 230px; }
.isions-list>li>.imgs>div>p{width: 100%;height: 100%;-webkit-transition: all 1s ease-in-out;-moz-transition: all 1s ease-in-out;-ms-transition: all 1s ease-in-out;transition: all 1s ease-in-out;}
.isions-list>li>.imgs>div:hover p{transform-style: preserve-3d;-webkit-transform-style: preserve-3d;-ms-transform-style: preserve-3d; transform: rotateY(0deg);-ms-transform: rotateY(0deg);-webkit-transform: rotateY(0deg); animation: frames2 1s;-webkit-animation: frames2 1s;-ms-animation: frames2 1s;}


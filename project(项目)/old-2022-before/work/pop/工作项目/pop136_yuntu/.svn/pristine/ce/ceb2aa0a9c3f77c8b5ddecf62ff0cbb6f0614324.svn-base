.tip-box{display: none;position: fixed;top: 70px;left: 50%;margin-left: -105px;width: 210px;height: 42px;line-height: 42px;background-color: #fff;box-shadow:0px 0px 13px 0px rgba(0,0,0,0.14);border-radius:5px;text-align: center;color: #2B2B2B;font-size:14px;z-index: 501;}
.tip-box:before{content: '';display: inline-block;width: 16px;height: 16px;margin-right: 8px;vertical-align: middle;margin-top: -2px;background: url(../../images/virtual/icon_warn.png) no-repeat center / cover;}
.tip-box.on:before{background: url(../../images/virtual/icon_success.png) no-repeat center / cover;}
.dialog-box{display: none;position: fixed;top: 50%;left: 50%;width: 400px;height: 280px;margin-left: -200px;margin-top: -140px;text-align: center;background-color: #fff;border-radius:5px;z-index: 501;box-shadow:0px 0px 13px 0px rgba(0,0,0,0.14);}
.dialog-box>i.close{position: absolute;width: 24px;height: 24px;top: 15px;right: 15px;background: url(../../images/virtual/icon_close.png) no-repeat center / cover;cursor: pointer;}
.dialog-box>.title{display: none;font-size:24px;color: #333;margin-top: 40px;}
.dialog-box>.btns{position: absolute;left: 0;bottom: 30px;width: 100%;}
.dialog-box>.btns>button{width:120px;height:40px;background-color:#eee;border-radius:5px;font-size:14px;color: #333;margin: 0 10px;cursor: pointer;}
.dialog-box>.btns>button.on{background-color: #1C232B;color: #fff;}

.dialog-box>.title[store~='delete'],
.dialog-box>.title[store~='delete-group'],
.dialog-box>.title[store~='add-group'],
.dialog-box>.title[store~='deit-group'],
.dialog-box>.ctn[store~='add-group'],
.dialog-box>.ctn[store~='deit-group'],
.dialog-box>.btns>button[store~='delete'],
.dialog-box>.btns>button[store~='delete-group'],
.dialog-box>.btns>button[store~='deit-group']{display: none;}
.dialog-box[store~='delete']>.title[store~='delete'],
.dialog-box[store~='delete-group']>.title[store~='delete-group'],
.dialog-box[store~='add-group']>.title[store~='add-group'],
.dialog-box[store~='deit-group']>.title[store~='deit-group'],
.dialog-box[store~='add-group']>.ctn[store~='add-group'],
.dialog-box[store~='deit-group']>.ctn[store~='deit-group']{display: block;}
.dialog-box[store~='delete']>.btns>button[store~='delete'],
.dialog-box[store~='delete-group']>.btns>button[store~='delete-group'],
.dialog-box[store~='deit-group']>.btns>button[store~='deit-group']{display: inline-block;}

.dialog-box[store~='delete']>.title{margin-top: 100px;}
.dialog-box[store~='delete-group']>.title{margin-top: 100px;}
.dialog-box[store~='add-group']>.ctn>input{width: 340px;height: 40px;padding-left: 10px;background: #fff;border-radius:5px;border:1px solid #d8d8d8;box-sizing: border-box;margin-top: 30px;}
.dialog-box[store~='deit-group']>.ctn>input{width: 340px;height: 40px;padding-left: 10px;background: #fff;border-radius:5px;border:1px solid #d8d8d8;box-sizing: border-box;margin-top: 30px;}
.dialog-box[store~='deit-group']>.btns{padding: 0 30px;box-sizing: border-box;}
.dialog-box[store~='deit-group']>.btns>.delete{float: left;width: auto;height: auto;margin: 10px 0 0;background-color: transparent;color: #666;}
.dialog-box[store~='deit-group']>.btns>.confirm{float: right;margin: 0;}


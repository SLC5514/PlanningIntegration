*{padding: 0;margin: 0;font-family:"Microsoft YaHei";}
.main{width: 100%;max-width: 720px; background: url("../image/beijing.png");background-size: 100%;margin: auto;}
.down-box{width: 100%;height: 10.88rem;padding-top: 4.5rem;padding-bottom: 3.30rem;}
.down-h1{font-size: 1.11rem;color: #FFFFFF;text-align: center;margin: .33rem 0;display: block;}
.down-span{display:block;font-size: .44rem;color: #FFFFFF;text-align: center;}
.down-list{width: 100%;height: 2.6rem;margin-top:.97rem;}
.down-list>ul{width: 100%;height: 100%;display: flex;justify-content: center;list-style-type: none;align-items: center;justify-content: center;}
.down-list>ul>li{width: 1.66rem;height: 2.58rem;margin-right: .28rem;}
.down-list>ul>li:last-child{margin-right: 0;}
.down-list>ul>li>img{width: 1.66rem;height: 1.66rem;}
.down-list>ul>li>span{font-family: "PingFangSC-Regular";font-size: .33rem;color: #FFFFFF;letter-spacing: 0;text-align: center;width: 100%;display: block;}
.down-but{width: 7.41rem;height: 1.05rem;background: #FC3655;border-radius: 100px;font-family: "PingFangSC-Semibold";font-size: .44rem;color: #FFFFFF;letter-spacing: 0;text-align: center;line-height: 1.05rem;margin: 2rem auto 0;}
.isWX{position: fixed;top: 0;left: 0;right: 0;background-color: #fff;opacity: 0.95;height: 4.25rem;font-family:"MicrosoftYaHeiLight";font-size: .44rem;color: #000000;text-align: center;line-height: 4.25rem;display: none;}
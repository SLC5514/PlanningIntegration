.share-box{display: none;position: fixed;top: 50%;left: 50%;width: 400px;height: 300px;margin-left: -200px;margin-top: -150px;text-align: center;background-color: #fff;border-radius:5px;z-index: 501;box-shadow:0px 0px 13px 0px rgba(0,0,0,0.14);}

<div>
	<div>
		<ul class="select_items paixu fl">
			{if isset($iTrendOrManual) && !empty($iTrendOrManual) && !$paramsArr['btype'] && $columnId != 131}
			<li class="pli">
				<span class="aspan">类型<i class="nav-icon"></i></span>
				<div class="showbox short-sum">
					<div class="dateHeight contentHolder">
						<div class="content">
							{foreach $iTrendOrManual as $key=>$type}
							<a class="clickme {if $key != 0}borderTop{/if}" href="{$type.link}" title="{$type.name}">{$type.name}</a>
							{/foreach}
						</div>
					</div>
				</div>
			</li>
			{/if}
			{if isset($seasons) && !empty($seasons)}
			<li class="pli">
				<span class="aspan">季节<i class="nav-icon"></i></span>
				<div class="showbox short-sum">
					<div class="dateHeight contentHolder">
						<div class="content">
							{foreach $seasons as $key=>$season}
							<a class="clickme {if $key != 0}borderTop{/if}" href="{$season.link}" title="{$season.name}">{$season.name}</a>
							{/foreach}
						</div>
					</div>
				</div>
			</li>
			{/if}
			{if !isset($paramsArr['boo']) && $columnId != 115 && $columnId != 71 && $columnId != 131}
			<li class="pli">
				<span class="aspan">书名<i class="nav-icon"></i></span>
				<div class="showbox short-sum">
					<div class="dateHeight contentHolder">
						<div class="content brand_display">
						</div>
					</div>
				</div>
			</li>
			{/if}
		    {if !isset($paramsArr['reg']) && $columnId != 115}
		    <li class="pli">
                <span class="aspan">地区<i class="nav-icon"></i></span>
				<div class="showbox region_box category_box">
                    <div class="search-area-box clearfix">
                        <input class="fl searInput" type="text" value="请输入国家或城市的关键字">
                        <input class="fr button neiyeList" type="button">
                    </div>
					<div class="allcc clearfix">
						<div class="area_qh"><span>地区：</span><a href="javascript:void(0);" class="on"><span>按区域</span><i></i></a><a href="javascript:void(0);"><span>按大洲</span><i></i></a></div>
					</div>
					<div class="mingc">
						<div class="fl">
							<span>大洲名称：</span>
							<a href="javascript:void(0);">美洲</a>
                        </div>
                        <div class="allred fl"><a href="javascript:void(0);" >全部亚洲国家>></a></div>						
					</div>					
					 <div class="category_div contentHolder ps-container">
						<div class="content">
                            <dl class="category_dl clearfix">
		                        <dt><a href="javascript:void(0);" data-para="Outerwearpop95Jacket|0" data-pos="9"></a></dt>
		                        <dd>
		                            <a href="javascript:void(0);" data-para="Outerwearpop95Jacket|Cape" data-pos="9"></a>
		                        </dd>
		                    </dl>
						</div>
					</div>
				</div>
		    </li>
		    {/if}
			{if isset($sClass) && !empty($sClass) && $columnId != 131}
			<li class="pli">
				<span  class="aspan">类型<i class="nav-icon"></i></span>
				<div class="showbox short-sum">
					<div class="dateHeight contentHolder">
						<div class="content">
							{foreach $sClass as $key =>$val}
							<a class="clickme {if $key != 0}borderTop{/if}" href="{$val.link}" title="{$val.name}">{$val.name}</a>
							{/foreach}
						</div>
					</div>
				</div>
			</li>
			{/if}
			{if !empty($books) && !isset($paramsArr['bra']) && ($columnId == 71 || $columnId == 131)}
			<li class="pli">
				<span class="aspan">品牌名<i class="nav-icon"></i></span>
				<div class="showPro showbox" style="height:500px;">

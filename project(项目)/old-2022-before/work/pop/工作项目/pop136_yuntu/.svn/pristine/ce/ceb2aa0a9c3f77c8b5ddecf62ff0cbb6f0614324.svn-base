.setuser-list>li>.ctn>.form-list>li.sms>.sms-code.disabled-ele{background: #eee;color: #333;cursor: no-drop;}
.setuser-list>li>.ctn>.form-list>li.pswdnew>.level{position: absolute;bottom: -24px;right: 14px;}
.setuser-list>li>.ctn>.form-list>li.pswdnew>.level>li{float: left;width:40px;height:18px;line-height:18px;text-align: center;background:#1d232c;opacity:0.5;font-size:12px;color: #fff;margin-left: 10px;}
.setuser-list>li>.ctn>.form-list>li.pswdnew>.level>li.on{opacity: 1;}

.setuser-list input::-webkit-input-placeholder{color:#999;font-size: 14px;}

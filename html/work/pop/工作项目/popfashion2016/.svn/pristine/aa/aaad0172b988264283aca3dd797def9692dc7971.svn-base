{literal}
<script id="T_Title" type="text/html">{{stitle}}</script>

<script id="T_LeftRightBtn" type="text/html">
{{if lUrl && visit }}<div class="leftBtn" data-col="{{lcol}}" data-t="{{ltable}}" data-id="{{lid}}" data-total={{ltotal}} data-index="{{loffset}}" data-type="{{type}}" data-isuc="{{isuc}}" data-wbid="{{wbid}}" data-ver="{{ver}}">上<br>一<br>{{word}}</div>{{/if}}
{{if rUrl }}<div class="rightBtn" data-col="{{rcol}}" data-t="{{rtable}}" data-id="{{rid}}" data-total={{rtotal}} data-index="{{roffset}}" data-type="{{type}}" data-isuc="{{isuc}}" data-wbid="{{wbid}}" data-ver="{{ver}}">下<br>一<br>{{word}}</div>{{/if}}
</script>

<script id="T_Tab" type="text/html">
{{if detailCnt>1}}
{{each detailList as value index}}
    <li {{if index==0}}class="on"{{/if}}>{{++index}}</li>
{{/each}}
{{/if}}
</script>

<script id="T_DetailList" type="text/html">
{{if visit}}
    {{if detailCnt}}
    {{each detailList as value index}}
    <img class="bigpic {{if index==0}}on{{/if}}" {{if index==0}}style="display: inline;"{{/if}}  src="{{if index==0}}{{value.bigPath}}{{/if}}" data-t="{{value.simpleName}}" data-id="{{value.id}}" data-sp="{{value.smallPath}}" data-bp="{{value.bigPath}}" data-col="{{value.columnid}}" data-shape="{{value.shape}}" data-size="{{value.size}}" alt="pic">
    {{/each}}
    <i class="enlarge" title="高清浏览"></i>
    {{/if}}
{{else}}
<div class="qx_remind">
    <div class="remind_info clearfix">
        <span class="fl"></span>
        <p class="fl">对不起，本书暂时只提供预览版本…<br/><span>如您想提前阅读本书内容，可咨询并购买实体版本</span></p>
    </div>
    <div class="remind_cous">
        <a class="qq_ic" href="{{qqlink}}" target="_blank" title="QQ咨询"><i></i>QQ咨询</a>
        <a class="tel_ic" href="javascript:void(0);" title="咨询电话"><i></i>咨询电话：{{telnumber}}</a>
    </div>
    <div class="remind_img">
        <span></span>
    </div>
</div>
{{/if}}
</script>

<script id="T_StyleInfo" type="text/html">
    {{if styleInfoCnt}}
    <div class="inf inf1 shadow">
        <h3>{{info}}信息</h3>
        <ul class="inflist repeat_inflist
        ">
            {{if styleInfo.brand.name}}
            <li class="brand_detail">
                <p>
                    <label>品牌：</label>
                    <span><a href="{{styleInfo.brand.link}}" target="_blank" class="mousehover" title="{{styleInfo.brand.name}}">{{styleInfo.brand.name}}</a></span>
                </p>
            </li>
            {{/if}}
            {{if styleInfo.classify.name}}
            <li>
                <p>
                    <label>归类：</label>
                    <span><a href="{{styleInfo.classify.link}}" target="_blank" class="mousehover" title="{{styleInfo.classify.name}}">{{styleInfo.classify.name}}</a></span>
                </p>
            </li>
            {{/if}}
			{{if styleInfo.industry.length > 0}}
			<li>
				<p>
					<label>行业：</label>
					<span>
					{{each styleInfo.industry as value index}}
						<a href="{{value.link}}" target="_blank" class="mousehover" title="{{value.name}}">{{value.name}}</a>
					{{/each}}
					</span>
				</p>
			</li>
			{{/if}}
            {{if styleInfo.gender.length > 0}}
            <li>
                <p>
                    <label>性别：</label>
                    <span>
					{{each styleInfo.gender as value index}}
						<a href="{{value.link}}" target="_blank" class="mousehover" title="{{value.name}}">{{value.name}}</a>
					{{/each}}
					</span>
                </p>
            </li>
            {{/if}}
			{{if styleInfo.catagory.length > 0}}
			<li>
				<p>
					<label>分类：</label>
					<span>
					{{each styleInfo.catagory as value index}}
						<a href="{{value.link}}" target="_blank" class="mousehover" title="{{value.name}}">{{value.name}}</a>
					{{/each}}
					</span>
				</p>
			</li>
			{{/if}}
			
            {{if styleInfo.subcatagory.name}}
            <li>
                <p>
                    <label>品名：</label>
                    <span><a href="{{styleInfo.subcatagory.link}}" target="_blank" class="mousehover" title="{{styleInfo.subcatagory.name}}">{{styleInfo.subcatagory.name}}</a></span>
                </p>
            </li>
            {{/if}}
            {{if view_count}}
            <li>
                <p>
                    <label>浏览量：</label>
                    <span>{{view_count}}次</span>
                </p>
            </li>
            {{/if}}
            {{if updateTime}}
            <li class="updateTime">
                <p>
                    <label>更新日期：</label>
                    <span>{{updateTime}}</span>
                </p>
            </li>
            {{/if}}
            {{if source}}
            <li class="pic_laiyuan">
                <p>
                    <label>图片来源：</label>
                    {{#source}}
                </p>
            </li>
            {{/if}}
        </ul>
    </div>
    {{/if}}
</script>

<script id="T_Label" type="text/html">
    {{if relationLabelsCnt}}
    <div class="keyword1 shadow">
        <h3>关键词</h3>
        <div class="keylist">
            {{each relationLabels as value index}}
            <a target="_blank" href="{{value}}" title="{{index}}">{{index}}</a>
            {{/each}}
        </div>
    </div>
    {{/if}}
</script>
<script id="T_StyleAd" type="text/html">
{{if adsCnt}}
<div class="add shadow">
    {{each ads as value index}}
    <a {{if value.sNewWindow}}target="_blank"{{/if}} href="{{value.sLink}}" title="{{value.sTitle}}"><img src="{{value.sImagePath}}" alt="{{value.sTitle}}"></a>
    {{/each}}
</div>
{{/if}}
</script>

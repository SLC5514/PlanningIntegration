@charset "utf-8";
.fl{float: left;}
.fr{float: right;}
body{background: #323940;}
/*底部图标*/

.pattern-title{font-size: 24px;color: #fff;line-height: 30px;text-align: center;}
.pattern-title>span{display: inline-block;width: 80px;height: 1px;vertical-align: middle;background: rgba(255, 255, 255, .3);margin: 0 18px;}

/* 2019/8/1之前下载提示 */
.overdue-warming{text-align: center;font-size: 14px;color: #d8b056;margin:10px 0;}

/* 我的设计花型 */
.design-content{margin-bottom: 64px;display: none;}
.designs-box{height: 204px;padding: 40px 0 0;overflow: hidden;}
.designs-box>.design-div{width: 100%;height: 100%;position: relative;overflow: hidden;}
.designs-box>.design-div>ul{width: 200%;}
.designs-box ul>li{float: left;margin-right: 36px;width: 200px;height: 200px;border:2px solid #40474e;}
.designs-box ul>li:hover{border-color: #d8b056;}
.designs-box ul>li>a{display: block;width: 100%;height: 100%;line-height: 200px;text-align: center;font-size: 0;background: #fff;overflow: hidden;}
.designs-box ul>li>a>img{vertical-align: middle;width: 100%;}
.design-div>a{ display: block; position: absolute;padding-top: 80px;background: #d8b056;width: 60px;cursor: pointer; height: 120px;right: 0;top: 2px;font-size: 16px;color: #fff;text-align: center;}
.design-div>a>div{padding-left: 3px;}
.design-div>a span{display: inline-block;vertical-align: middle;width: 6px;height: 6px;border-radius: 3px;background: #fff;margin-right: 3px;}

/* 最新趋势 */
.trend-content{margin-bottom: 64px;display: none;}
.trend-box{padding: 40px 0 0;overflow: hidden;}
.trend-box>.trend-div{width: 100%;height: 100%;position: relative;overflow: hidden;}
.trend-box>.trend-div>a{ display: block; position: absolute;padding-top: 120px;background: #d8b056;width: 60px;cursor: pointer; height: 100%;right: 0;top: 0;font-size: 16px;color: #fff;text-align: center;box-sizing: border-box;}
.trend-box>.trend-div>a>div{padding-left: 3px;}
.trend-box>.trend-div>a span{display: inline-block;vertical-align: middle;width: 6px;height: 6px;border-radius: 3px;background: #fff;margin-right: 3px;}
.trend-box ul{width: 200%;}
.trend-box ul>li{float: left;margin-right: 30px;width: 275px;height: 274px;background:#42494f;box-shadow:0px 2px 16px 0px rgba(0,0,0,0.12);border-radius:5px 5px 0px 0px;}
.trend-box ul>li>a{display: block;width: 100%;height: 192px;line-height: 192px;text-align: center;font-size: 0;overflow: hidden;background: #52575d;}
.trend-box ul>li>a>img{vertical-align: middle;width: 100%;}
.trend-box ul>li>.text-down{border-top: none;position: relative;min-height: 82px;}
.trend-box ul>li>.text-down>a>.time{display: none;color: #000;cursor: default;width: 100%;padding: 0 10px;box-sizing: border-box;cursor: pointer;height: 30px;line-height: 30px;background:rgba(255, 255, 255, 0.66);position: absolute;position: absolute;top: -30px;}
.trend-box ul>li>.text-down>a>.title{overflow: hidden;color: #fff;cursor: pointer;padding: 12px 10px 0;max-height: 24px;line-height:24px;font-size: 16px;white-space: nowrap;text-overflow: ellipsis;}
.trend-box ul>li>.text-down>.label{margin: 12px 0;max-height: 22px;padding: 0 10px;overflow: hidden;line-height: 22px;font-size: 0;}
.trend-box ul>li>.text-down>.label>a{font-size: 14px;color: #999;margin-right: 8px;padding-left: 8px;}
.trend-box ul>li>.text-down>.label>a:hover{color: #D7B056;}
.trend-box ul>li>.text-down>.label>a:first-child{padding-left: 0;}
.trend-box ul>li>.text-down>.label>a+a:before{content: '';display: inline-block;height: 10px;border-left: 1px solid #B2B2B2;position: relative;left: -8px;}
.trend-box ul>li:hover>.text-down>a>.time{display: block;}

/* 提示 */
.tip-box{display: none;position: fixed;top: 30px;left: 50%;margin-left: -105px;width: 210px;height: 42px;line-height: 42px;background-color: #fff;box-shadow:0px 0px 13px 0px rgba(0,0,0,0.14);border-radius:24px;text-align: center;color: #2B2B2B;font-size:14px;z-index: 501;}
.tip-box:before{content: '';display: inline-block;width: 16px;height: 16px;margin-right: 12px;vertical-align: middle;margin-top: -2px;background: url(../../images/report/icon_tip_success.png) no-repeat center / cover;}

/* 最新趋势 普通用户提示 */
.trend-prompt{display: none;position: fixed;left: 0;top: 0;width: 100%;height: 288px;text-align: center;color: #fff;background:rgba(41,47,54,0.95);z-index: 500;}
.trend-prompt>div{position: relative;}
.trend-prompt .close{position: absolute;top: 20px;right: 0;width: 42px;height: 42px;background: url(../../images/report/icon_close.png) no-repeat center / 100%;cursor: pointer;}
.trend-prompt .close:hover{background: url(../../images/report/icon_close_hv.png) no-repeat center / 100%;}
.trend-prompt .title{font-size:20px;font-weight:bold;line-height:27px;padding-top: 55px;margin-bottom: 24px;}
.trend-prompt .info{font-size:14px;line-height:22px;margin-bottom: 33px;}
.trend-prompt .btn{width:220px;height:50px;line-height:50px;background:#d8b056;border-radius:5px;font-size:16px;cursor: pointer;color: #fff;}
.trend-prompt .btn:hover{background: #dba428;}

.pattern-content{margin: 0 auto;padding-top: 80px;background: #323940;min-height: 400px;}
.pattern-top>.analysis-box{background: #40474e;display: none;}
.color-analysis{width: 100%;margin: 0 auto; position: relative;height: 350px;z-index: 2;}
.load-pie{background: url(../../images/pattern/loading_pie.gif) center no-repeat;}

.color-analysis>.pie {position: relative;height: 100%; float: left;width: 50%;}
.load-pie>.pie {display: none;}
.color-analysis>.pie>.analysis-pic {position: absolute;width: 98px;height: 99px;line-height: 98px;color: #fff;text-align: center;border-radius: 50%;overflow: hidden; top: 126px;left: 50%;margin-left: -49px;z-index: 3;}
.color-analysis>.pie>.analysis-pic>a{display: block;width: 100%;height: 100%;line-height: 98px;color: #fff;text-align: center;}
.color-analysis>.pie>.analysis-pic img{width: 97px;vertical-align: middle;}
/*.color-analysis>.pie>.analysis-pic1{background: url(../../images/pattern/color-analysis.png) center no-repeat;}
.color-analysis>.pie>.analysis-pic2{background: url(../../images/pattern/color-back.png) center no-repeat;}
*/
#main-category { width: 100%;height: 350px;float: left;position: relative;z-index: 2;}
#main-color { width: 100%;height: 350px;float: left;position: relative;z-index: 2;}

.tech-support {text-align: center;font-size: 12px;line-height: 20px;color: #999;padding-bottom: 14px;}
.tech-support>p {color: #ccc; width: 307px;height: 32px;border-radius: 3px;background: #333a42;margin: 0 auto;line-height: 32px;}
.tech-support>p>img {vertical-align: middle;}

.analysis-show {position: relative; height: 50px;border-top: 2px solid #1c232b;}
.analysis-show>div {cursor: pointer; position: absolute;index: 3;width: 210px; height: 40px; top: 0px;left: 50%;margin-left: -105px;background: url(../../images/pattern/slide-down.png) top no-repeat;}
.analysis-show>.slid-up{background: url(../../images/pattern/slide-up.png) top no-repeat;}

/*团列表筛选项*/
.select-tbox{padding-bottom: 20px;}
.select-tbox,.select-tbox a{color: #fff;}
.screen-box{background: #42494f;position: relative;z-index: 5;height: 60px;}
.screen-box>.paixu {float: left;height: 100%;}
.screen-box>.paixu>li {position: relative;z-index: 2;float: left;margin:0 10px; cursor: pointer;height: 60px;line-height: 60px;}
.screen-box>.paixu>li:hover{background: url(../../images/pattern/seck-bg.png) repeat;}
.paixu .seck-content a:hover{color: #d8b056;}
.screen-box>.paixu>.paixu-load{color: #999;font-size: 14px;cursor: default;}
.screen-box>.paixu>.paixu-load:hover{background: #42494f;}
.screen-box>.paixu>.paixu-load>img{width: 16px;vertical-align: middle;margin-right: 10px;}

.paixu>li>.aspan {font-size: 12px;position: relative;padding: 0 10px;}
.paixu>li>.aspan>span{background-position: -15px -130px; display: inline-block;*display: inline;*zoom: 1; width: 9px;height: 5px;margin-left: 10px;vertical-align: middle;margin-top: -2px;}
.paixu>li>.showbox{visibility: hidden; background: url(../../images/pattern/seck-bg.png) repeat; line-height: 12px;position: absolute;z-index: 10;top: 47px;left: 0px;cursor: default;}
.paixu>li:hover .showbox{visibility: visible;}
.paixu>li>.sum {padding: 0 0px 10px 0px;width: 100%;}
.paixu .showbox>.date-height {max-height: 180px;position: relative;overflow: hidden;}
.sum>.date-height a {color: #fff; display: block;font-size: 12px;text-align: center;padding: 9px 0;cursor: pointer;}
.paixu>li .category-box {width: 496px;}
.paixu>li .big-pattern{width: 705px;}

.paixu>li .category-div {width: 496px;max-height: 290px;position: relative;overflow: hidden;}
.category-div .category-dl {padding: 6px 0;}
.category-box .category-dl>dt {width: 80px;text-align: right;float: left;cursor: pointer;padding-right: 10px;}
.category-box .category-dl>dt>a {font-size: 12px;line-height: 22px;color: #fff;font-weight: bold;}
.category-box .category-dl>dd {float: left;width: 378px;border-left: 1px solid #353a42;overflow: hidden;padding: 0px 0 0px 10px;}
.category-box .category-dl>dd>a {font-size: 12px;line-height: 22px;padding-right: 8px;color: #fff;float: left;cursor: pointer;display: inline-block;*display: inline;*zoom: 1; white-space: nowrap;}
.category-box .category-dl.border-bottom {border-bottom: 1px solid #353a42;}


.paixu .patterns-cont{max-height: 273px;position: relative;overflow: hidden;}
.patterns-cont>.seck-content{padding: 15px 0 15px 20px;}
.patterns-cont>.seck-content>ul{width: 681px;}
.patterns-cont>.seck-content>ul>li{float: left;width: 170px;height: 24px;line-height: 22px;}
.patterns-cont>.seck-content>ul>li>a{display: block;width: 160px; cursor: pointer;height: 100%;color: #fff;font-size: 12px;text-overflow: ellipsis;white-space: nowrap;overflow: hidden;}
.patterns-cont>.seck-content>ul>li>.brand-ibra{color: #d8b056;}

.paixu .sum-color {width: 172px;padding-left: 10px;}
.paixu .sum-color>.color-div>.seck-content{padding: 6px 0 4px;}
.paixu .sum-color>.color-div>.seck-content>a{float: left;display: block; width: 36px;height: 36px;margin: 0 6px 6px 0;}
.paixu .sum-color .box-color {display: block;position: relative; width: 36px;height: 36px; color: #fff; text-align: center;line-height: 36px;}
.paixu .sum-color .box-color>p{display: none;height: 100%;width: 100%;background: url(../../images/pattern/coloropa.png) center no-repeat;}


/*筛选搜索项*/
.screen-result{float: right;padding-right: 20px;}
.screen-result>.finds-tyle {height: 60px;line-height: 60px;margin-right: 20px; float: left;color: #fff;font-size: 14px;}
.screen-result>.finds-tyle>span {color: #d8b056;}

.screen-result>.search-box {background: #333; font-size: 0; float: left;border-radius: 3px;height: 30px;line-height:30px;position: relative;top: 15px; width: 180px;padding-left: 20px;}
.screen-result>.search-box>input{color: #c5c6c7; vertical-align: middle; font-size: 12px;border:0 none;height: 30px;line-height: 30px;width: 144px;}
.screen-result>.search-box>.neiye-list {float: right; width: 35px;cursor: pointer;padding: 0 10px;background-image: url(../../images/common/icon.png);background-repeat: no-repeat;background-position: -39px -124px;}
.screen-result>.search-box>.sear-input{float: left;background: #333;}

.keycho{color: #fff;font-size: 12px;line-height: 24px;padding: 20px 0 0;display: none;}
.keycho>.yixz {float: left; display: inline-block;*display: inline;*zoom: 1;height: 24px;width: 60px;padding-right: 5px;}
.keycho>.key-allbox {float: left;}
.key-allbox>.key-word{height: 24px;float: left;white-space: nowrap;margin-right: 10px;}
.key-allbox>.key-word>a {display: block;color: #fff;padding: 0 26px 0 10px;height: 22px;line-height: 22px;background: #40474e;position: relative;border: 1px solid #999;}
.key-allbox>.key-word>a>span{background-position: -85px -129px;width: 9px;height: 9px;cursor: pointer;position: absolute;right: 8px;top: 7px;}
.key-allbox>.key-word>a:hover{border: 1px solid #d8b056;}
.key-allbox>.key-word>a:hover span{background-position: -111px -129px;}
.key-allbox>.key-word>a>i{position: absolute;display: block; background-position: -34px -54px;width: 7px;height: 7px;top: 8px;}

.key-allbox>.key-word>.auto-sx>p {display: inline-block;*display: inline;*zoom: 1; vertical-align: middle;margin-top: -3px;max-width: 210px;overflow: hidden;}
.key-allbox>.key-word>.auto-sx>p.bg-p{height: 16px;margin-left: 3px;width: 35px;}
.key-allbox>.delefilter {float: left;margin-left: 12px;display: block;padding-left: 18px;position: relative;}
.key-allbox>.delefilter>span{position: absolute;left: 0;top: 7px;width: 12px;height: 12px;background-position: -140px -130px;}
.key-allbox>.delefilter:hover{color: #d8b056;}
.key-allbox>.delefilter:hover span{background-position: -169px -130px;}


.pattern-section{min-height: 500px;}
/*图案列表*/
.pattern-list{width: 100%;overflow: hidden;padding: 10px 0 1px;}
.pattern-list>ul{width: 1260px;*padding-bottom: 60px;}
.pattern-list>ul>li{position: relative; width: 200px;height: 200px;overflow: hidden; float: left;margin: 0 50px 60px 0px;}
.pattern-list>ul>li>a{display: table-cell;*display: block; vertical-align: middle; background: #fff; overflow: hidden; width: 200px;height: 200px;line-height: 200px; text-align: center;position: relative;}
.pattern-list>ul>li>a>img{display: block;vertical-align: middle;max-width: 200px;margin: auto;}
.pattern-list>ul>li>a>.dis-posion{display: none;opacity: 0.5; position: absolute;font-size: 12px;color: #fff;text-align: center; z-index: 2;width: 190px;bottom: 12px;background: #d8b056;padding: 5px 5px 0; overflow: hidden;}
.pattern-list .dis-posion>h3{overflow: hidden; text-overflow: ellipsis;white-space: nowrap;color: #fff; line-height: 26px;padding-top: 2px;font-weight: normal;}
.pattern-list .dis-posion>h3:hover{text-decoration:underline;}
.pattern-list .dis-posion>.list-words{text-align: center; height: 26px;line-height: 26px; overflow: hidden;}
.pattern-list .dis-posion>.list-words>p{display: inline-block;*display: inline;*zoom: 1;height: 26px;line-height: 26px;padding: 0 4px;}
.pattern-list .dis-posion>.list-words>p:hover{text-decoration:underline;}
.pattern-list .dis-posion>p{height: 22px;line-height: 22px;padding-bottom: 9px;}
.pattern-list .dis-posion>p>span{display: inline-block;*display: inline;*zoom: 1;width: 15px;height: 16px;margin-right: 2px;vertical-align: middle;background-position: -329px -129px;}

.pattern-list .color-bott{height: 12px;position: absolute;z-index: 2;width: 100%;bottom: 0;overflow: hidden;}
.pattern-list .color-bott>a{float: left;height: 100%;display: block;}

.list-more{width:120px;text-align: center;cursor: pointer; margin: 0 auto;font-size: 14px;color: #fff;line-height: 20px;}
.list-more>span{display:inline-block;*display: inline;*zoom: 1;margin-right: 6px;vertical-align: middle; width: 16px;height: 15px;background-position: -198px -130px;left: 0;top: 4px;}

/* add收藏 */
.collect-data{position: absolute;background: url(../../images/common/collect.png) no-repeat 0 0;width: 30px;height: 30px;right: 10px;top: 10px;display: none;z-index: 10;}
.collect-data:hover,.collect-data.on{background-position: -50px 0;}

/*搜索结果为空*/
.pattern-null{display: none; padding: 110px 0 360px;text-align: center;}
.pattern-null>.null-pic{height: 175px;background: url(../../images/common/null-smile.png) center no-repeat;}
.pattern-null>p{color: #fff;font-size: 16px;line-height: 20px;padding: 62px 0 45px;}
.pattern-null>.link-msg{text-align: center;font-size: 0;}
.pattern-null>.link-msg>a{font-size: 14px;margin-right: 10px; display: inline-block;*display: inline;*zoom: 1;color: #fff;width: 150px;height: 40px;line-height: 40px;background: #d8b056;border-radius: 3px;}
.pattern-null>.link-msg>a:hover{background: #dba428;}
.pattern-null>.link-msg>a>span{display: inline-block;*display: inline;*zoom: 1;width: 19px;height: 19px;vertical-align: middle;margin-right: 2px; background-position: -243px -130px;}
.pattern-null>.link-msg>div{font-size: 14px;display: inline-block;*display: inline;*zoom: 1;color: #fff;width: 240px;height: 40px;line-height: 40px;background: #787c81;border-radius: 3px;}
.pattern-null>.link-msg>div>span{display: inline-block;*display: inline;*zoom: 1;width: 19px;height: 19px;vertical-align: middle;margin-right: 2px; background-position: -286px -130px;}



/*无权限下拉弹出层*/
.general-user-info-fixbox{display: none;z-index: 550;position: fixed;left: 0px;top: 0px;width: 100%;padding-bottom: 70px;padding-bottom: 70px;color: #fff;background-color:rgba(28,35,43,0.93);text-align: center;}
.general-user-info-fixbox>div{position: relative;}
.general-user-info-fixbox>div>button{display: inline-block;z-index: 10;position: absolute;right: 0px;top: -30px;width: 40px;height: 40px;background-color: transparent;border-style: none;background-image: url(../../images/pattern/icon1.png);background-repeat: no-repeat;background-position: center;}
.general-user-info-fixbox>div>button:hover{background-image: url(../../images/pattern/icon1-active.png);}
.general-user-info-fixbox>div>h3{font-size: 20px;margin-top: 50px;font-weight: bold;color: #fff;}
.general-user-info-fixbox>div>p{margin: 20px auto 30px;font-size: 14px;line-height: 1.5em}
.general-user-info-fixbox>div>a{display: inline-block;font-size: 16px;width: 220px;height: 50px;line-height: 50px;border-radius: 5px;text-align: center;background-color: #d8b056;color: #fff;}
.general-user-info-fixbox>div>a:hover{background-color: #dba428;}


@media only screen and (min-width: 1500px){      
	.color-analysis{width: 80%;}
	.pattern-list>ul{width: 1570px;}
	.pattern-list>ul>li{margin: 0 60px 60px 0px;}
	.designs-box ul>li{margin-right: 49px;}
}


.rightc-bottom{display: none;}
/* 翻页页码 */
.rightc-bottom .pages{ height: 38px; text-align: center; margin-bottom: 25px;line-height:38px;}
.rightc-bottom .pages span,.pages a{ min-width: 18px;border: 1px solid #ccc; padding: 0 9px; height: 36px;line-height: 36px; text-align: center; color: #a9a9a9; font-size: 12px;display: inline-block; background-color: #fff;vertical-align: top; cursor: pointer;  margin-right:4px;}
.rightc-bottom .pages a:hover{background-color: #d8b056;border: 1px solid #d8b056; color:#fff;}
.rightc-bottom .pages span.hov{background-color: #d8b056;border: 1px solid #d8b056;color:#fff; cursor: auto;}
.rightc-bottom .pages .prevPage{background: url(../../images/common/icon.png) no-repeat -190px -232px #fff;}
.rightc-bottom .pages .nextPage{background: url(../../images/common/icon.png) no-repeat -147px -232px #fff;}
.rightc-bottom .pages input{ background-color: #fff; border: 1px solid #ccc;  color:#a9a9a9;font-size: 12px;width: 56px; height: 36px; line-height: 36px;text-align: center; vertical-align: top;  margin-right: 5px; padding: 0 2px;}
.rightc-bottom .pages .ellipsis{ background-color: transparent;}
.rightc-bottom .pages .goBtn{ background-color: #d8b056;border: 1px solid #d8b056; color: #fff; cursor: pointer; padding: 0 12px;}


































									<div>免费试读</div>
									<img src="{$value.smallPath}" alt="pic" />
								</a>
								<div>
									<p>{$value.title|escape:"html"}</p>
									<div>
										<a href="/member/register/" target="_blank" title="注册">注册</a> 或  <a href="javascript:void(0);" class="loginLayer" title="登录">登录</a>  后可全文浏览
									</div>
								</div>
							</li>
							{/foreach}
						</ul>
					</div>
				</div>
				{/if}
			</div>
		</div>
		{/if}
		{if $P_UserType==5 && $powerDetails['P_Tryout']}
		<!--免费报告游客提示-->
		<div class="login-trial">
			<div class="login-trial-content">
				<img src="{$cover}" alt="icon" />
				<div class="trial-bg"></div>
				<div class="trial-disc">
					<h2>登录注册后，您可免费浏览报告全文</h2>
					<a class="redRg btn-l" href="/member/register/" rel="nofollow" target="_blank" title="注册">注册</a><a class="loginLayer btn-r" href="javascript:void(0);" rel="nofollow" title="登录">登录</a>
				</div>
			</div>
		</div>
		{/if}
	{/if}


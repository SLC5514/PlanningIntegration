.speed-buy>a>i{vertical-align: middle; width: 20px;height: 20px;margin: 0 6px 0 45px;display: inline-block;*zoom: 1;*display: inline;background:url(../../images/new-detail/report-icons.png) no-repeat -181px -77px;}
.speed-buy>a:hover{background: #f4cb69;}


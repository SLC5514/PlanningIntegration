.top-main .bottom .bottom-left{background: url(../../images/top/bottom-left.png);left: -82px;}
.top-main .bottom .bottom-left:hover{background: url(../../images/top/bottom-left-hover.png);}
.top-main .bottom .bottom-right{background: url(../../images/top/bottom-right.png);right: -82px;}
.top-main .bottom .bottom-right:hover{background: url(../../images/top/bottom-right-hover.png);}
.top-main .top-load{text-align: center;line-height: 35px;display: none;}
.top-main .top-load img{width: 30px;vertical-align: -9px;}
.top-main .top-load span{font-size:16px;font-weight:400;color:#333333;margin-left: 10px;display: inline-block;}
.img-box{position: relative;font-size: 0;padding:10px 10px 0;}
.img-box>a{width: 275px;height: 196px;display: inline-block;overflow: hidden;}
.img-box>a>img{max-width: 100%;border-radius: 5px;}
.img-box>div.col-date-box{position: absolute;left:10px;bottom: 0;background-color: #ffffff;opacity: .7;filter: alpha(opacity=70);height: 40px;width: 275px;line-height: 40px;border-radius: 0 0 5px 5px;display: none;}
.img-box>div.col-date-box>a{font-size: 14px;color: #333;margin-left: 10px;}
.img-box>div.col-date-box>span{font-size: 14px;color: #333;margin-right: 10px;}
.img-box>div.collect-state{background: url(../../images/fashion_home/icon.png) no-repeat -83px -42px;width: 25px;height: 25px;position: absolute;right:20px;top:20px;cursor: pointer;display: none;}
.img-box>div.collect-state.on-state,.img-box>div.collect-state:hover{background-position: -121px -42px;}
.text-labels{position: relative;height: 90px;}
.text-labels .text-position{position: absolute;left:0;top:0;width: 275px;border-radius: 0 0 5px 5px;padding:0 10px 10px;z-index: 10;}
.text-position>p{padding-top: 13px;}
.text-position>p>a{width: 100%;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;font-size: 18px;color: #333;line-height: 30px;display: block;}
.text-position>div{height: 22px;overflow: hidden;padding-top: 10px;}
.text-position>div>span{margin-bottom: 10px;display: inline-block;}
.text-position>div>span>a{font-size: 14px;color: #646464;margin-right: 10px;display: inline-block;}
.text-position>div>span+span>a{border-left: 1px solid #dfdfdf;padding-left: 10px;}
.text-position>div>span:hover a{color: #d8b056;}
.bottom-list ul>li:hover{background-color: #fff;}
.bottom-list ul>li:hover .img-box>div.col-date-box{display: block;}
.bottom-list ul>li:hover .text-position{background-color: #fff;}
.bottom-list ul>li:hover .text-position>p>a{white-space: normal;}
.bottom-list ul>li:hover .collect-state{display: block;}


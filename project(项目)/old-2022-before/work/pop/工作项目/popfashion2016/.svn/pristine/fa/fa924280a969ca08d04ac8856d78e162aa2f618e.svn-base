						<div class="items">
							<div class="letter" name="STAR"><i></i></div>
						</div>
						<!--品牌名开始-->
						{foreach $books as $val}
						<div class="items">
							<div class="letter" name="{$val}">{$val}</div>
						</div>
						{/foreach}

.nav-right{position: fixed;z-index: 10; width: 45px;background: #fff;right: 0px;top: 26%;font-size: 0;}
.nav-right>ul>li{text-align: center;margin-top: 1px;background: #333;position: relative;width: 45px;cursor: pointer;}
.nav-right>ul>li:hover{background: #e8a135;}
.nav-right>ul>li:hover .nav-detail{display: block;}
.nav-right>ul>li>p{line-height: 15px; color: #fff;font-weight: bold;font-size: 14px;}
.nav-right>ul>li>i{display:inline-block;*display: inline;*zoom: 1; vertical-align: middle; width: 18px;height: 18px;margin-bottom: 5px;}
.nav-r1,.nav-r2{padding-top: 12px;height: 93px;}
.nav-r3{padding-top: 20px;height: 85px;}
.nav-r4{display: none; padding-top: 6px;height: 40px;}
.nav-right>ul>.nav-r4>i{margin-bottom: 3px;}
.nav-r1>.nav-detail{display: none; padding-right: 8px;width: 120px;height: 35px;line-height: 35px;text-align: center;font-size: 14px;color: #e8a135;font-weight: bold;position: absolute;left: -138px;top: 35px;background: url(../images/i5.png) center top no-repeat;}
.nav-r3>.nav-detail{display: none; padding: 112px 8px 0 0px;width: 120px;height: 68px;line-height: 20px;text-align: center;font-size: 12px;color: #000;position: absolute;left: -138px;top: -32px;background: url(../images/i6.png) center top no-repeat;}
.nav-right>ul>.nav-r4>p{font-size: 12px;}

.nav-r1>i{background: url(../images/isions.png) -146px -30px no-repeat;}
.nav-r2>i{background: url(../images/isions.png) -105px -30px no-repeat;}
.nav-r3>i{background: url(../images/isions.png) -180px -30px no-repeat;}
.nav-r4>i{background: url(../images/isions.png) -216px -30px no-repeat;}


@keyframes frames1{
    0%{
    	opacity: 1;
        -o-transform: translateX(0px);
        -ms-transform: translateX(0px);
        -moz-transform: translateX(0px);
        transform: translateX(0px);
    }
    39.9999%{
    	opacity: 0;
    	-o-transform: translateX(-140px);
        -ms-transform: translateX(-140px);
        -moz-transform: translateX(-140px);
        transform: translateX(-140px);
    }
    40%{
    	opacity: 0;
    	-o-transform: translateX(140px);
        -ms-transform: translateX(140px);
        -moz-transform: translateX(140px);
        transform: translateX(140px);
    }
    100%{
    	opacity: 1;
    	-o-transform: translateX(0px);
        -ms-transform: translateX(0px);
        -moz-transform: translateX(0px);
        transform: translateX(0px);
    }
}

@-webkit-keyframes frames1{
    0%{
    	opacity: 1;
        -o-transform: translateX(0px);
        -ms-transform: translateX(0px);
        -moz-transform: translateX(0px);
        transform: translateX(0px);
    }
    39.9999%{
    	opacity: 0;
    	-o-transform: translateX(-140px);
        -ms-transform: translateX(-140px);
        -moz-transform: translateX(-140px);
        transform: translateX(-140px);
    }
    40%{
    	opacity: 0;
    	-o-transform: translateX(140px);
        -ms-transform: translateX(140px);
        -moz-transform: translateX(140px);
        transform: translateX(140px);
    }
    100%{
    	opacity: 1;
    	-o-transform: translateX(0px);
        -ms-transform: translateX(0px);
        -moz-transform: translateX(0px);
        transform: translateX(0px);
    }
}


@keyframes frames2{
    0%{
    	transform: rotateY(0deg);
    	-moz-transform: rotateY(0deg);
    	-ms-transform: rotateY(0deg);
    }
    100%{
    	transform: rotateY(360deg);
    	-moz-transform: rotateY(360deg);
    	-ms-transform: rotateY(360deg);
    }
}

@-webkit-keyframes frames2{
    0%{
    	transform: rotateY(0deg);
    	-webkit-transform: rotateY(0deg);
    }
    100%{
    	transform: rotateY(360deg);
    	-webkit-transform: rotateY(360deg);
    }
}

@-ms-keyframes frames2{
    0%{
    	transform: rotateY(0deg);
    	-ms-transform: rotateY(0deg);
    }
    100%{
    	transform: rotateY(360deg);
    	-ms-transform: rotateY(360deg);
    }
}



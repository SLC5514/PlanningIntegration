/* 智能助力 */
.intel-section{background: #fff;}
.intel-title{color: #333;font-size: 36px;text-align: center;padding: 60px 0 40px;}
.intel-section>p{font-size: 16px;color: #666;line-height: 30px;text-align: center;}

.intel-section>ul{width: 1200px;margin: 0 auto;padding-top: 46px;height: 284px;padding-bottom: 50px;}
.intel-section>ul>li{float: left;width: 200px;height: 274px;}
.intel-section>ul>li>a{text-align: center; color: #333; display: block;padding-top: 30px;width: 200px;height: 244px;text-align: center;font-size: 0;}

.intel-section>ul>li>a>img{width: 92px;}

.intel-section>ul>li>a>p{padding: 36px 0 20px;font-size: 20px;transition: all .2s ease-in-out;}
.intel-section>ul>li>a>span{font-size: 14px;display: none;}

.intel-section>ul>li:hover{box-shadow: 0 0 5px 2px #ddd;}
.intel-section>ul>li:hover p{padding: 16px 0 20px;}
.intel-section>ul>li:hover a{color: #d8b056;}
.intel-section>ul>li:hover span{display: inline;}


/* 展示列表 */
.hlist-section{background: #eee;padding-bottom: 80px;}
.hlist-box{width: 1200px;overflow: hidden;margin: 0 auto;}
.hlist-box>ul{width: 1270px;padding-top: 80px;height: 800px;}
.hlist-box>ul>li{float: left;width: 270px;margin-right: 40px;margin-bottom: 40px;}
.hlist-box>ul>li>a{display: block;width: 100%;background: #fff;position: relative;}
.hlist-box>ul .pic-b{background: #ddd;width: 270px;height: 180px;line-height: 180px;font-size: 0;overflow: hidden;}
.hlist-box>ul .pic-b>img{width: 100%;vertical-align: middle;}
.hlist-box>ul .hlist-c{height: 180px;}
.hlist-box>ul .hlist-c1{font-size: 24px;font-weight: bold;color: #333;text-align: center;padding: 33px 0 15px;}
.hlist-box>ul .hlist-c2{width: 30px;height: 2px;background: #d8b056;margin: 0 auto;}
.hlist-box>ul .hlist-c3{padding: 0 10px; font-size: 14px;color: #999;text-align: center;line-height: 20px;padding-top: 15px;}

.hlist-box>ul .hlist-detail{height: 100%;width: 100%;position: absolute;z-index: 2;left: 0;top: 0;background: #d8b056;opacity: 0;transition: all .4s ease-in-out;filter:alpha(opacity=0);}
.hlist-box>ul .hlist-detail1{font-size: 24px;font-weight: bold;color: #fff;text-align: center;padding: 68px 0 15px;}
.hlist-box>ul .hlist-detail2{width: 30px;height: 2px;background: #fff;margin: 0 auto;}
.hlist-box>ul .hlist-detail3{padding: 0 5px; font-size: 14px;color: #fff;text-align: center;line-height: 20px;padding-top: 15px;height: 112px;}
.hlist-box>ul .hlist-detail4{font-size: 14px;color: #fff;width: 228px;height: 38px;line-height: 38px;border: 1px solid #fff;border-radius: 1px;text-align: center;margin: 0 auto;}

.hlist-box>ul>li:hover .hlist-detail{opacity: 1;filter:alpha(opacity=100);}

.hlist-section>.hlist-btns{padding-top: 40px;text-align: center;font-size: 0;}
.hlist-section>.hlist-btns>a{display: inline-block;color: #000;font-size: 16px;width: 178px;height: 46px;line-height: 46px; border: 1px solid #666;background: #fff;margin: 0 15px;border-radius: 3px;}
.hlist-section>.hlist-btns>a.hlist-btn-cur{color: #fff;background: #666;}

.hlist-section>.hlist-btns>a:hover{background: #d8b056;border: 1px solid #d8b056;color: #fff;}

.bottom-section{padding-top: 5px;color: #6a6a6a;background-color: #fff;}
.bottom-section>h3{display: block;margin-top: 116px;font-size: 30px;text-align: center;position: relative;}
.bottom-section>h3>i{width: 100%;height: 1px;background: #6a6a6a;display: block;}
.bottom-section>h3>span{background: #fff;position: absolute;padding: 10px 45px;top: -33px;left: 50%;margin-left: -105px;}
.contact-div{font-size: 18px;float: left;margin-left: 20px;}
.contact-div>span{font-size: 30px;display: block;}
.contact-div>button{display: inline-block;width: 180px;height: 40px;line-height: 40px;margin: 45px auto 170px;background-color: #d8b056;color: #fff;border-radius: 2px}
.contact-div>button:hover{background-color: #f4cb69;}
.contact-div>p{font-size: 14px;line-height: 30px;}
.contact-div>p>span{font-size: 30px;color: #d8b056;vertical-align: -2px}

.form-div{float: right;margin-right: 20px;}
.form-div>h3{font-size: 16px;margin-bottom: 30px;}

.form-list{width: 482px;font-size: 14px;margin-bottom: 35px;color: #fff;}
.form-list>li{position: relative;height: 40px;line-height: 40px;padding: 0 18px;margin-bottom: 30px;border: 1px solid #9fa5ab;}
.form-list>li.select-itme{padding-right: 0px;}
.form-list>li.txt-box{height: auto;line-height: normal;}
.form-list>li.txt-box>textarea{width: 100%;height: 96px;padding: 12px 0;display: block;background-color: transparent;color: #fff;overflow-y: auto;resize:none}
.form-list>li>label{display: inline-block;width: 80px;}
.form-list>li>input{font-size: 14px;padding: 10px 0;width: 320px;margin-left: 15px;border-style: none;background-color: transparent;color: #fff}
.form-list>li>select{-webkit-appearance: none !important;-moz-appearance: none !important;font-size: 14px;padding: 10px 0;width: 348px;margin-left: 15px;border-style: none;background-color: transparent;color: #fff;background-image: url(../../images/home/icon7.png);background-repeat: no-repeat;background-position: 100% center;}
.form-list>li>span{z-index: 2;position: absolute;right: 0px;top: 36px;width: 24px;height: 24px;background-position: -423px -78px;opacity: 0;filter: alpha(opacity=0);}
.form-list>li>p{display: none;z-index: 2;position: absolute;left: 0;top: 35px;font-size: 12px;color: #f75132;}

.sub-btn{display: block;width: 482px;padding: 10px 0;font-size: 16px;color: #fff;background-color: #d8b056;border-radius: 2px;}
.sub-btn:hover{background-color: #f4cb69;}




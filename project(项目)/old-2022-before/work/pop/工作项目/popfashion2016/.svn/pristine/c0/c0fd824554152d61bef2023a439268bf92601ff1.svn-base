							<p class="r_title clearfix"><span class="fl"><i></i>时尚商学院推荐</span></p>
							<ul class="clearfix">
								{foreach $res.vendorads as $val}
								<li><a href="{$val.sAdLinkAddr}" target="_blank" title="{$val.sCompanyName}"><img src="{$val.sAdImageUrl}" alt="pic"><p class="name_bg">{$val.sCompanyName}</p></a></li>
								{/foreach}
							</ul>
						</div>
						{/if}
						<div class="report_bottom_infor">
							<div class="information_box">
								<img class="img_left" src="{STATIC_URL3}/global/images/report_ewm1.png" alt="pic">
							</div>
						</div>
					</div>
				{/if}
			</div>

			<!--相关面料-->
			{if $showFabric && !$isPdf}
			<div class="relevant-about js-relevant-about" id="report-index1">
				<div class="w-1112">

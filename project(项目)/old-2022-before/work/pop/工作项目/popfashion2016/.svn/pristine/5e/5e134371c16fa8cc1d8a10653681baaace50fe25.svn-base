    .section4>.con-lf>div>div>.it-scolor{width:3.5rem;height:3.5;margin-left:-1.75rem;margin-top:-1.75rem;}
    .section4>.con-lf>div>div>.it-scolor>img{width:100%;height:100%;}
    .section4>.con-lf>ul{margin-top:0.375rem;}
    .section4>.con-lf>ul>li{width:4rem;height:2.25rem;margin-left:0.75rem;}
    .section4>.con-lf>ul>li>img{width:100%;height:100%;}

    /* section5 */
    .section5{padding-top:2rem;width:94%;margin:0 auto;padding-bottom:3rem;}

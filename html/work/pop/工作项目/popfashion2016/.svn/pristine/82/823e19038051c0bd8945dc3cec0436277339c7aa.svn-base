	.banner-prev{top: 150px;left: 60px;margin-left: 0; }
	.banner-next{top: 150px;right: 60px;margin-right: 0;}
	.banner-navs{top: 236px;margin-right: -750px;}
	
	.report-section{width: 1500px;margin: 0 auto;}
	
	.list-nav-l{width: 340px;height: 130px;}
	.report-search-box>input.fl{width: 284px;}
	/* .list-nav{margin-left: -750px;} */
	.list-nav-r{width: 1120px;height: 110px;margin-top: 20px;}
	.list-nav-r>ul>li>a{height: 88px;padding-top: 22px;}
	.list-nav-r>ul>li>a>p{font-size: 16px;}
	
	.rightc_main .report-list-new{width: 1530px;}
	
	.list-nav-l>h2{font-size: 24px;padding: 12px 0;}
	.list-nav-l>.this-infor{right: 20px;top: 18px;}
	.list-nav-l>p{margin-top: 10px;}
}


















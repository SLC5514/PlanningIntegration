{extends file="layout.html"}
{block name="selfcss"}
<link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/pop_layer.css?{STATIC_CHANGE_TIME}">
<link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/lists/video-list-inside.css?{STATIC_CHANGE_TIME}">
{/block}
{block name="header"}{/block}
{block name="main"}
<input id="encrypt" type="hidden" data-sign="{$sign}" data-timestamp="{$timeStamp}" data-token="{$token}">
<input type="hidden" id="link" data-col="{$columnId}" data-sor="{$link['sortRandom']}" data-url="{if isset($link['url'])}{$link['url']}{/if}" data-param="{if isset($link['param'])}{$link['param']}{/if}" data-id="{$id}" data-videoType="{$data.video_type}" data-bra="{$data.brand_tid}" {if $data.video_type == 1} data-reg="{$data.region}" {/if} data-t="presscon" data-search="{if isset($link['search'])}{$link['search']}{/if}" data-reg={$reg}/>
<div class="video-detail-section">
    <div class="video-detail-header clearfix">
        <div class="web-logo js-web-logo fl">
            <a href="/" target="_blank">
                <img src="{STATIC_URL2}/global/images/lists/video_list/pop_logo.png"/>
            </a>
        </div>
        <div class="video-seleted-labels-section fl">
            {if $data.brand_tid}
                <a class="brand-selected-labels js-brand-selected" data-bra="{$data.brand_tid}">{$data.brand_name}</a>
            {/if}
            <span class="split-line"></span>
            <div class="season-seleted-labels-box js-swiper-season-main">
                <div class="season-seleted-labels-content js-season-seleted-labels js-swiper-child">
                    {if $filterData.iSeason}
                    {foreach $filterData.iSeason as $id => $name}
                        <a {if $id == $filter.seasonId} class="select-on" {/if} data-sea="{$id}">{$name}</a>
                    {/foreach}
                    {/if}
                </div>
                <a class="labels-switch labels-switch-prev js-switch-btn js-swiper-switch-prev"></a>
                <a class="labels-switch labels-switch-next js-switch-btn js-swiper-switch-next"></a>
                <div class="b-nav-mask"></div>
            </div>
        </div>
    </div>
    <div class="video-detail-infor {if empty($data.memo) && empty($data.tidbits_id) && empty($data.video_id)}video-detail-infor-blank{/if}">
        <div class="contentW clearfix">
            <div class="video-detail-infor-lf fl">
                <div class="video-detail-infor-main">
                    <h1>{$data.title}</h1>
                    {if $data.video_type == 1}
                    <p class="video-labels">
                        {if $data.seasonName}
                        <span>{$data.seasonName}</span>
                        {/if}
                        {if $data.regionName}
                        <span><em></em>{$data.regionName}</span>
                        {/if}
                        {if $data.brand_name}
                        <span><em></em>{$data.brand_name}</span>
                        {/if}
                        {if $data.genderName}
                        <span><em></em>{$data.genderName}</span>
                        {/if}
                    </p>
                    {else}
                    <p class="video-labels">
                        {if $data.seasonName}
                        <span>{$data.seasonName}</span>
                        {/if}
                        {if $data.brand_name}
                        <span><em></em>{$data.brand_name}</span>
                        {/if}
                        {if $data.genderName}
                        <span><em></em>{$data.genderName}</span>
                        {/if}
                    </p>
                    {/if}
                    {if $P_UserType!=4}
                    {if $bIsCollect == 1}
                    <a class="collect-status" id="collect" href="javascript:void(0);" data-iscollected="{$bIsCollect}" data-col="{$columnId}" data-tab="video"  data-id="{$data.id}" data-type="" data-para="" title="已收藏"><i></i><span>已收藏</span></a>
                    {else}
                    <a class="collect-status nzt" id="collect" href="javascript:void(0);" data-iscollected="{$bIsCollect}" data-col="{$columnId}" data-tab="video" data-id="{$data.id}" data-type="" data-para="" title="收藏"><i></i><span>收藏</span></a>
                    {/if}                   
                    {/if}

                    {if !empty($data.memo) && $data.video_type == 2}
                    <div class="insprition-intro">
                        {$data.memo}
                    </div>
                    {/if}

                </div>
            </div>

            {if ( ($data.video_type == 1) && (  !empty($data.video_id) || !empty($data.tidbits_id) ) ) || ($data.video_type == 2 && !empty($data.video_id))}
            <!--视频是否显示-->
            <div class="video-detail-infor-rg fr">
                <div class="video-detail-switch-main">
                    <div class="switch-video-btn js-switch-video-btn">
                        <a data-videoId="{$data.video_id}" data-type="video" {if $data.video_type == 2} style="display: none;" {/if} {if empty($data.video_id)} style="display: none;" {else} class="v-on"{/if}>发布会视频</a>
                        <a data-videoId="{$data.tidbits_id}" data-type="tidbits" {if empty($data.video_id) && !empty($data.tidbits_id)} class="v-on" {/if} {if empty($data.tidbits_id)} style="display: none;"{/if}><i></i>幕后花絮</a>
                    </div>
                    <div class="play-video-box">
                        <div class="play-video-img">
                            <div class="close-video-positiion"></div>
                            <div id="runwayPlayer"></div>
                        </div>
                    </div>
                </div>
            </div>
            {/if}
        </div>
    </div>
    {if $data}
    <div class="video-lists-section">
        <div class="con_width">
            <div class="empty-data-recommend">
                {if $data.video_type == 2 && !empty($videoRecommend)}
                <p>其他广告大片推荐</p>
                <div class="lookbook-recommend-box">
                    <div class="swiper-container lookbook-recommend-list js-recommend-list">
                        <ul class="swiper-wrapper clearfix">
                            {foreach $videoRecommend as $item}
                            <li class="swiper-slide">
                                <div class="pic-box">
                                    <a href="{$item.inside_url}" target="_blank">
                                        {if $item.video_tag == 2}
                                        {$item.cover}
                                        {else}
                                        <img src="{$item.cover}"/>
                                        {/if}
                                    </a>
                                    <div class="hover-link">
                                        <div>
                                            <a href="{$item.inside_url}" target="_blank"><i></i>完整浏览</a>
                                        </div>
                                        <p class="update-time"><i></i>{$item.publish_time|date_format:'%Y-%m-%d'}</p>
                                    </div>
                                </div>
                                <div class="image-title">{$item.title}</div>
                            </li>
                            {/foreach}
                        </ul>                        
                    </div>
                    <div class="recommend-switch">
                        <a class="recommend-prev js-recommend-prev"></a>
                        <a class="recommend-next js-recommend-next"></a>
                    </div>
                </div>
                {/if}
                {if $data.video_type == 1 && !empty($videoRecommend)}
                <p>其他发布会推荐</p>
                <div class="runway-recommend-box">
                    <div class="swiper-container runway-recommend-list js-recommend-list">
                        <ul class="swiper-wrapper clearfix lazyload">
                            {foreach $videoRecommend as $item}
                            <li class="swiper-slide">
                                <a href="{$item.inside_url}" target="_blank">
                                    {if $item.video_tag == 2}
                                    {$item.cover}
                                    {else}
                                    <img src="{$item.cover}"/>
                                    {/if}
                                    <div class="image-title">
                                        <span>{$item.title}</span>
                                    </div>
                                </a>
                            </li>
                            {/foreach}
                        </ul>
                    </div>
                    <div class="recommend-switch">
                        <a class="recommend-prev js-recommend-prev"></a>
                        <a class="recommend-next js-recommend-next"></a>
                    </div>
                </div>
                {/if}
            </div>
        </div>
    </div>
    {/if}
</div>
<div  class="pop_layer_page pop_layer_page1" id="pop_layer">
    {include file="bookpresscon_layer_template.html"}
</div>
{/block}
{block name="other"}
<div id="loadingBg"><div class="noPic"></div></div>
{/block}
{block name="self"}
<script type="text/javascript" src="{STATIC_URL3}/global/js/secret/crypto-js/crypto-js.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL3}/global/js/secret/jsencrypt.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL3}/global/js/fashion/regionAction.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL3}/global/js/fashion/waterFall.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL3}/global/js/common/template.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL2}/global/js/common/imagesloaded.pkgd.min.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL2}/global/js/fashion/aggregation.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL2}/global/js/fashion/util.collect.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="https://player.youku.com/jsapi"></script>
<script type="text/javascript" src="{STATIC_URL2}/global/js/lib/swiper/swiper.min.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL3}/global/js/lists/video-list-inside.js?{STATIC_CHANGE_TIME}"></script>

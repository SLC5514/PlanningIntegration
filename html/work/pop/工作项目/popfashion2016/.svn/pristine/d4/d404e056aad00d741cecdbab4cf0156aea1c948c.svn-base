<script>
	$(function(){
		var _index=0,lun_timer=null, is_lun=false,review_ul=$('.js-isions-list'),_len=$('.js-isions-list>li').length/2,list_w=$('.js-isions-list>li').eq(0).outerWidth(true)||500;

    	//自动轮播启动
    	function lunSetInt(){
    		clearInterval(lun_timer);
    		lun_timer = setInterval(function(){
    			is_lun=true;
	    		_index++;
	    		if(_index>_len){
	    			_index=1;
	    			review_ul.stop().css({
		    			"margin-left":"0px"
		    		});
	    		}

	    		if(_index == _len){
	    			$('.js-list-nv>li').eq(0).addClass('item-sec').siblings().removeClass('item-sec');
	    		}else{
	    			$('.js-list-nv>li').eq(_index).addClass('item-sec').siblings().removeClass('item-sec');
	    		}
	    		review_ul.stop().animate({
	    			"margin-left":_index*-1*list_w+"px"
	    		},500,function(){
	    			is_lun=false;
	    		});
    		},5000);
    	}

    	//移入轮播区域
    	$('.js-isions-list>li').hover(function(){
    		clearInterval(lun_timer);
    	},function(){
    		lunSetInt();
    	});


    	$('.js-left-btn').on('click',function(){
    		if(is_lun==true){
    			return;
    		}
    		clearInterval(lun_timer);
    		is_lun=true;		//正在轮播
    		_index--;
    		if(_index<0){
    			_index=_len-1;
    			review_ul.stop().css({
	    			"margin-left":_len*-1*list_w+"px"
	    		});
    		}

    		$('.js-list-nv>li').eq(_index).addClass('item-sec').siblings().removeClass('item-sec');
    		review_ul.stop().animate({
    			"margin-left":_index*-1*list_w+"px"
    		},500,function(){
    			is_lun=false;
    			lunSetInt();
    		});
    	});

    	$('.js-right-btn').on('click',function(){
    		if(is_lun==true){
    			return;
    		}
    		clearInterval(lun_timer);
    		is_lun=true;
    		_index++;
    		if(_index>_len){
    			_index=1;
    			review_ul.stop().css({
	    			"margin-left":"0px"
	    		});
    		}

    		if(_index == _len){
    			$('.js-list-nv>li').eq(0).addClass('item-sec').siblings().removeClass('item-sec');
    		}else{
    			$('.js-list-nv>li').eq(_index).addClass('item-sec').siblings().removeClass('item-sec');
    		}
    		review_ul.stop().animate({
    			"margin-left":_index*-1*list_w+"px"
    		},500,function(){
    			is_lun=false;
    			lunSetInt();
    		});
    	});

    	$('.js-list-nv>li').on('click',function(){
    		var i_index = $(this).index();
    		if(is_lun==true || $(this).hasClass('item-sec')){
    			return;
    		}
    		clearInterval(lun_timer);
    		is_lun=true;
    		if(_index>=_len){
    			review_ul.stop().css({
	    			"margin-left":"0px"
	    		})
    		}else if(_index<0){
    			review_ul.stop().css({
	    			"margin-left":_len*-1*list_w+"px"
	    		})
    		}

    		_index = i_index;
    		$('.js-list-nv>li').eq(_index).addClass('item-sec').siblings().removeClass('item-sec');
    		review_ul.stop().animate({
    			"margin-left":_index*-1*list_w+"px"
    		},500,function(){
    			is_lun=false;
    			lunSetInt();
    		});

    	});


    	//返回顶部
    	$('.js-scrollt').on('click',function(){
    		$('html,body').animate({
    			'scrollTop':'0px'
    		},400);
    	});


    	var scroll_timer = null;
    	//根据页面滚动高度执行动画
    	setTimeout(function(){
    		scrollAni();
    	},1200);

    	function scrollAni(){

    		var w_t = $(window).scrollTop(), _i=0;
	    	if(w_t > 200){
				$('.js-scrollt').fadeIn(50);
			}else{
				$('.js-scrollt').fadeOut(50);
			}
			clearTimeout(scroll_timer);
			scroll_timer = setTimeout(function(){
				if(w_t < 550){
					_i = 0;
				}else if(w_t >= 550 && w_t < 1320){
					_i = 1;
				}else if(w_t >= 1320 && w_t < 2300){
					_i = 2;
				}else{
					_i = 3;
				}
				aniSet(_i);

			},240);

    	}

    	//执行动画
    	function aniSet(i){
    		i = i||0;
    		var _ele = $('.js-season-section>div').eq(i);
    		_ele.data('sec', 1);
    		var ani_class = 'set'+(i+1);
    		_ele.addClass(ani_class);

    		if(ani_class == 'set3'){
    			lunSetInt();
    		}

    		clearTimeout(scroll_timer);
    	}
    	//页面滚动
    	$(window).on('scroll',function(){
    		scrollAni();
    	});
    })
</script>

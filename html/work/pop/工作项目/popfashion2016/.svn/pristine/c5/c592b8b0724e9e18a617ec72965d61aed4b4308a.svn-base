 /* lightbox //////////////////////////////////////////// */
#jquery-overlay { position: absolute; top: 0; left: 0; z-index: 130; width: 100%; height: 500px; }
#jquery-lightbox { position: absolute; top: 0; left: 0; width: 100%; z-index: 140; line-height: 0; text-align: center; }
#jquery-lightbox a img { border: none; }
#lightbox-container-image-box { position: relative; width: 250px; height: 250px; margin: 0 auto; background-color: #fff; }
#lightbox-container-image { padding: 10px; }
#lightbox-loading { position: absolute; top: 40%; left: 0%; width: 100%; height: 25%; line-height: 0; text-align: center; }
#lightbox-nav { position: absolute; top: 0; left: 0; width: 100%; height: 100%; z-index: 10; }
#lightbox-container-image-box > #lightbox-nav { left: 0; }
#lightbox-nav a { outline: none; }
#lightbox-nav-btnPrev, #lightbox-nav-btnNext { display: block; width: 40%; height: 100%; zoom: 1; }
#lightbox-nav-btnPrev { left: 0; float: left; }
#lightbox-nav-btnNext { right: 0; float: right; }
#lightbox-container-image-data-box { width: 100%; margin: 0 auto; padding: 0 10px 0; font: 10px/1.4em tahoma; background-color: #fff; overflow: auto; }
#lightbox-container-image-data { padding: 0 10px; color: #666; }
#lightbox-container-image-data #lightbox-image-details { float: left; width: 70%; text-align: left; }
#lightbox-image-details-caption { font-weight: 700; }
#lightbox-image-details-currentNumber { clear: left; display: block; padding-bottom: 1.0em; }
#lightbox-secNav-btnClose { float: right; width: 66px; padding-bottom: 0.7em; }

center table td ul li{text-align:left;}
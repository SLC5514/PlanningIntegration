@charset "utf-8";


/* body{background: #323940;} */


.design-contain{min-height: 700px;}
.design-frame-box{/* min-height: 500px; */height: 810px;position: relative;background-color: #323940;}
.design-frame-box .frame-box{position: absolute;height: 400px;width: 470px;top:50%;margin-top: -200px;left: 50%;margin-left: -235px;}
.design-frame-box .frame-box .box-img{height: 175px; background: url(../../images/common/vip-smile1.png) center no-repeat;}
.design-frame-box .frame-box>span{padding: 60px 0 45px;text-align: center;color: #fff;font-size: 16px;display: block;}
.design-frame-box .frame-box>.link-msg{text-align: center;font-size: 0;}
.design-frame-box .frame-box>.link-msg>span{font-size: 14px;margin-right: 30px;display: inline-block;color: #fff;width: 220px;height: 40px;line-height: 40px;background: #d8b056;border-radius: 3px;cursor:pointer;}
.design-frame-box .frame-box>.link-msg>span:hover{background-color: #dba428;}
body .loading-div{display: none;}
body .bg-div{display: none;}







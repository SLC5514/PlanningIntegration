body{background-color: #f7f7f7;}
/* 首页banner */
.fashion-home-banner{width: 100%;height: 310px;background:url(../../images/fashion_home/swiper_bg1200.png) no-repeat center center;overflow: hidden;}
.swiper-container-banner{width: 1200px;overflow: hidden;height: 270px;position: relative;margin:20px auto;text-align: center;}
.swiper-container-banner>ul.roundabout-holder{position: relative;width: 675px;height: 270px;margin:0 auto}
.roundabout-holder>li{width: 675px;height: 270px;}
.roundabout-holder>li>a{display: block;width: 100%;height: 100%;}
.roundabout-holder>li>a>img{width: 100%;height: 100%;border-radius: 8px;}
.swiper-btn-switch{position: absolute;top: 50%;width: 52px;height: 51px;cursor: pointer;z-index:30;margin-top: -26px;}
.swiper-prev{background: url(../../images/fashion_home/s_left.png) no-repeat;left: 42px;}
.swiper-next{background: url(../../images/fashion_home/s_right.png) no-repeat;right: 48px;}
.swiper-prev:hover{background: url(../../images/fashion_home/s_left_hover.png) no-repeat;}
.swiper-next:hover{background: url(../../images/fashion_home/s_right_hover.png) no-repeat;}
.pagination-switch{position: absolute; bottom: 12px;font-size: 0;display: inline-block;z-index:30;}
.pagination-switch>span{width: 20px;height: 8px;display: inline-block;background: url(../../images/fashion_home/icon.png) no-repeat -20px -154px;}
.pagination-switch>span.swiper-on{background-position: -45px -154px;}


/* 个性标签 */
.interest-tags-contain{width: 100%;overflow: hidden;margin-bottom: 35px;}
.interest-tags-selected{text-align: center;margin-top: 30px;}
.interest-tags-selected>p{max-width: 380px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;color: #333;font-size: 14px;display: inline-block;vertical-align: middle;}
.interest-tags-selected>a.set-preference{width: 96px;height: 30px;background-color: #fff;border:1px solid #cfcfcf;border-radius: 15px;display: inline-block;vertical-align: middle;line-height: 30px;font-size: 14px;color: #333;margin-left: 10px;cursor: pointer;}
.interest-tags-selected>a.set-preference:hover{border-color: #d8b056;}
.interest-tags-selected>a.open-sub-account{width: 180px;height: 30px;line-height: 30px;text-align: center;color: #333;font-size: 14px;background: #fff;border-radius: 15px;float: right;}
.interest-tags-selected>a.open-sub-account:hover{color: #d8b056;}

.interest-tags-box{background: url(../../images/fashion_home/ins_bg.png) no-repeat;height: 290px;margin-top: 25px;padding: 30px 30px 0;}

.my-exclusive-box>h2{font-size: 30px;color: #2b2b2b;}
.my-exclusive-link{margin-top: 17px;}
.my-exclusive-link>a{display: block;font-size: 18px;color: #333;line-height: 24px;margin-bottom: 20px;}
.my-exclusive-link>a>i{background: url(../../images/fashion_home/icon.png?2020022701) no-repeat;display: inline-block;vertical-align: middle;margin-right: 11px;margin-top: -3px;}
.my-exclusive-link>a.is-recommend>i{background-position: -74px -154px;width: 19px;height: 15px;}
.my-exclusive-link>a.is-history>i{background-position: -104px -154px;width: 20px;height: 20px;}
.my-exclusive-link>a.is-collect>i{background-position: -132px -154px;width: 21px;height: 21px;}
.my-exclusive-link>a.is-workbench>i{background-position: -166px -154px;width: 20px;height: 20px;}
.my-exclusive-link>a.is-information>i{background-position: 0 -184px;width: 18px;height: 16px;}
.my-exclusive-link>a:hover{color: #FF7A45;}

.special-area-box{width: 320px;margin-left: 90px;}
.special-area-box>h2{font-size: 30px;color: #2b2b2b;}
.special-area-link{margin-left: -50px;margin-top: 15px;}
.special-area-link>li{padding: 0 5px;float: left;margin-left: 50px;margin-bottom: 34px;position: relative;cursor: pointer;}
.special-area-link>li>a>span{font-size: 18px;font-weight: bold;color: #2b2b2b;position: relative;z-index: 1;}
.special-area-link>li>a>i{position: absolute;width:0;height: 8px;background-color: #FBDE5A;bottom: 0;left: 0;z-index: 0;transition: all 0.5s;}
.special-area-link>li:hover i{width: 100%;}

.follow-box{width: 537px;margin-left: 85px;}
.follow-box>h2{font-size: 30px;color: #2b2b2b;}
.follow-box>h2>a{font-size: 14px;color: #2b2b2b;position: relative;top: 17px;cursor: pointer;}
.follow-box>h2>a:hover{color: #D8B056;}
.follow-link{padding-top: 11px;font-size: 0;margin-left: -20px;height: 212px;overflow: hidden;padding-right: 5px;}
.follow-link>span{background: #D4D5D9;width: 118px;height: 38px;display: inline-block;margin-left: 20px;margin-bottom: 20px;cursor: pointer;font-size: 24px;color: #333;text-align: center;line-height: 38px;border-radius: 19px;}
.follow-link>span.on{background-color: #1C232B;color: #fff;}
.follow-link .follow-tags-list{display: inline;font-size: 0;}
.follow-link .follow-tags-list>div{width: 118px;height: 38px;text-align: center;border-radius: 19px;background-color: #f1f1f1;display: inline-block;vertical-align: top;margin-left: 20px;margin-bottom: 20px;position: relative;}
.follow-link .follow-tags-list>div>a{display: inline-block;width: 100%;height: 100%;font-size: 14px;color: #2b2b2b;line-height: 38px;border-radius: 19px;}
.follow-link .follow-tags-list>div>a:hover{background-color: #1C232B;color: #fff;}
.follow-link .follow-tags-list>div>em{background: url(../../images/fashion_home/del.png?2019121701) no-repeat 0 0;width: 122px;height: 46px;position: absolute;right: -5px;top: -5px;display: none;z-index: 2;cursor: pointer;}
.follow-link .follow-tags-list>div>a>span{overflow: hidden;text-overflow: ellipsis;white-space: nowrap;width: 100px;padding: 0 4px;height: 100%;display: inline-block;}

/* 款式流行排行榜 */
.top-ranking-style-contain{padding-top: 10px;}
.ranking-title>h3{font-size: 40px;color: #2b2b2b;line-height: 56px;position: relative;display: inline;}
.ranking-title>h3>i{background: url(../../images/fashion_home/icon.png) no-repeat -76px 0;width: 12px;height: 12px;position: absolute;top: 7px;right: -16px;cursor: pointer;}
.ranking-title .ranking-all-intro{background-color: #fff;width: 360px;padding:10px;font-size: 12px;color: #666666; border:1px solid #eee; position: absolute;line-height: 24px;left: 185px;bottom: 55px;display: none;z-index:30;}
.ranking-title .ranking-all-intro>i{background: url(../../images/fashion_home/arrow.png) no-repeat;width: 11px;height: 8px;display: inline-block;position: absolute;bottom: -8px;left:18px;}
.ranking-title>i:hover .ranking-all-intro{display: block;}
.ranking-eng{font-size: 18px;line-height: 25px;color: #acacac;padding-top: 2px;letter-spacing: 0.9px;}
.top-ranking-box{margin-top: 30px;}
/* 色彩排行榜 */
.color-ranking-content{width: 100%;height: 370px;background-color: #fff;border-radius: 5px;}
.color-ranking-content:hover{box-shadow: 0 0 10px rgba(15,15,15,0.1);}
.intro-title{height: 65px;border-bottom: 1px solid #f1f1f1;}
.intro-title>h4{margin-left: 30px;font-size: 20px;color: #333;display: inline-block;position: relative;margin-top: 20px;}
.intro-title>h4>i{background: url(../../images/fashion_home/icon.png) no-repeat -76px 0;width: 12px;height: 12px;display: inline-block;position: absolute;right:-6px;top: 0;}
.color-ranking-content .intro-title>h4>i{right: -16px;}
.ranking-per-intro{background-color: #fff;width: 360px;padding:10px;font-size: 12px;color: #666666; border:1px solid #eee; position: absolute;line-height: 24px;left: 195px;bottom: 38px;display: none;}
.ranking-per-intro-color{left: 84px;}
.ranking-per-intro>i{background: url(../../images/fashion_home/arrow.png) no-repeat;width: 11px;height: 8px;display: inline-block;position: absolute;bottom: -8px;left: 18px;}
.ranking-per-intro>i{background: url(../../images/fashion_home/arrow.png) no-repeat;width: 11px;height: 8px;display: inline-block;position: absolute;bottom: -8px;left: 18px;}
.season-filter{margin-left: 38px;}


/* 色块 */
.color-ranking-data-box .ranking-all-box{float: left;width: 280px;margin-left: 30px;margin-top: 6px;display: none;}
.color-ranking-data-box .ranking-all-box>div{width: 280px;height: 232px;}
.color-ranking-data-box .ranking-all-box>p{font-size: 14px;color: #333;text-align: center;margin-top: 4px;}
.ranking-all-color .color-value{width: 100%;font-size: 0;margin-top:20px;height: 52px;line-height: 52px;margin-top: 8px;}
.ranking-all-color .color-value>a{width: 25%;height: 40px;display: inline-block;vertical-align: middle;cursor: pointer;box-shadow: 5px 0 7px rgba(0,0,0,0.1);}
.ranking-all-color .color-value>a:first-child{border-radius: 5px 0 0 5px;}
.ranking-all-color .color-value>a:last-child{border-radius: 0 5px 5px 0;}
.ranking-all-color .color-value>a:hover{box-shadow: 0 0 10px rgba(0,0,0,0.1);}
.ranking-all-color .color-value>a.on{height:52px;box-shadow: 0 0 10px rgba(0,0,0,0.1);}
/* 色彩比例条 */
.ranking-top-color{float: left;padding-left: 6px;margin-top: 14px;width: 870px;height: 290px;overflow: hidden;display: none;}
.ranking-top-color>ul>li{float: left;text-align: center;width: 85px;margin-bottom: 50px;}
.ranking-top-color>ul>li>a>span{font-size: 14px;color: #333;display: inline-block;}
.ranking-top-color>ul>li>a>p{font-size: 16px;color: #333;width: 100%;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.ranking-top-color>ul>li>a>div{width: 100%;position: relative;height: 200px;margin:8px 0 10px;}
.ranking-top-color>ul>li>a>div>em{display: inline-block;width: 40px;position: absolute;bottom: 0;left:50%;border-radius: 20px;margin-left: -20px;transition: all 0.5s;height: 0;box-shadow: 0 2px 7px 0 rgba(0,0,0,0.10);;}
.ranking-top-color>ul>li:hover a>span{color: #d8b056;}
.ranking-top-color>ul>li:hover a>p{color: #d8b056;}
.ranking-top-color>ul>li:hover a>div>em{box-shadow: 0 2px 7px 0 rgba(0,0,0,0.15);}

/* 品牌图案 */
.brand-pattern-ranking-content{margin-left: -30px;}
.ranking-content{background-color: #fff;border-radius: 5px;margin-top: 30px;float: left;margin-left: 30px;width: 585px;height: 320px;}
.ranking-content:hover{box-shadow: 0 0 10px rgba(15,15,15,0.1);}
.ranking-content>h4{height: 68px;border-bottom: 1px solid #f2f2f2;line-height: 68px;font-size: 20px;color: #333;padding-left: 40px;position: relative;}
.ranking-content>h4>i{background: url(../../images/fashion_home/icon.png?2019121801) no-repeat -76px 0;width: 12px;height: 12px;position: absolute;top:30px;left:290px;cursor: pointer;} 
.label-filter{width: 138px;height: 30px;display: inline-block;margin-top: 20px;margin-right: 30px;position: relative;}
.label-filter>span{width: 124px;height: 30px;display: inline-block;color: #333;font-size: 14px;background-color: #ecedee;line-height: 30px;padding-left: 16px;cursor: pointer;}
.label-filter>span>i{background: url(../../images/fashion_home/icon.png?2019121801) no-repeat -17px -130px;display: inline-block;width: 11px;height: 6px;position: absolute;right:17px;top: 11px;}
.label-filter-downlist{width: 100%;position: absolute;left:0;top:30px;background-color: #fff;border:1px solid #f1f1f1;visibility: hidden; z-index:11;}
.label-filter-content{max-height: 200px;overflow:hidden; position: relative;}
.label-filter-content>a{display: block;height: 30px;padding-left: 16px;line-height: 30px;cursor: pointer;font-size:14px;}
.label-filter-content>a:hover{color: #d8b056;}
.label-filter>span:hover{color: #d8b056;}
.label-filter>span:hover i{background-position: 0 -130px;}

/* 品牌排行榜 */
.brand-ranking-data-box>ul{width: 585px;height: 254px;position: relative;display: none;}
.brand-ranking-data-box>ul>li{position: absolute;white-space: nowrap;cursor: pointer;}
.brand-ranking-data-box>ul>li>a{border-radius: 50%;display: block;padding:0 5px;text-align: center;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;color: #fff;font-size: 14px;}
.brand-ranking-data-box>ul>li.brand-position1{left:26px;top:86px;}
.brand-ranking-data-box>ul>li.brand-position1>a{width: 132px;height: 142px;background-color: #E33872;line-height: 142px;}
.brand-ranking-data-box>ul>li.brand-position2{left:211px;top:18px;}
.brand-ranking-data-box>ul>li.brand-position2>a{width: 96px;height: 106px;background-color: #E33872;line-height: 106px;}
.brand-ranking-data-box>ul>li.brand-position3{left:438px;top:92px;}
.brand-ranking-data-box>ul>li.brand-position3>a{width: 96px;height: 106px;background-color: #E33872;line-height: 106px;}
.brand-ranking-data-box>ul>li.brand-position4{left:244px;top:130px;}
.brand-ranking-data-box>ul>li.brand-position4>a{width: 96px;height: 106px;background-color: #4D52FF;line-height: 106px;}
.brand-ranking-data-box>ul>li.brand-position5{left:123px;top:14px;}
.brand-ranking-data-box>ul>li.brand-position5>a{width: 68px;height: 78px;background-color: #4D52FF;line-height: 78px;}
.brand-ranking-data-box>ul>li.brand-position6{left:328px;top:13px;}
.brand-ranking-data-box>ul>li.brand-position6>a{width: 61px;height: 71px;background-color: #4D52FF;line-height: 71px;}
.brand-ranking-data-box>ul>li.brand-position7{left:347px;top:92px;}
.brand-ranking-data-box>ul>li.brand-position7>a{width: 68px;height: 78px;background-color: #37BEA8;line-height: 78px;}
.brand-ranking-data-box>ul>li.brand-position8{left:369px;top:177px;}
.brand-ranking-data-box>ul>li.brand-position8>a{width: 55px;height: 65px;background-color: #37BEA8;line-height: 65px;}
.brand-ranking-data-box>ul>li.brand-position9{left:424px;top:21px;}
.brand-ranking-data-box>ul>li.brand-position9>a{width: 50px;height: 60px;background-color: #37BEA8;line-height: 60px;}
.brand-ranking-data-box>ul>li.brand-position10{left:178px;top:120px;}
.brand-ranking-data-box>ul>li.brand-position10>a{width: 49px;height: 59px;background-color: #37BEA8;line-height: 59px;}
.brand-ranking-data-box>ul>li>div{position: absolute;background-color: #fff;box-shadow: 0 1px 4px 0 rgba(0,0,0,0.1);border-radius: 5px 5px 5px 0;padding: 8px 13px;left:50%;top:-48px;line-height: 20px;font-size: 14px;color: #333;z-index:10;display: none;}
.brand-ranking-data-box>ul>li>div>i{background: url(../../images/fashion_home/icon.png) no-repeat -40px -130px;width: 15px;height: 11px;position: absolute;left: 0;bottom: -10px;}
.brand-ranking-data-box>ul>li:hover div{display: block;}
.brand-ranking-data-box>ul>li:hover a{box-shadow: 0 0 2px 2px rgba(0,0,0,0.1);}

/* 图案排行榜 */
.pattern-ranking-data-box>ul{padding:0 30px;}
.pattern-ranking-data-box>ul>li{margin-top: 20px;cursor: pointer;}
.pattern-ranking-data-box>ul>li>a>em{font-size: 14px;color: #333;font-style: normal;width: 30px;display: inline-block;text-align: center;}
.pattern-ranking-data-box>ul>li>a>span{font-size: 14px;color: #333;width: 80px;display: inline-block;text-align: center;margin:0;}
.pattern-ranking-data-box>ul>li>a>div.pattern-color-value-box{width: 340px;height: 12px;display: inline-block;position: relative;}
.pattern-ranking-data-box>ul>li .pattern-color-value-box>div{background-image:linear-gradient(90deg, #7794fe 0%, #4c54fe 100%); background: #4c54fe\0;;width: 100%;height: 100%;border-radius: 20px;position: relative;transition: all 1s;width: 0;}
.pattern-ranking-data-box>ul>li>a>strong{font-size: 14px;color: #333;margin-left: 16px;}
.pattern-ranking-data-box>ul>li div.hover-infor{position: absolute;background-color: #fff;box-shadow: 0 1px 4px 0 rgba(0,0,0,0.1);border-radius: 5px 5px 5px 0;padding: 8px 13px;left:40%;top:-66px;line-height: 20px;font-size: 14px;color: #333;z-index:10;display: none;white-space: nowrap;}
.pattern-ranking-data-box>ul>li div.hover-infor>i{background: url(../../images/fashion_home/icon.png) no-repeat -40px -130px;width: 15px;height: 11px;position: absolute;left: 0;bottom: -10px;}
.pattern-ranking-data-box>ul>li:hover div.hover-infor{display: block;}
.pattern-ranking-data-box>ul>li:hover em,.pattern-ranking-data-box>ul>li:hover span,.pattern-ranking-data-box>ul>li:hover strong{color: #d8b056;}



/* 无数据 */
.top-data-null{text-align: center;display: none;}
.top-data-null>img{margin-top: 50px;}
.top-data-null>div{font-size: 18px;color: #999;line-height: 36px;}
.top-data-null>div>span{max-width: 140px;display: inline-block;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;vertical-align: middle;margin-top: -5px;}
.top-data-null>div em{color: #d8b056;padding-left: 5px;}

/* 加载中 */
.loading-top-data{text-align: center;}
.loading-top-data>img{margin-top: 70px;}


/* 最新报告 - 热门报告 */
.reports-infor-contain{background: url(../../images/fashion_home/m_1200_bg.jpg) no-repeat center top;width: 100%;margin-top: 50px;padding-top: 70px;}
.latest-report-box{width: 786px;}
.latest-report-top{position: relative;}
.latest-report-title>h3{font-size: 40px;color: #fff;}
.latest-report-title>p{font-size: 18px;color: #f1f1f1;padding-top: 8px;line-height: 25px;}
.latest-report-jump{line-height: 30px;height: 30px;position: absolute;right: 15px;bottom: 5px;}
.latest-report-jump>a{background: url(../../images/fashion_home/icon.png?2019121701) no-repeat 0 0;display: inline-block;width: 30px;height: 30px;cursor: pointer;}
.latest-report-jump>a.left-jump{background-position: -107px -105px;float: left;}
.latest-report-jump>a.right-jump{background-position: -153px -105px;float: right;}
.latest-report-jump>a:hover.left-jump{background-position: 0 -42px;}
.latest-report-jump>a:hover.right-jump{background-position: -40px -42px;}
.latest-report-jump>span{font-size: 14px;color: #fff;overflow: hidden;margin:0 10px;width: 28px;float: left;text-align: center;}
.report-list-box{margin-top: 30px;}
.report-list-box>ul{margin-left: -10px;display: none;}
.report-list-box>ul>li{width: 255px;float: left;margin-left: 10px;margin-bottom: 20px;border-radius: 5px;}
.report-list-box>ul>li .img-box{position: relative;font-size: 0;padding:10px 10px 0;}
.img-box>a{width: 235px;height: 168px;display: inline-block;overflow: hidden;}
.img-box>a>img{max-width: 235px;max-height: 168px;border-radius: 5px;}
.img-box>div.col-date-box{position: absolute;left:10px;bottom: 0;background-color: rgba(255,255,255,0.7);height: 40px;width: 235px;line-height: 40px;border-radius: 0 0 5px 5px;display: none;}
.img-box>div.col-date-box>a{font-size: 14px;color: #333;margin-left: 10px;}
.img-box>div.col-date-box>span{font-size: 14px;color: #333;margin-right: 10px;}
.img-box>div.collect-state{background: url(../../images/fashion_home/icon.png) no-repeat -83px -42px;width: 25px;height: 25px;position: absolute;right:20px;top:20px;cursor: pointer;display: none;}
.img-box>div.collect-state.on-state,.img-box>div.collect-state:hover{background-position: -121px -42px;}
.text-labels{position: relative;height: 90px;}
.text-labels .text-position{position: absolute;left:0;top:0;width: 235px;border-radius: 0 0 5px 5px;padding:0 10px 10px;z-index: 10;}
.text-position>p{padding-top: 13px;}
.text-position>p>a{width: 100%;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;font-size: 18px;color: #333;line-height: 30px;display: block;}
.text-position>div{height: 22px;overflow: hidden;padding-top: 10px;}
.text-position>div>span{margin-bottom: 10px;display: inline-block;}
.text-position>div>span>a{font-size: 14px;color: #646464;margin-right: 10px;display: inline-block;}
.text-position>div>span+span>a{border-left: 1px solid #dfdfdf;padding-left: 10px;}
.text-position>div>span:hover a{color: #d8b056;}
.report-list-box>ul>li:hover{background-color: #fff;}
.report-list-box>ul>li:hover .img-box>div.col-date-box{display: block;}
.report-list-box>ul>li:hover .text-position{background-color: #fff;}
.report-list-box>ul>li:hover .text-position>p>a{white-space: normal;}
.report-list-box>ul>li:hover .collect-state{display: block;}

.hot-reports-box{width: 400px;}
.hot-reports-box>h3{font-size: 40px;color: #fff;}
.hot-reports-box>p{font-size: 18px;color: #f1f1f1;padding-top: 8px;}
.hot-reports-content{background-color: #fff;border-radius: 5px;margin-top: 42px;font-size: 0;height: 534px;overflow: hidden;}
.hot-reports-content>em.line-top{width: 140px;height: 1px;background-color: #f1f1f1;display: inline-block;margin:35px 0 0 20px;}
.hot-reports-content>em.line-bottom{float: right;margin-right: 20px;margin-top: 15px;}
.hot-reports-content>ul{margin-top: 16px;}
.hot-reports-content>ul>li{width: 100%;height: 60px;line-height: 60px;margin-bottom: 2px;}
.hot-reports-content>ul>li>a{width: 100%;height: 100%;display: block;}
.r-ranking-num{width: 30px;height: 30px;border-radius: 50%;text-align: center;line-height: 30px;font-size: 20px;color: #a2a2a2;background-color: #f1f1f1;margin-left: 20px;margin-top:15px;float: left;}
.r-ranking-num.first-num{background-color: #fcebf1;color: #df3c72;}
.r-ranking-num.second-num{background-color: #e4e6ff;color: #4c54fe;}
.r-ranking-num.third-num{background-color: #ebf8f6;color: #37bca8;}

/*环绕*/
.type4{
	display: none;
    border-radius: 50%;
    transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    left: -5px;top: -5px;
    border: 5px solid #e2c67b;
    z-index: 1;
}
.spn1{
    position: absolute;
    top: 0px;
    border-radius: 50%;
    top: -5px;
    left: -5px;
    width: 100%;
    height: 100%;
    display: block;
    border: 5px solid #fff;
    border-right-color:transparent;border-top-color:transparent;
    animation: loading4 3s linear infinite normal;
    -ms-animation: loading4 3s linear infinite normal;
    -webkit-animation: loading4 3s linear infinite normal;
    -moz-animation: loading4 3s linear infinite normal;
    -o-animation: loading4 3s linear infinite normal;
}
.spn2{
    position: absolute;
    top: -5px;
    left: -5px;
    border-radius: 50%;
    width: 100%;
    height: 100%;
    display: block;
    border: 5px solid #e2c67b;
    border-right-color:transparent;border-top-color:transparent;
    animation: loading2 3s steps(1,end) infinite normal;
    -ms-animation: loading2 3s steps(1,end) infinite normal;
    -webkit-animation: loading2 3s steps(1,end) infinite normal;
    -moz-animation: loading2 3s steps(1,end) infinite normal;
    -o-animation: loading2 3s steps(1,end) infinite normal;
}















/*css3动画*/
@keyframes loading4{
    0%{ 
        transform: rotate(0deg);
    }
    100%{
        transform: rotate(360deg);
    }
}
@-ms-keyframes loading4{
    0%{ 
        -ms-transform: rotate(0deg);
    }
    100%{
        -ms-transform: rotate(360deg);
    }
}
@-webkit-keyframes loading4{
    0%{ 
        -webkit-transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(360deg);
    }
}
@-moz-keyframes loading4{
    0%{ 
        -moz-transform: rotate(0deg);
    }
    100%{
        -moz-transform: rotate(360deg);
    }
}
@-o-keyframes loading4{
    0%{ 
        -o-transform: rotate(0deg);
    }
    100%{
        -o-transform: rotate(360deg);
    }
}

.default-button, .ani-trans{animation: trans 0.7s steps(12, start) infinite;-webkit-animation: trans 0.7s steps(12, start) infinite;-ms-animation: trans 0.7s steps(12, start) infinite; transform: translateY(-4px);-webkit-transform: translateY(-4px);-ms-transform: translateY(-4px);}

@keyframes trans{
    0%{
		transform: translateY(-4px);
		-webkit-transform: translateY(-4px);
		-ms-transform: translateY(-4px);
    }
    50%{
		transform: translateY(4px);
		-webkit-transform: translateY(4px);
		-ms-transform: translateY(4px);
    }
    100%{
		transform: translateY(-4px);
		-webkit-transform: translateY(-4px);
		-ms-transform: translateY(-4px);
    }
}


.ps .ps__rail-x:hover, .ps .ps__rail-y:hover, .ps .ps__rail-x:focus, .ps .ps__rail-y:focus, .ps .ps__rail-x.ps--clicking, .ps .ps__rail-y.ps--clicking{background-color: transparent;opacity: 1;}
.ps:hover > .ps__rail-x, .ps:hover > .ps__rail-y, .ps--focus > .ps__rail-x, .ps--focus > .ps__rail-y, .ps--scrolling-x > .ps__rail-x, .ps--scrolling-y > .ps__rail-y{opacity: 1;}
.ps__rail-y:hover > .ps__thumb-y, .ps__rail-y:focus > .ps__thumb-y, .ps__rail-y.ps--clicking .ps__thumb-y{width: 6px;background-color: #D9D9D9;}
.ps__thumb-y{ width: 6px; right: 5px; background-color: #D9D9D9;cursor: pointer; transition: background-color .2s linear, width .2s ease-in-out, right .2s ease-in-out; -webkit-transition: background-color .2s linear, width .2s ease-in-out, right .2s ease-in-out; -moz-transition: background-color .2s linear, width .2s ease-in-out, right .2s ease-in-out; -ms-transition: background-color .2s linear, width .2s ease-in-out, right .2s ease-in-out; -o-transition: background-color .2s linear, width .2s ease-in-out, right .2s ease-in-out; }


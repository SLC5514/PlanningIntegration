{
  "name": "vite-v3-ssg",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run server",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build && vite build --ssr",
    "server": "ts-node ./server",
    "server:prod": "cross-env NODE_ENV=production && npm run server",
    "dev:spa": "vite",
    "build:spa": "vue-tsc --noEmit && vite build",
    "serve:spa": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.5.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.9",
    "@vitejs/plugin-vue": "^1.9.3",
    "express": "^4.17.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-ssr": "^0.3.30",
    "vue-tsc": "^0.3.0"
  }
}

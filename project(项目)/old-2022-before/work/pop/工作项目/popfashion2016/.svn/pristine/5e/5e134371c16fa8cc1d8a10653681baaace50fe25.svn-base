    .section3>.con-lf>h2>img{width:100%;height:100%;}
    .section3>.con-lf>p{font-size:0.75rem;margin:2rem 0.85rem 0 1.25rem;color:#666;line-height:1.5rem;}
    .section3>.con-lf>a{display:none;}
    .section3>.con-rg{float:none;width:auto;height:7.75rem;margin:2.5rem 1.25rem 0;overflow: hidden;}
    .section3>.con-rg>div.cb-pic1{left:0;width:9.375rem;height:7.75rem;}
    .section3>.con-rg>div.cb-pic1>img{width:100%;height:100%;}
    .section3>.con-rg>div.cb-pic2{width:6.25rem;height:4.5rem;top:2.625rem;right:0.25rem;}
    .section3>.con-rg>div.cb-pic2>img{width:100%;height:100%;}
    
    /* section4 */
    .section4{padding-top:2.5rem;padding-bottom:2.5rem;width:94%;margin:0 auto;}    

/* banner */
.color-banner{background:url(../images/banner.jpg) no-repeat center top;width:100%;height:550px;text-align: center;overflow: hidden;position: relative;opacity:0;filter:alpha(opacity=0);transition:opacity 0.5s ease 0.3s;-webkit-transition: opacity 0.5s ease 0.3s;}
.color-banner>div{opacity:0;filter:alpha(opacity=0);transition:opacity 1s ease 0.8s;-webkit-transition: opacity 1s ease 0.8s;}
.color-banner>div>h1{font-size:60px;color:#fff;margin-top:160px;letter-spacing:5px;}
.color-banner>div>p{font-size:20px;color:#fff;line-height:36px;margin-top:50px;letter-spacing:1px;}
.color-banner>div>p.h5-title{display:none;}
.color-banner>div>a.h5-reg-btn{display:none;}
/* banner动画 */
.anit-banner{opacity:1;filter:alpha(opacity=100);}
.anit-banner>div{opacity:1;filter:alpha(opacity=100);}


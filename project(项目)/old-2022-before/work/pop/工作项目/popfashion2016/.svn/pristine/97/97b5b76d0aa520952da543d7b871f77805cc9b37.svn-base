{extends file="layout.html"}
{block name="title"}{$title}{/block}
{block name=keywords}{$keywords}{/block}
{block name=description}{$description}{/block}

{block name="selfcss"}
<link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/per.css"/>
{/block}

{block name="main"}
<div class="cross_content">
        {block name="topads"}

							<a target="{if $ad.sNewWindow}_blank{/if}" href="{$ad.sLink}" title="{$ad.sTitle}"><img src="{$ad.sImagePath}" alt="pic"/></a>
							{/foreach}
						</div>
						{/if}
						{if !empty($res.vendorads)}

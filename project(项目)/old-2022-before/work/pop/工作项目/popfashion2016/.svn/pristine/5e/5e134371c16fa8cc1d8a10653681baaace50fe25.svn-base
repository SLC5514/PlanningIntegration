    .section4>.con-rg>a{display:none;}
    .section4>.con-rg>h2.pc-it{display:none;}
    .section4>.con-rg>h2.h5-it{display:block;}
    .section4>.con-lf{width:14.975rem;height:9.5rem;background-size:100%;margin:2rem auto 0;float:none;}
    .section4>.con-lf>h3{height:1.25rem;line-height:1.25rem;margin-left:0.75rem;font-size:0.65rem;}
    .section4>.con-lf>div{height:8.25rem;}

    <div class="bg-layer js-bg-layer"></div>
    <div class="rclick-layer downimg-layer-data js-downimg-right">
        <div class="down-main-top clearfix">
            <h2 class="fl">下载文件格式</h2><div class="fr down-close js-down-close"></div>
        </div>
        <table class="down-table" width="100%" id="J_DownloadType2"></table>
    </div>    

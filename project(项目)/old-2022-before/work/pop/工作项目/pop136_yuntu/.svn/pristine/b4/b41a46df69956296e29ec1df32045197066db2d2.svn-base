.tool-div{margin-top: 10px;margin-left: 34px;}
.tool-div>div{position: relative;width: 2px;height: 120px;margin-top: 4px;margin-bottom: 6px;background-color: #70757a;}
.tool-div>div>button{position: absolute;top: 0px;left: -6px;display:inline-block;*display: inline-block;width: 13px;height: 13px;border:1px solid #999999;background-color: #e5e5e5;cursor: pointer;border-radius: 50%}
.tool-div>button{display:inline-block;*display: inline-block;width: 30px;height: 30px;margin-left: -14px;border-style: none;background-color: transparent;cursor: pointer;}
.tool-div>button>img{width: 16px}
.add-ratio{top: 0px;}

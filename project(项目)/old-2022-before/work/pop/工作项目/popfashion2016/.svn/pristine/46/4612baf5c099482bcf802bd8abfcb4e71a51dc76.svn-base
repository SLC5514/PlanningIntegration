body{background-color: #f9f9f9;font-family: 'Microsoft YaHei','PingFang SC','Lantinghei SC','HanHei SC','Helvetica Neue','Open Sans',Arial,'Hiragino Sans GB','微软雅黑',STHeiti,'WenQuanYi Micro Hei',SimSun,sans-serif;}
html,body{min-width:1200px;}
input::-ms-clear{display:none;}
input::-ms-reveal{display:none;}
input:-webkit-autofill {  
    -webkit-box-shadow: 0 0 0px 1000px white inset !important;  
}
.fl{float:left;}
.fr{float:right;}
.txt-c{text-align: center;}
.redtext{color:#d8b056;font-style:normal;}
.contant{width:100%;}
.con_width{width:1200px;margin:0 auto;}
/* 头部 */
.header{position:relative;width:100%;background:#000;z-index: 22;} 
.head-top{height:40px;background:#1c232b;width:100%;z-index:99;position:fixed;top:0;left:0;min-width:1200px;}
.contentW{width:1200px;margin:0 auto;position: relative;}

.sign-out-div{height: 20px;line-height: 20px;padding-top: 9px;margin-right: 20px;}
div.sign-out-div>a{color: #999;}
.new-header-section{background-color: #fff;}
/* 会员过期提醒start */
.remind-list{width: 100%;height: 30px;line-height: 30px;background:rgba(255,122,69,0.11);text-align: center;color: #666;z-index: 97;display: none;border-radius: 5px;}
.remind-list>.interval{width: 100%;height: 1px;background-color: #fff;}
.remind-list>span{font-size: 12px;display: inline-block;}
.remind-list>span>img{vertical-align: -1px;margin-right: 5px;height: 12px;width: 12px;}
.remind-list>a>img{width: 11px;height: 11px;margin-left: 10px;vertical-align: -1px;}
.remind-list>span>span{color: #FF7A45;font-size: 12px;margin: 0 5px;font-weight: bold;}
.active>span>span{font-style:normal;font-size: 16px;background-color: #ff6633;color: #e8e9e9;padding: 0 3px;}
.active{background-color: #d8b056;}
/* 会员过期提醒end */


@media screen and (min-width:1500px) {
	.contentW{width:1500px;}
}
/*手机端*/
.m_layer{background:url(../images/pbg.jpg) no-repeat;width:7.5rem;height:100%;overflow:hidden;margin:0 auto;position: fixed;left:0;bottom:0;z-index:120;display:none;}
.m_middle{width:6.8rem;height:4rem;background:#fff;position:absolute;top:50%;left:50%;margin-top:-2rem;margin-left:-3.4rem;border-radius:4px;}
.box_top{padding:0.4rem;overflow:hidden;}
.box_top .m_img{float:left;width:1.8rem;height:1.8rem;}

/*闪烁*/
@-webkit-keyframes animate1{
    0%,8%{
        -webkit-transform:scale(1);
    }
    13%,21%{
        -webkit-transform:scale(0.8);
    }
    26%,34%{
        -webkit-transform:scale(0.8);
    }
    39%,47%{
        -webkit-transform:scale(1);
    }

    53%,61%{
        -webkit-transform:scale(0.8);
    }
    66%,74%{
        -webkit-transform:scale(1);
    }
    79%,87%{
        -webkit-transform:scale(0.8);
    }
    92%,100%{
        -webkit-transform:scale(1);
    }
}
@-moz-keyframes animate1{
   0%,8%{
        -moz-transform:scale(1);
    }
    13%,21%{

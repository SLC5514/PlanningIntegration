    .section2>.con-lf>ul.small-ul>li.brand-li>p{width:2.9rem;height:0.75rem;left:2.775rem;top:0.724rem;line-height:0.8rem;}

    .section2>.con-lf>ul.big-ul{margin-top:1.25rem;}

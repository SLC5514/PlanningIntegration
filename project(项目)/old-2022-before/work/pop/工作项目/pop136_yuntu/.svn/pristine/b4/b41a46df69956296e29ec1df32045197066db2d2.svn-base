@charset "utf-8";
body{background-color: #323940}
.view{min-width: 1340px}
.header{min-width: 1340px;border-bottom: 1px solid #494f55;}
.content{padding-top: 30px;}
/*vitual*/
.virtual-box{position: relative;padding-bottom: 30px;}




/*用户站点模板选择*/
.user-model-box{display: none;z-index: 505;position: fixed;left: 50%;top: 50%;padding: 60px 114px;width: 306px;margin-left: -267px;margin-top: -225px;background-color: #fff;text-align: center}
.user-model-box>h3{font-size: 24px;margin-bottom: 20px;color: #333;font-weight: bold;}
.user-model-box>p{font-size: 16px;color: #333;margin-bottom: 50px;}
.user-model-box>button{font-size: 14px;display: block;width: 100%;height: 45px;line-height: 45px;background-color: #d8b056;color: #fff;margin-top: 20px;border-radius: 3px;}
.user-model-box>button:hover{background-color: #dba428;}
.user-model-box>ul>li{display: inline-block;width: 140px;height: 40px;line-height: 40px;margin-bottom: 30px;font-size: 16px;background-color: #fff;color: #333;border: 1px solid #333;border-radius: 3px;cursor: pointer;}
.user-model-box>ul>li:hover{border: 1px solid #d8b056;background-color: #d8b056;color: #fff;}
.user-model-box>ul>li.now-choice{border: 1px solid #d8b056;background-color: #d8b056;color: #fff;}



/*没有设计花型*/
.no-my-pattern{color: #fff;text-align: center;}
.no-my-pattern>h3{color: #fff;margin-top: 32px;margin-bottom: 20px;}
.no-my-pattern>a{display: inline-block;color: #fff;width: 300px;height: 40px;line-height: 40px;color: #fff;background-color: #d8b056;border-radius: 3px;}
.no-my-pattern>a:hover{background-color: #dba428;}

/*相似推荐*/
.recommend-box{;z-index: 100;position: fixed;left: 0;bottom: 0;right: 380px;height: 50px/9;min-width: 990px}
div.full-state{right: 0!important;}
.full-state .recommend-show-btn>.icon{background-position: -325px -78px}
.full-state .function-div{background-color: transparent;}
.function-div{text-align: center;background-color: #42494f;}
.droped-now{border-top: 1px solid #5b6166}
.droped-now .file-btn,.download-pic-btn,.reset-pattern-btn,.recommend-show-btn,.my-pattern{border-top: none}
.file-btn,.download-pic-btn,.reset-pattern-btn,.recommend-show-btn,.my-pattern{display: inline-block;height: 61px;line-height: 61px;padding: 0 20px;color: #fff;background-color: #42494f;font-weight: bold;border-right: 1px solid #5b6166;border-top: 1px solid #5b6166;vertical-align: middle;}
.file-btn{height: 60px;;line-height: 60px;border-left: 1px solid #5b6166;cursor: pointer;}
.file-btn>.icon{width: 20px;height: 25px;margin-right: 4px;background-position: -196px -78px;vertical-align: middle;}
.file-btn:hover,.download-pic-btn:hover,.reset-pattern-btn:hover,.recommend-show-btn:hover,.my-pattern:hover{background-color: #323940}

.my-pattern>.icon{width: 20px;height: 20px;margin-right: 4px;background-position: -153px -316px;vertical-align: middle;}
.recommend-show-btn.active-status{background-color: #323940}
.my-pattern.active-status{background-color: #323940}

.download-pic-btn>.icon{width: 20px;height: 25px;margin-right: 4px;background-position: -228px -78px;vertical-align: middle;}
.reset-pattern-btn>.icon{width: 20px;height: 25px;margin-right: 4px;background-position: -257px -78px;vertical-align: middle;}

.recommend-div{display: none;height: 156px;padding:15px 40px 0;background: #323940;overflow-x:auto;overflow-y:hidden;}
.recommend-show-btn>.icon{width: 20px;height: 25px;margin-right: 4px;background-position: -292px -78px;vertical-align: middle;}

.recommend-contain{height: 100%;margin: 0 auto;position: relative;}
.recommend-left-btn{cursor: pointer; position: absolute;z-index: 6; top: 51px;left: 0;height: 40px; line-height: 40px; width: 40px;background-position: -204px -180px}
.recommend-right-btn{cursor: pointer; position: absolute;z-index: 6; top: 51px;right: 0;height: 40px;line-height: 40px; width: 40px;background-position: -276px -180px}
.recommend-lbtn-on{background-position: -133px -180px;}
.recommend-rbtn-on{background-position: -349px -180px;}

.recommend-disc{font-weight: bold; float: left;margin-left: 62px; text-align: center; background: #eee;width: 140px;height: 110px;padding-top: 30px;font-size: 16px;line-height: 20px;color: #333;border-top: 1px solid #fff;}
.recommend-disc>div{margin: 0 auto 3px auto;width: 64px;height: 60px;background: url(../../images/virtual/recommend-icon1.png) no-repeat center;}

/*图片列表*/
.recommend-pics{height: 143px;margin-left: 80px;margin-right: 80px;overflow: hidden;}    
.recommend-list{height: 100%;overflow: hidden;margin-left: 0;text-align: center;}
.recommend-list>li{float: left;width: 138px;height: 138px;margin-right: 25px;overflow: hidden;padding: 1px;background: #ddd;border: 1px solid #fff;}    
.recommend-list>.recommend-item{border: 1px solid #e4af63;background: #e4af63;}
.recommend-list>li>a{display: block;width: 100%;height: 100%;line-height: 138px; overflow: hidden;background: #eee;}    
.recommend-list>li>a>img{display: block;width: 100%;border: 0 none;}    


/*右侧工具*/
.control-div{z-index: 101;position: absolute;right: 0;top: 0;bottom: 0;width: 380px;background-color: #1c232b;color: #fff;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}
.control-div>.control-containt{padding: 30px 60px 20px;background-color:#1c232b;}
.control-div>.control-containt>h3{font-size: 16px;padding-bottom: 20px;margin-bottom: 20px;border-bottom: 1px solid #13181e;color: #fff;text-align: center;box-shadow: 0px 1px 0px #303c4a}
.control-div>.control-containt>label{display: inline-block;margin-bottom: 20px;}

.tab-pattern-nav{margin-bottom: 20px;text-align: center;}
.tab-pattern-nav>li{width: 128px;height: 40px;line-height: 40px;font-size: 14px;display: inline-block;border:1px solid #fff;background-color: #fff;color: #333;cursor: pointer;}
.tab-pattern-nav>li.now-choice{background-color: #d8b158;color: #fff;border:1px solid #d8b158;}

.detail-btn{position: relative;left: 10px;width: 44px;height: 75px;color: #fff;background-image: url(../../images/virtual/icon5.png);background-repeat: no-repeat;background-position: center 10px;background-size: 23px;background-color: #1c232b;border:1px solid #424549;border-radius: 5px}
.detail-btn:hover{background-image: url(../../images/virtual/icon5-active.png);background-color: #d8b158;color: #fff;border:1px solid #d8b158;}
.no-detail{background-image: url(../../images/virtual/icon5-no.png);color: #666;}
.no-detail:hover{background-image: url(../../images/virtual/icon5-no.png);background-color: #1c232b;color: #666;border:1px solid #424549;}
.tool-box .detail-btn>span{display: inline-block;line-height: 15px;margin-top: 36px;margin-left: 2px}

.cursor-sty{cursor: url(../../images/virtual/icon6.png?version=2017/11/27),auto !important;}
.get-img-color{display: inline-block;width: 30px;height: 30px;margin-left: 182px;background-image: url(../../images/virtual/icon7.png);background-repeat: no-repeat;background-position: center;vertical-align: middle;}

/*调色板*/
.color-container{display: inline-block;width: 100%;margin-bottom: 20px;position: relative;}
.series-canvas{width: 100%;height: 100%;}
.detail-canvas{width: 100%;}

.color-series-div{position: relative;display: inline-block;width: 100%;height: 40px;}
.color-series-div>span{pointer-events:none;position: absolute;z-index: 10;left: -5px;top: 0;width: 11px;height: 100%;}
.color-series-div>span:before{z-index: 10;position: absolute;content: "";left: 0;top: -5px;border: 5px solid transparent;border-top-color: #fff;}
.color-series-div>span:after{z-index: 10;position: absolute;content: "";left: 0;bottom: -5px;border: 5px solid transparent;border-bottom-color: #fff;}

.check-detail-color{width: 260px;margin-top: 15px;padding-bottom: 10px;background-color: #fff;text-align: center;}
.check-detail-color>button{border:1px solid #ccc;border-radius: 5px;width: 80px;height: 30px;margin-top: 15px;}
.detail-color-div{position: relative;overflow: hidden;}
.detail-color-div>span{pointer-events:none;position: absolute;z-index: 10;right: -5px;top: -5px;display: inline-block;width: 10px;height: 10px;border:1px solid #111;border-radius: 50%;box-shadow: 0px 0px 2px 1px #fff}
.color-val-div{text-align: center;}
.color-val-div>label{display: inline-block;text-align: right;margin-right: 5px;color: #333}
.color-val-div>input{display: inline-block;width: 80px;padding: 0.4em 0.5em;margin-top: 10px}
.color-val-div>span{display: inline-block;width: 50px;height: 50px;vertical-align: middle;}
.hr-div{margin-top: 10px;padding-top: 30px;border-top: 1px solid #303c4a;box-shadow: 0px -1px 0px #13181e}
.return-back-btn{display: block;width: 100%;height: 40px;line-height: 40px;font-size: 14px;background-color: #787c81;color: #fff;border-radius: 5px}
.return-back-btn:hover{background-color: #d8b056;}


/*左侧模型*/
.pattern-list-div{z-index: 10;position: fixed;top: 60px;left: 0;bottom: 0;}
.description-btn{z-index: 100;position: fixed;left: 5px;bottom: 242px;display: inline-block;width: 100px;height: 40px;line-height: 40px;font-size: 14px;text-align: center;color: #fff;background-color: #d8b056;border-radius: 3px;}
.description-btn:hover{background-color: #dba428;}

.pattern-tab-list{z-index: 2;position: absolute;left: 0;top: 0;bottom: 0;padding-top: 30px;background-color: #1c232b}
.pattern-tab-list>li{width: 110px;height: 50px;line-height: 50px;font-size: 14px;color: #666;font-weight: 600;text-align: center;cursor: pointer;}
.pattern-tab-list>li.now-choice{color: #d8b056;background-color: #42494f}
/*.pattern-tab-list>li:hover{color: #d8b056;background-color: #42494f}*/
.pattern-tab-list>li>img{width: 18px;margin-right: 5px;vertical-align: middle;}
.xiangbao>li>span{display: inline-block;width: 4.5em;text-align: left;}
.xiezi>li>span{display: inline-block;width: 3.5em;text-align: left;}
.jiafang>li>span{display: inline-block;width: 4em;text-align: left;}

.item-box{padding-top: 30px;height: 100%;padding-bottom: 20px;width: 328px;margin-left: 110px;background-color: #42494f;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}


.pattern-list-content{position: relative;height: 100%;overflow: hidden;}
.pattern-list{margin-left: 25px;display: none;}
.pattern-list>li{position: relative;float: left;width: 125px;height: 165px;margin-right: 20px;margin-bottom: 20px;cursor: pointer;overflow: hidden;background-color: #fff;text-align: center;}
.pattern-list>li>img{display: inline-block;*display: inline-block;width: 100%;line-height: 165px;text-align: center;
    -webkit-transition: all 0.3s;
    -moz-transition: all 0.3s;
    -o-transition: all 0.3s;
    -ms-transition: all 0.3s;
    transition: all 0.3s;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
}
.pattern-list>li>span{z-index: 20;position: absolute;right: 5%;top: 25%;display: block;*display: block;width: 90%;height: 36px;line-height: 36px;margin-top: -18px;color: #fff;background-color: #d8b056;border-radius: 18px;text-align: center;font-size: 14px;
    opacity: 0;
    filter:alpha(opacity=0);
    -webkit-transition: all 0.3s;
    -moz-transition: all 0.3s;
    -o-transition: all 0.3s;
    -ms-transition: all 0.3s;
    transition: all 0.3s;
}
.pattern-list>li:hover>span{
    opacity: 1;
    filter:alpha(opacity=100);
    top: 50%;
}
.pattern-list>li:hover>img{
    -webkit-transform: translate(-50%, -50%) scale(1.02);
    -moz-transform: translate(-50%, -50%) scale(1.02);
    -o-transform: translate(-50%, -50%) scale(1.02);
    -ms-transform: translate(-50%, -50%) scale(1.02);
    transform: translate(-50%, -50%) scale(1.02);
}
.pattern-list .border-div{z-index: 10;position: absolute;left: 0px;top: 0px;right: 0;bottom: 0;border:1px solid #eeeeee;border-radius: 2px;}
.pattern-list .pattern-bg{z-index: 5;position: absolute;left: 0;top: 0;width: 100%;height: 100%;background-color: #000;opacity: 0;filter:alpha(opacity=0);
    -webkit-transition: all 0.4s;
    -moz-transition: all 0.4s;
    -o-transition: all 0.4s;
    -ms-transition: all 0.4s;
    transition: all 0.4s;
}
.pattern-list>li:hover .pattern-bg{
    opacity: 0.7;
    filter:alpha(opacity=70);
}
.pattern-list>li:hover .border-div{border:3px solid #e4af63;}
.pattern-list>li.now-choice .border-div{border:3px solid #e4af63;}

.ps-container .ps-scrollbar-y{z-index: 50}



/*旋转工具*/
.rotate-section{position: relative;width: 215px;margin: auto;color: #fff;font-weight: bold;text-align: center;}
.rotate-section>ul{z-index: 10;width: 215px;height: 215px;position: relative;border-radius: 50%;transform:rotateZ(180deg);transition:transform 0.3s;}
.rotate-section>ul>li{z-index: 10;position: absolute;height: 1em;line-height: 1em}
li.rotate-pos1{left: 50%;top: 0px;margin-left: -3px;transform:rotateZ(0deg);}
li.rotate-pos2{right: 28px;top: 28px;transform:rotateZ(45deg);}
li.rotate-pos3{right: -4px;top: 50%;margin-top: -11px;transform:rotateZ(90deg);}
li.rotate-pos4{right: 28px;bottom: 28px;transform:rotateZ(135deg);}
li.rotate-pos5{left: 50%;bottom: 0px;margin-left: -11px;transform:rotateZ(180deg);}
li.rotate-pos6{left: 28px;bottom: 28px;transform:rotateZ(225deg);}
li.rotate-pos7{left: -1px;top: 50%;margin-top: -7px;transform:rotateZ(270deg);}
li.rotate-pos8{left: 28px;top: 28px;transform:rotateZ(315deg);}
li.rotate-pos1:before,li.rotate-pos3:before,li.rotate-pos5:before,li.rotate-pos7:before{content: "";display: block;z-index: 10;position: absolute;width: 2px;height: 10px;top: 20px;left: 50%;margin-left: -1px;background-color: #274496}

.rotate-div{z-index: 50;position: absolute;left: 0;top: 50%;width: 100%;margin-top: -21px;}
.rotate-div>h3{margin-bottom: 6px;font-size: 12px;color: #fff;}
.rotate-div>input{width: 46px;height: 20px;line-height: 20px;font-size: 14px;background-color: #181d24;color: #fff;font-weight: bold;border-style: none;text-align: center;}


.rotate-point-plate{z-index: 20;position: absolute;left: 50%;top: 50%;width: 156px;height: 156px;margin-left: -78px;margin-top: -78px;background-image: url(../../images/virtual/icon12.png);background-repeat: no-repeat;background-position: center 95%;background-color: #1c232b;border-radius: 50%;}
.rotate-bg-plate{z-index: 5;position: absolute;left: 50%;top: 50%;width: 176px;height: 176px;margin-left: -88px;margin-top: -88px;background-color: #181e25;border-radius: 50%;}

/*试衣模型*/
.pattern-box{
    width: 500px;
    padding-top: 30px;
    margin: 30px auto 0;text-align: center;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}
.pattern-area{
    width: 430px;
    display: inline-block;
    background-color: #fff;
    overflow: hidden;vertical-align: top;
    /*margin-left: 20px;*/
    /*height: 770px;*/
}
.main-canvas{width: 430px;height: 770px;display: block;}
.tool-box{display: inline-block;margin-left: 0px}

.share-box>.ctn .select{text-align: left;display: inline-block;width: 279px; height: 40px;line-height: 40px; padding-left: 20px; font-size: 14px;color: #999; background: #fff; border-radius:5px; border:1px solid #d8d8d8; box-sizing: border-box;cursor: pointer;position: relative;}
.share-box>.ctn .select:after{content: '';position: absolute;right: 9px;top: 16px;width: 13px;height: 8px;background: url(../../images/user/icon_down.png) no-repeat center / cover;}

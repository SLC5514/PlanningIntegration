{
  "name": "comprehensive",
  "version": "1.0.0",
  "description": "综合项目",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.1.6",
    "babel-plugin-import": "^1.13.3",
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0",
    "egg-view-nunjucks": "^2.3.0",
    "vue": "^3.0.5",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "less": "^4.1.1",
    "typescript": "^4.1.3",
    "vite": "^2.3.5",
    "vite-plugin-style-import": "^0.10.1",
    "vue-tsc": "^0.0.24"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-comprehensive",
    "stop": "egg-scripts stop --title=egg-server-comprehensive",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "web-dev": "cd web && vite",
    "web-build": "cd web && vue-tsc --noEmit && vite build",
    "web-serve": "cd web && vite preview"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },

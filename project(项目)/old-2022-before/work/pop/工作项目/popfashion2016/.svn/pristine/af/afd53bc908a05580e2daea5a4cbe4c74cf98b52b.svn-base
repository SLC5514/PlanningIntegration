*{padding: 0;margin: 0;}
img{border:none;}
.content{padding: 40px 0;background-color: #f9f9f9}
.txt-div{margin-bottom: 40px;}
.txt-div>h3{font-size: 24px;margin-bottom: 15px;color: #333;}
.txt-div>h3>img{width: 40px;margin-right: 10px;vertical-align: -12px;}
.txt-div>p{font-size: 14px;line-height: 1.8em}
.download-div{padding: 90px 0;background-color: #fefefe;text-align: center;}
.download-div>a{display: inline-block;*display: inline-block;margin-right: 80px;color: #333}
.download-div>a>img{width: 88px;margin-bottom: 20px;}
.nclear{*zoom:1;}
.nclear:after{content: "";display: block;clear: both;}
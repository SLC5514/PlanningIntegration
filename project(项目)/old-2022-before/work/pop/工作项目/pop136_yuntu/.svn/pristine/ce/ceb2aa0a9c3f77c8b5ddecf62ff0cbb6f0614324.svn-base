/*修改账户信息*/
.setuser-list{margin-top: 80px;position: relative;margin-left: 215px;}
.setuser-list>li{margin-bottom: 20px;}
.setuser-list>li>.title{font-size:14px;color: #646464;font-weight:bold;display: inline-block;height: 40px;line-height: 40px;border-bottom: 2px solid transparent;cursor: pointer;}
.setuser-list>li>.ctn{display: none;position: absolute;top: 0;left: 285px;}
.setuser-list>li.on>.title{color: #2B2B2B;border-bottom-color: #1D232C;}
.setuser-list>li.on>.ctn{display: block;}
.setuser-list>li>.ctn>.form-list{width: 500px;}
.setuser-list>li>.ctn>.form-list>li{margin-bottom: 28px;height: 50px;line-height: 50px;position: relative;white-space: nowrap;font-size: 0;border-bottom: 1px solid #333;box-sizing: border-box;}
.setuser-list>li>.ctn>.form-list>li>label{float: left;width: 125px;font-size:14px;color: #333;}
.setuser-list>li>.ctn>.form-list>li>input{float: left;width: 375px;height: 100%;font-size:14px;color: #333;padding: 0 10px;box-sizing: border-box;border: none;}
.setuser-list>li>.ctn>.form-list>li>.err{display: none;font-size:12px;color: #C82F2F;position: absolute;bottom: -21px;line-height: 17px;}
.setuser-list>li>.ctn>.form-list>li>.btn{width:500px;height:40px;line-height:40px;background:#1d232c;border-radius:5px;opacity:0.5;font-size:14px;color: #fff;cursor: pointer;margin-top: 25px;}
.setuser-list>li>.ctn>.form-list>li>.btn:hover,
.setuser-list>li>.ctn>.form-list>li>.btn.on{opacity: 1;}
.setuser-list>li>.ctn>.form-list>li:last-child{border-bottom: none;}

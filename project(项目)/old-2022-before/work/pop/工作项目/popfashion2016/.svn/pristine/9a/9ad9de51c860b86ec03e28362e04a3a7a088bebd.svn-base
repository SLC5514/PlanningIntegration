{extends file="layout.html"}

{block name="selfcss"}
<link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/global.css?{STATIC_CHANGE_TIME}">
<link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/perfect-scrollbar.css?{STATIC_CHANGE_TIME}">
<link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/mid-tourist.css?{STATIC_CHANGE_TIME}">
{/block}

{block name="main"}
<div class="tour_banner con_width">
	{foreach $aTopAds as $_ad}
    <a href="{$_ad.sLink}" {if $_ad.sNewWindow eq 1}target="_blank"{/if} title="{$_ad.sTitle}"><img src="{$_ad.sImagePath}" alt="pic"/></a>
	{/foreach}
    <!-- <a href="/member/register/?spid=2016080801" target="_blank"><img src="{STATIC_URL1}/global/images/banner/1500-100-2.gif?20160808"/></a> -->
</div>
<div class="main tour_login con_width {if $P_UserType==4}already-login{/if} {if $partVip}part-vip{/if}">
	<div class="clearfix con-box {if $is_runways}is-runways{/if} {if $column == 116}is-insprition{/if}">
		{if $aBroadsideAd}
		<div class="trial-r fr">
            <div class="trial-rm clearfix">
                <div itemscope class="cont-l-pic fl lazyload">
                    {if $is_runways}
                        {$cover}
                    {else}
                        {if $column == 116}
                        <img alt="{$tit}" itemprop="thumbnail" style="width:208px;height:155px" src="{STATIC_URL1}/global/images/loading/inspiration.gif" data-original="{$cover}" />
                        {else}
                        <img alt="{$tit}" itemprop="thumbnail" style="width:165px;height:220px" src="{STATIC_URL1}/global/images/loading/reference165-220.gif" data-original="{$cover}"/>
                        {/if}
                    {/if}
                </div>
                <div class="cont-r-msg fl">
                    <h3>{if isset($tit)}{$tit}{/if}</h3>
                    <p class="js-cont-p">{$descriptionHtml}</p>
                    <div class="label-msg-col">
                        <label class="fl">标签：</label>
                        {if $labelsOrigin == 'style'}
                        <p class="fl">
                            {foreach $labels as $label}
                            <a href="{$label.dLink}" target="_blank" title="{$label.name}">{$label.name}</a>
                            {/foreach}
                        </p>
                        {else}
                        <p class="fl clearfix">
                            {if $middlePageTags.brandName}
                            {if $middlePageTags.brandLink && $middlePageTags.brandLink neq 'javascript:void(0);'}
                            <a target="_blank" href="{$middlePageTags.brandLink}" title="{$middlePageTags.brandName}">{$middlePageTags.brandName}</a>
                            {else}
                            {if $column != 10}
                            {$middlePageTags.brandName}
                            {else}
                            <a class="video-blank" href="javascript:void(0);" title="{$middlePageTags.brandName}">{$middlePageTags.brandName}</a>
                            {/if}
                            {/if}
                            {/if}
                            <a target="_blank" href="{$middlePageTags.colLink}" title="{$middlePageTags.colName}">{$middlePageTags.colName}</a>
                            {if $middlePageTags.regName}
                            <a target="_blank" href="{$middlePageTags.regLink}" title="{$middlePageTags.regName}">{$middlePageTags.regName}</a>
                            {/if}
                            {if $middlePageTags.seaName}
                            <a target="_blank" href="{$middlePageTags.seaLink}" title="{$middlePageTags.seaName}">{$middlePageTags.seaName}</a>
                            {/if}
                            {if $middlePageTags.genInfo}
                            {foreach $middlePageTags.genInfo as $val}
                            {if $column != 10}
                            <a target="_blank" href="{$val.link}" title="{$val.sName}">{$val.sName}</a>
                            {else}
                            <a class="video-blank" href="javascript:void(0);" title="{$val.sName}">{$val.sName}</a>
                            {/if}
                            {/foreach}
                            {/if}
                            {if $middlePageTags.indInfo}
                            {foreach $middlePageTags.indInfo as $val}
                            <a target="_blank" href="{$val.link}" title="{$val.sName}">{$val.sName}</a>
                            {/foreach}
                            {/if}
                            {if $middlePageTags.catInfo}
                            {foreach $middlePageTags.catInfo as $val}
                            <a target="_blank" href="{$val.link}" title="{$val.sName}">{$val.sName}</a>
                            {/foreach}
                            {/if}
                            {if $middlePageTags.scatInfo}
                            {foreach $middlePageTags.scatInfo as $val}
                            <a target="_blank" href="{$val.link}" title="{$val.sName}">{$val.sName}</a>
                            {/foreach}
                            {/if}
                            {if $middlePageTags.fairsName}
                            <a target="_blank" href="{$middlePageTags.fairsLink}" title="{$val.sName}">{$middlePageTags.fairsName}</a>
                            {/if}
                            {if $middlePageTags.marketName}
                            <a target="_blank" href="{$middlePageTags.marketLink}" title="{$val.sName}">{$middlePageTags.marketName}</a>
                            {/if}
                        </p>
                        {/if}
                    </div>
                </div>
            </div>
		</div>
		{/if}
		<div class="trial-l fl">
            {if $is_runways}
            <h2 class="part-title1">开通VIP，随时掌握最新T台资讯</h2>
            {else}
            <h2 class="part-title1">图册列表需要VIP权限<br>开通VIP可浏览全站趋势图片及行业深度报告书籍</h2>
            {/if}
			<h2 class="part-title2">诶，您购买的套餐权限不支持浏览本内容</h2>
			<p class="trial-type1">您现在是游客状态，拨打<span>4008-210-500</span>联系客服开通试用账号体验全站</p>
			<p class="trial-type2">您现在是普通用户，拨打<span>4008-210-500</span>联系客服开通试用账号体验全站</p>
			<p class="trial-type3">需要<span class="trial-type3-msg">{$sGender}{if $iIndustry}/{/if}{$iIndustry}&nbsp;&nbsp;{$columnP}{if $columnC}/{/if}{$columnC}</span>的权限，拨打<span>4008-210-500</span>联系客服开通试用账号</p>
			<a class="set-vip-link part-vip-link js-contact-qq-btn" href="javascript:void(0);" data-type="after" title="联系客服"><i></i>联系客服  快速开通试用账号</a>
			<a class="set-vip-link js-contact-qq-btn" href="javascript:void(0);" title="联系客服"><i></i>联系客服  快速开通VIP试用账号</a>
			<a class="set-trial-link js-contact-qq-btn" href="javascript:void(0);" style="display:none;" title="在线申请试用">在线申请试用</a>
			<div class="is-login">
                {if $is_runways}
                <p style="color: #f4523b;font-weight: bold;">注册/登录后即可预览本场T台图册</p>
                {else}
                <p>注册/登录后可<span>清晰浏览</span>款式列表，及部分<span>免费</span>精选报告！</p>
                {/if}
            	<a class="redRg btn-l" href="/member/register/" rel="nofollow" target="_blank" title="注册">注册</a>
            	<a class="loginLayer btn-r" href="javascript:void(0);" rel="nofollow" title="登录">登录</a>
        	</div>
		</div>
		
	</div>
	
	<p class="cont_b_rp">
        <img src="{STATIC_URL2}/global/images/login_txt.png" alt="pic"/>
    </p>
</div>
{/block}

{block name="self"}
<script type="text/javascript" src="{STATIC_URL2}/global/js/fashion/work.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL2}/global/js/common/Lazyload.js?{STATIC_CHANGE_TIME}"></script>
{literal}
<script type="text/javascript">
$(function(){
    $(".lazyload img").lazyload({effect : "fadeIn"});
    
    if($('.js-cont-p').length>0 && $('.is-runways').length<=0){
    	var cont_msg = $('.js-cont-p').text();
    	var new_cont_msg=general.fn.cutByWidth(cont_msg,1430,14)||'';
	    if(new_cont_msg!=cont_msg){
	    	$('.js-cont-p').text(new_cont_msg);
	    }
    }


    var P_UserType_action = '4';
    if(P_UserType=='1'||P_UserType=='2'){
        P_UserType_action = '1';
    }else if(P_UserType=='3'){
        P_UserType_action = '2';
    }else if(P_UserType=='4'){
        P_UserType_action = '3';
    }else if(P_UserType=='5'){
        P_UserType_action = '4';
    }

    var statistics_action_token = '';
    if( typeof statistics_token == 'undefined'){
        statistics_action_token = '';
    }else{
        statistics_action_token = statistics_token;
    }
    var action = {		//统计数据
        token: statistics_action_token,
		iUserType: P_UserType_action,		//用户类型
        	
		iUserId: '',  			//用户ID
		sChildUserId: '', 		//子账号id
		sTableName: '', 		//假表名
		iPrid: '',				//当前详情ID
		
		iColumnId: '',			//主栏目ID
		iSubColumnId: '',  		//子栏目ID
		sSelfUrl: location.href, 			//当前URL
		sRefererUrl: location.href,		//上个页面URL
		iSite: 1, 				//站点
		sLang: 'cn',			//语言
		
		iVisit: 0    		//是否能访问 0-无访问权限 1-有访问权限
    }
    
    setAction();
    function setAction(){  // 配置统计数据

        var _href = location.href;
        

        var _id = parseFloat( _href.split('id_')[1]||'' );
        var _col = parseFloat( _href.split('col_')[1]||'' );
        var _t = ( ( _href.split('t_')[1]||'' ).split('-')[0]||'' ).split('/')[0]||'';
        

        action.sRefererUrl = document.referrer||'';

        action.iUserId = P_AccountId;	//主账号
        if(P_UserType=='2'){
            action.sChildUserId = P_UserId;	//子账号  
        }
        
        if( _href.indexOf('/books/innermaga') != -1 ){  // 流行画册
            var id_arr = ( _href.split('id_')[1]||'' ).split('I')||[];
            var this_id = id_arr[id_arr.length-1];
            _id = parseFloat( this_id );
            _t = 'imgcol_set';
            _col = '130';
            action.iColumnId = '6';
        }

        var param_el = $('#param');
        action.sTableName = _t+'';
		action.iPrid = _id+'';
        action.iSubColumnId = _col+'';
        
    }

    // 浏览量统计
    actionView();
    function actionView(){
        var _href = location.href;
        if( _href.indexOf('/books/') != -1 ){  // 书籍栏目
            $.ajax({
                "url": "//api.pop136.com/internal/statistics.php?action=view&" + Math.random(),
                "data": action,
                "type": 'get',
                'dataType': "jsonp",
                'jsonp': "callback"
            });
        }
    }

    // 下载统计
    var action_down = true, action_timer = null;
    function actionDown(){
        var _href = location.href;
        if( action_down && _href.indexOf('/books/') != -1 ){     // 在能够下载统计时， 并且书籍的
            action_down = false;
            $.ajax({
                "url": "//api.pop136.com/internal/statistics.php?action=down&" + Math.random(),
                "data": action,
                "type": 'get',
                'dataType': "jsonp",
                'jsonp': "callback"
            });
            
            clearTimeout(action_timer);
            action_timer = setTimeout(function(){
                action_down = true;
            }, 3000);
        }
    }

    // 下载统计
    $('a.download').on('click', function(){
        if( oCommon.downloadPrivilege() ){
            actionDown();
        }
        // return false;
    })



    
});
</script>
{/literal}
{/block}

<script type="text/javascript">
	var page_img_path_str="{STATIC_URL2}";
	var page_js_path_str="{STATIC_URL3}";

	$(function(){
		setTimeout(function(){
			scrollAni();
    	},800);
    	
    	function scrollAni(){
    		var _h = document.documentElement.clientHeight || document.body.clientHeight;
			var w_t = $(window).scrollTop();
			//var ani_top1 = $('.js-ani1').offset().top;
			var ani_top2 = $('.js-ani2').offset().top;
			var ani_top3 = $('.js-ani3').offset().top;
			var ani_top4 = $('.js-ani4').offset().top;
			var ani_top5 = $('.js-ani5').offset().top;
			var ani_top6 = $('.js-ani6').offset().top;
			var ani_top7 = $('.js-ani7').offset().top;
	    	
			clearTimeout(scroll_timer);
			var scroll_timer;
			scroll_timer = setTimeout(function(){
				if( w_t < 500 ){
					$('.js-ani1').addClass('ani1');
				}

				if( (ani_top2 - w_t) < (_h - 300) && (ani_top2 - w_t)>-100 ){
					$('.js-ani2').addClass('ani2');
				}

				if( (ani_top3 - w_t) < (_h - 300) && (ani_top3 - w_t)>-200 ){
					$('.js-ani3').addClass('ani3');
				}

				if( (ani_top4 - w_t) < (_h - 300) && (ani_top4 - w_t)>-200 ){
					$('.js-ani4').addClass('ani4');
				}

				if( (ani_top5 - w_t) < (_h - 300) && (ani_top5 - w_t)>-200 ){
					$('.js-ani5').addClass('ani5');
				}


				if( (ani_top6 - w_t) < (_h - 300) && (ani_top6 - w_t)>-200 ){
					$('.js-ani6').addClass('ani6');
				}

				if( (ani_top7 - w_t) < (_h - 300) && (ani_top7 - w_t)>-200 ){
					$('.js-ani7').addClass('ani7');
				}
				
			},240);
			
    	}    	
    	
    	//页面滚动
    	$(window).on('scroll',function(){
    		scrollAni();
    	});

    	// tab切换
    	$(".index-box").eq(0).show();
    	$(".js-ul-name").on('mouseenter','li',function(){
    		var index = $(this).index();
    		$(this).addClass('cur').siblings().removeClass('cur');
    		$(".index-box").eq(index).show().siblings('.index-box').hide();
    	});

    });
</script>

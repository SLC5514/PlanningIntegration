@charset "utf-8";
body{background-color: #fff;}
.main-section{font-family: "微软雅黑"}
.main-section>img{width: 100%;display: block;}
.page-section{padding: 50px 0px;overflow: hidden;}
.content-section{width: 1000px;margin: auto;}
.page-section .title-img{display: block;margin: auto;}
.page-section .left-div{float: left;}

.page-section .right-div{float: right;}
.page-section .show-div{position: relative;text-align: right;}
.page-section .button-div{z-index: 10;position: absolute;}
.page-section .hover-button{display: inline-block;font-size: 20px;font-style: italic;width: 180px;height: 60px;line-height: 60px;border-radius: 30px;font-weight: bold;text-align: center;color: #fff;vertical-align: middle;
	transform:translateX(150px);
	opacity: 0;
	transition: all 0.1s;
	display: none\9;
}
.page-section .default-button{display: inline-block;width: 60px;height: 60px;line-height: 60px;border-radius: 30px;font-weight: bold;text-align: center;color: #fff;background-color: #000;vertical-align: middle;
	transition: all 0.1s;
}
.page-section .hover-button>img{display: inline-block;margin-left: 15px;}
.page-section .default-button>img{display: inline-block;vertical-align: -2px;}

.page-section .show-div:hover .hover-button{opacity: 1;transform:translateX(180px);-webkit-transform:translateX(180px);  display: inline-block\9;}
.page-section .show-div:hover .default-button{opacity: 0;transform:translateX(160px); -webkit-transform:translateX(160px);  display: none\9;}



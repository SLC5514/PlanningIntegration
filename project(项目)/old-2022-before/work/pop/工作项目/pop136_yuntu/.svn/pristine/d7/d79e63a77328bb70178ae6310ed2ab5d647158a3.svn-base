@charset "utf-8";


body{background: #323940;}


.list-section{padding-top: 30px;}

.design-title{font-size: 24px;color: #fff;line-height: 30px;text-align: center;padding-top: 38px;}
.design-title>span{display: inline-block;width: 80px;height: 1px;vertical-align: middle;background: rgba(255, 255, 255, .3);margin: 0 18px;}

/* 2019/8/1之前下载提示 */
.overdue-warming{text-align: center;font-size: 14px;color: #d8b056;margin:10px 0 0;}

/*图案列表*/
.design-list{overflow: hidden;padding: 40px 0 1px;min-height: 500px;width: 1200px;margin: 0 auto;}
.design-list>h3>button{float: right;display: inline-block;font-size: 16px;height: 34px;line-height: 34px;padding: 0px 15px;background-color: #787c81;color: #fff;border-radius: 3px;}
.design-list .confirm-deletion-btn{display: none;background-color: #f4523b;}
.design-list .cancel-deletion-btn{display: none;margin-right: 10px;}
.design-list .cancel-deletion-btn:hover{background-color: #d8b056;}
.design-list>ul{width: 1260px;}
.design-list>ul>li{position: relative; width: 200px;height: 300px;overflow: hidden; float: left;margin: 0 50px 60px 0px;}
.design-list>ul>li>a{display: block; background: #fff; overflow: hidden; width: 200px;height: 300px;line-height: 300px; text-align: center;position: relative;color: #333;font-size: 0;}
.design-list>ul>li>a>img{vertical-align: middle;max-width: 200px;margin: auto;}
.design-list>ul>li>a>.dis-posion{display: none;opacity: 0.5; position: absolute;font-size: 12px;color: #fff;text-align: center; z-index: 2;width: 190px;bottom: 0px;background: #d8b056;padding: 5px 5px 0; overflow: hidden;}
.design-list .dis-posion>.list-words>p:hover{text-decoration:underline;}
.design-list .dis-posion>p{height: 22px;line-height: 22px;padding-bottom: 9px;}
.design-list .dis-posion>p>span{display: inline-block;width: 15px;height: 16px;margin-right: 2px;vertical-align: middle;background-position: -329px -129px;}

.design-list .checked-bg{display: none;z-index: 10;position: absolute;left: 0px;top:0px;width: 100%;height: 100%;background-color: rgba(238,238,238,0.85);background-image: url(../../images/design/icon1.png);background-repeat: no-repeat;background-position: center;}
.design-list>ul>li.now-choice .checked-bg{display: block;}

.go-back{margin-bottom: 20px;width: 80px;background-color: #787c81;border-radius: 3px;font-size: 16px;height: 34px;line-height: 34px;text-align: center;color: #fff;cursor: pointer;display: inline-block;}
.go-back:hover{background-color: #d8b056;}
.go-back>i{display: inline-block;width: 9px;height: 16px;background: url(../../images/common/design1.png) center no-repeat;vertical-align: middle;margin-right: 7px;}







@media only screen and (min-width: 1500px){      
    .design-list{width: 1500px;}
	.design-list>ul{width: 1570px;}
	.design-list>ul>li{margin: 0 60px 60px 0px;}
}

.laypage-section{display: none;}
/* 翻页页码 */
.laypage-section .layui-box{ height: 38px; text-align: center; margin-bottom: 25px;line-height:38px;}
.laypage-section .layui-box span,.layui-box a,.layui-box button{ min-width: 18px;border: 1px solid #ccc; padding: 0 9px; height: 36px;line-height: 36px; text-align: center; color: #a9a9a9; font-size: 12px;display: inline-block; background-color: #fff;vertical-align: top; cursor: pointer;  margin-right:4px;}
.laypage-section .layui-box a:hover{background-color: #d8b056;border: 1px solid #d8b056; color:#fff;}
.laypage-section .layui-box span.layui-laypage-curr{background-color: #d8b056;border: 1px solid #d8b056;color:#fff; cursor: auto;}
.laypage-section .layui-box .layui-laypage-prev{background: url(../../images/common/icon.png) no-repeat -190px -232px #fff;}
.laypage-section .layui-box .layui-laypage-next{background: url(../../images/common/icon.png) no-repeat -147px -232px #fff;}
.laypage-section .layui-box input{ background-color: #fff; border: 1px solid #ccc;  color:#a9a9a9;font-size: 12px;width: 56px; height: 36px; line-height: 36px;text-align: center; vertical-align: top;  margin-right: 5px; padding: 0 2px;}
.laypage-section .layui-box .ellipsis{ background-color: transparent;}
.laypage-section .layui-box .layui-laypage-btn{height: 38px;line-height: 38px;background-color: #d8b056;border: 1px solid #d8b056; color: #fff; cursor: pointer; padding: 0 12px;}








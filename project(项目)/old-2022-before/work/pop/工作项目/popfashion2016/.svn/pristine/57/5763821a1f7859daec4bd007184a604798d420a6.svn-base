.patter-contain>.list-prev>i{display: inline-block;*display: inline;*zoom: 1;width: 16px;height: 32px;background: url(../../images/new-detail/patter-list-prev.png) no-repeat center;}
.patter-contain>.list-next>i{display: inline-block;*display: inline;*zoom: 1;width: 16px;height: 32px;background: url(../../images/new-detail/patter-list-next.png) no-repeat center;}
.patter-contain>.list-cont{*position: relative;width: 100%; overflow: hidden;}
.patter-contain>.list-cont>ul{width: 100000px;}
.patter-contain>.list-cont>ul>li{position: relative; cursor: pointer; background: #fff; float: left;width: 68px;font-size: 0; height: 91px;line-height: 91px; overflow: hidden;margin-right: 4px;border: 2px solid #f5f5f5;}
.patter-contain>.list-cont>ul>li.list-sec,.patter-contain>.list-cont>ul>li.list-sec:hover{border: 2px solid #d8b056;;}
.patter-contain>.list-cont>ul>li:hover{border: 2px solid #f4cb69;}
.patter-contain>.list-cont>ul>li>img{vertical-align: middle;width: 100%;}
.patter-contain>.list-cont>ul>li>.fast-v{position: absolute;z-index: 3; width: 68px;height: 68px;left: 0;top: 0; background: url(../../images/new-detail/detail-bg70.png) repeat 0 0;}
/*图案列表*/
.patter-contain>.list-cont>ul.js-patter-list{padding-top:12px;}
.patter-contain>.list-cont>ul.js-patter-list li{height:68px;line-height:68px;}
.patter-contain>.list-cont>ul.js-patter-list li.list-sec{border: 2px solid #d8b056;;}

.patter-contain>.list-hv-on:hover{background: #e5e5e5;}

.patter-contain>.kuan-num{display: none; cursor: pointer;background: #f9f9f9; position:absolute;right: 46px;top: 10px;font-size: 14px;color: #333;line-height: 20px; text-align: center; height: 75px;width: 40px;padding-top: 16px;}
.patter-contain>.kuan-num>p{color: #d8b056;; overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}

.img-detail .imgs-num{ border-radius: 2px; display: none;margin-left: 19px; background: #d8b056;; height: 34px;width: 262px;line-height: 34px;position: relative;color: #fff;font-size: 14px;text-indent: -20px;text-align: center;}
.img-detail .imgs-num:hover{background: #f4cb69;}
.img-detail .imgs-num>i{display: block;position: absolute;right: 46px;top: 7px;height: 20px;width: 20px;background: url(../../images/new-detail/icon1.png) no-repeat center;}

.list-load{text-align: center;padding: 110px 0 220px;}
.list-load>img{margin-bottom: 22px;}
.list-load>p{font-size: 14px;color: #999;padding-top: 4px;}

.btn-load{display: none;position: absolute;left: 0;top: 0; width: 100%;height: 100%; background: #fff;}
.btn-load i{display: block;width: 100%;height: 100%;background: url(../../images/alertloading_m.gif) no-repeat center;}


.bzoom{cursor: url(//img.t.sinajs.cn/t5/style/images/common/big.cur), auto !important;}
.szoom{cursor: url(//img.t.sinajs.cn/t5/style/images/common/small.cur), auto !important;}
.dragIcon{cursor: move;}


/*无权限*/
.vip-workbench{text-align:center;}
.vip-workbench>p{font-size:14px;color:#333;margin-top:60px;margin-bottom:40px;line-height:25px;}
.vip-workbench>a{width:160px;height:40px;background:#d8b056;font-size:14px;color:#fff;border-radius:3px;line-height:40px;display:inline-block;}

/*下载图片*/

.downimg-layer{background:#fff;width:550px;min-height: 140px;position:absolute;bottom:56px;left:-10px;z-index:10;padding-bottom:10px;box-shadow:1px 1px 10px #ccc;display:none;}
.rclick-layer{background:#fff;width:550px;min-height: 140px;position:absolute;left:50%;margin-left:-275px;top:150px;padding-bottom:10px;z-index:101;display:none;}
.down-main-top{height:50px;line-height:50px;position:relative;margin-bottom:10px;border-bottom:1px solid #ededed;}
.down-main-top>h2{font-size:14px;color:#333;font-weight:bold;padding-left:20px;}
.down-close{width:22px;height:22px;background:url(../../images/new-detail/report-icons.png) no-repeat -14px -287px;position:absolute;right:26px;top:16px;cursor:pointer;}
.down-close:hover{background-position:-42px -287px;}
.down-table td,.down-table th{color:#333;font-size:14px;font-weight:bold;text-align:center;vertical-align: middle;}
.down-table td{height:45px;font-weight:normal;position: relative;}
.down-table td .eps-txt{line-height: 1;font-family: "Microsoft YaHei";font-size: 12px;letter-spacing: 1px;color: #a6a6a6;position: absolute;width: 100%;left: 54px;bottom: -2px;text-align: left;}
.down-table td a{background:#d8b056;width:95px;height:36px;display:inline-block;border-radius:3px;color:#fff;font-size:14px;line-height:36px;text-align:center;cursor:pointer;}
.box-arrow{background:url(../../images/new-detail/report-icons.png) no-repeat -69px -290px;width:19px;height:12px;position:absolute;bottom:-11px;left:70px;}
/*弹层*/
.bg-layer{background:url(../../images/opa50.png) repeat;width:100%;height:100%;position:fixed;top:0;left:0;z-index:100;display:none;}
.content-layer{width:500px;position: fixed; top:40%; left: 50%; margin-left: -250px; border-radius:4px;box-shadow:0 0 4px rgba(0, 0, 0, 0.2); -moz-box-shadow:0 0 4px rgba(0, 0, 0, 0.2); -ms-box-shadow:0 0 4px rgba(0, 0, 0, 0.2);  background:#fff; z-index: 103; display: none;}
.content-layer p{ text-align: center; font-size: 16px; font-weight: normal; color: #666; line-height:30px; padding: 45px 0 45px;  }
.content-layer p a{padding: 0 3px; color:#c90000;font-weight: 700;}
.content-layer p>span{max-width:230px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;display:inline-block;vertical-align:middle;margin-top:-5px;}


/*面料定制提示*/
.popky-infor{background:#fff;position:absolute;bottom:56px;left:-10px;z-index:10;padding-bottom:10px;box-shadow:1px 1px 10px #ccc;width:220px;height:70px; display: none}

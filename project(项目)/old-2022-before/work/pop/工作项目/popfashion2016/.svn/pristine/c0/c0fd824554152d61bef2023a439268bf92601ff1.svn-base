							<p class="r_title clearfix"><span class="fl"><i></i>时尚商学院推荐</span></p>
							<ul class="clearfix">
								{foreach $res.vendorads as $val}
								<li><a href="{$val.sAdLinkAddr}" target="_blank" title="{$val.sCompanyName}"><img src="{$val.sAdImageUrl}" alt="pic"><p class="name_bg">{$val.sCompanyName}</p></a></li>
								{/foreach}
							</ul>
						</div>
						{/if}
					</div>
				{else}
					<div class="layer-main {if $isMos}layer-main-max{/if}" style="margin:0;" data-type="pdf">
						{if $res.bigImgPath}
							{if $res.iIsFlash}
							<div class="bannerpic lazyload" style="display: table-cell;vertical-align: middle;height: 682px;">
								<img width="1110px" height="624px" src="{STATIC_URL3}/global/images/loading/search_loding.gif" data-original="{STATIC_URL3}{$res.bigImgPath}" alt="pic">
							</div>
							{else}
							<div class="bannerpic lazyload" style="padding: 18px 0px;">
								<img src="{STATIC_URL3}/global/images/loading/search_loding.gif" data-original="{STATIC_URL3}{$res.bigImgPath}" alt="pic">
							</div>
							{/if}
						{/if}
						{if $res.table=='t_trend_report'}
							{include file="t_trend_report_layer_pdf.html"}
						{elseif $res.table=='specialtopic'}
							{include file="specialtopic_layer_pdf.html"}
						{elseif $res.table=='specialtopic_graphic'}
							{include file="specialtopic_graphic_layer_pdf.html"}
						{else}
							{include file="fashion_shows_layer.html"}
						{/if}
						{if $res.ads}
						<div class="report_bottom_ad">

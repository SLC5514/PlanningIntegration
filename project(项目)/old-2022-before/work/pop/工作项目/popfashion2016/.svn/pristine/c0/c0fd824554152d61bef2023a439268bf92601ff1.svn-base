                                {/if}
                            </div>
                            {/if}
							{if $res.bigImgPath}
							<div class="bannerpic lazyload" style="padding: 70px 0px 0; width: 1110px; height: 624px;">
								<img src="{STATIC_URL3}/global/images/loading/search_loding.gif" data-original="{STATIC_URL3}{$res.bigImgPath}" alt="pic">
							</div>
							{/if}
						</div>
					</div>
					<div class="layer-main layer-indexdiv-main {if $isMos}layer-main-max{/if}">

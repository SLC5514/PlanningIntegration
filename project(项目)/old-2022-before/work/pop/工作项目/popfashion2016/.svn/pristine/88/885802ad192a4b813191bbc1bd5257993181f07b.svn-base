.p5{padding-top:70px;height:1120px;}
.p5>h2{width:170px;height:170px;background:url(../images/t12.png?2019070904) no-repeat;margin:0 auto;}
.p5>div.col-all{font-size:0;margin:80px auto 0;width:1000px;}
.col-all>div>a{margin-left:32px;width:210px;height:100px;display:inline-block;margin-bottom:40px;position:relative;}

.col-all>div>a:before,
.col-all>div>a:after{position:absolute;display:block;content:"";transition:all 0.1s;-webkit-transition:all 0.1s;-ms-transition:all 0.1s;}
.col-all>div>a:before{top:0;left:0;width:0;height:0;background:url(../images/line-t.png?2019070904) no-repeat;transition-delay: 0.3s; -ms-transition-delay: 0.3s;-webkit-transition-delay: 0.3s;}
.col-all>div>a:after{bottom:0;right:0;width:0;height:0;background:url(../images/line-b.png?2019070904) no-repeat;transition-delay: 0.1s; -ms-transition-delay: 0.1s;-webkit-transition-delay: 0.1s;}

.col-all>div>a span:before,
.col-all>div>a span:after{position:absolute;display:block;content:"";transition:all 0.1s;-webkit-transition:all 0.1s;-ms-transition:all 0.1s;} 
.col-all>div>a span:before{left:0;bottom:0;height:0;width:0;background:url(../images/line-l.png?2019070904) no-repeat;transition-delay: 0s; -ms-transition-delay: 0s;-webkit-transition-delay: 0s;}
.col-all>div>a span:after{right:0;top:0;height:0;width:0;background:url(../images/line-r.png?2019070904) no-repeat;transition-delay: 0.2s; -ms-transition-delay: 0.2s;-webkit-transition-delay: 0.2s;}
.col-all>div>a:hover:after,
.col-all>div>a:hover:before{width:210px;height:2px;}
.col-all>div>a:hover span:after,
.col-all>div>a:hover span:before{height:100px;width:2px;}
.col-all>div>a:hover:before{transition-delay: 0s; -ms-transition-delay: 0s;-webkit-transition-delay: 0s;}
.col-all>div>a:hover:after{transition-delay: 0.2s; -ms-transition-delay: 0.2s;-webkit-transition-delay: 0.2s;}
.col-all>div>a:hover span:after{transition-delay: 0.1s; -ms-transition-delay: 0.1s;-webkit-transition-delay: 0.1s;}
.col-all>div>a:hover span:before{transition-delay: 0.3s; -ms-transition-delay: 0.3s;-webkit-transition-delay: 0.3s;}

.col-all>div{opacity:0;}
.col-all>div.col1{transition: all 0.5s linear;-webkit-transition: all 0.5s linear ;-ms-transition: all 0.5s linear;}
.col-all>div.col2{transition: all 0.6s linear 0.6s;-webkit-transition: all 0.6s linear 0.6s;-ms-transition: all 0.6s linear 0.6s;}
.col-all>div.col3{transition: all 0.7s linear 1.3s;-webkit-transition: all 0.7s linear 1.3s;-ms-transition: all 0.7s linear 1.3s;}
.col-all>.tell{width: 940px;height: 365px;background-color: #ffffff;border: solid 2px #cccccc;padding-top: 32px;box-sizing: border-box;margin: 0 auto;
  transition: opacity 0.8s linear 2s;-webkit-transition: opacity 0.8s linear 2s;-ms-transition: opacity 0.8s linear 2s;}
.col-all>.tell>.tit{font-size: 24px;line-height: 22px;color: #333333;margin-bottom: 10px;text-align: center;}
.col-all>.tell>.contact-box{margin: 0;background: none;width: 940px;}
.col-all>.tell>.contact-box .tel-style>p,
.col-all>.tell>.contact-box .tel-style>p.tel{color: #333;}
.col-all>.tell>.contact-box .tel-style>p>i{background: url(../images/icon.png?2019070904) no-repeat -139px -68px;}

/*动画*/
.set5 .col-all>div{opacity:1;}


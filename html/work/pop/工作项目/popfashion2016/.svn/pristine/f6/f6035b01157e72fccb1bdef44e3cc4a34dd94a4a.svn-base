

body{background-color:#fff;}
.h5-header-box{display:none;}
.h5-footer{display:none;}
.h2-h5{display:none;}
/* banner */
.color-banner{background:url(../images/banner.jpg) no-repeat center top;width:100%;height:550px;text-align: center;overflow: hidden;position: relative;opacity:0;filter:alpha(opacity=0);transition:opacity 0.5s ease;-webkit-transition: opacity 0.5s ease;}
.color-banner>div{opacity:0;filter:alpha(opacity=0);transition:opacity 1s ease 0.5s;-webkit-transition: opacity 1s ease 0.5s;}
.color-banner>div>h1{font-size:60px;color:#fff;margin-top:140px;letter-spacing:5px;}
.color-banner>div>p{font-size:20px;color:#fff;line-height:36px;margin-top:50px;letter-spacing:1px;backface-visibility: hidden;-webkit-backface-visibility:hidden;}
.color-banner>div>p.h5-title{display:none;}
.color-banner>div>a.h5-reg-btn{display:none;}
/* banner动画 */
.anit-banner{opacity:1;filter:alpha(opacity=100);}
.anit-banner>div{opacity:1;filter:alpha(opacity=100);}

/* content1 */
.section-top{background:url(../images/bg1.jpg) no-repeat center top;width:100%;height:1330px;overflow: hidden;}

/* section1 */
.section1{padding-top:154px;padding-bottom:100px;}
.section1>.con-lf{width:540px;margin-left:40px;transform:translateY(120px);-webkit-transform:translateY(120px);opacity:0;filter:alpha(opacity=0);transition:all 0.5s ease;-webkit-transition:all 0.5s ease;}
.section1>.con-lf>h2{margin-top:55px;}
.section1>.con-lf>p{width:360px;margin-left:60px;font-size:16px;color:#333;line-height:27px;margin-top:30px;}
.section1>.con-lf>a{width:198px;height:40px;background-color:#505050;font-size:18px;color:#fff;display:inline-block;margin-left:50px;line-height:40px;text-align:center;margin-top:30px;position: relative;}
.section1>.con-lf>a>em{width:0;height:100%;position: absolute;left:0;top:0;background-color:#7592fe;transition:all 0.3s ease;}
.section1>.con-lf>a>span{position: relative;z-index:1;}
.section1>.con-lf>a>span>i{background:url(../images/arrow.png) no-repeat;width:21px;height:5px;display:inline-block;vertical-align:middle;margin-left:10px;margin-top:-2px;}
.section1>.con-rg{background:url(../images/pic_bg.png) no-repeat;width:599px;height:380px;box-shadow:0 0 10px rgba(120,120,120,0.2);transform:translateY(120px);-webkit-transform:translateY(120px);opacity:0;filter:alpha(opacity=0);transition:all 0.5s ease;-webkit-transition:all 0.5s ease;}
.section1>.con-rg>h3{height:50px;line-height:50px;font-size:20px;color:#666;margin-left:30px;}
.section1>.con-rg>div{position: relative;}
.section1>.con-rg>div>.img-left{width:303px;height:330px;opacity:0;filter:alpha(opacity=0);transform:scale(0);-webkit-transform:scale(0);transition:all 0.5s ease 1s;-webkit-transition:all 0.5s ease 1s;}
.section1>.con-rg>div>.img-right{width:262px;height:256px;margin-top:40px;margin-left:6px;transform: translateX(-60%);-webkit-transform: translateX(-60%);transition:all 0.5s ease 0.5s;-webkit-transition:all 0.5s ease 0.5s;}
.section1>.con-rg>div>.img-layer{width:100%;height:100%;background-color:rgba(0,0,0,0.2);position: absolute;left:0;top:0;z-index:1;opacity:0;filter:alpha(opacity=0);transform:scale(0);-webkit-transform:scale(0);transition:all 0.7s ease 1.5s;-webkit-transition:all 0.7s ease 1.5s;}
.section1>.con-rg>div>.img-layer>img{width:110px;height:110px;position: absolute;left:50%;top:50%;margin-left:-55px;margin-top:-55px;}
/* section1动画 */
.anit-section1>.con-lf{opacity:1;filter:alpha(opacity=100);transform:translateY(0);-webkit-transform:translateY(0);}
.anit-section1>.con-rg{opacity:1;filter:alpha(opacity=100);transform:translateY(0);-webkit-transform:translateY(0);}
.anit-section1>.con-rg>div>.img-right{transform: translateX(0);-webkit-transform: translateX(0);}
.anit-section1>.con-rg>div>.img-left{transform:scale(1);-webkit-transform:scale(1);opacity:1;filter:alpha(opacity=100);}
.anit-section1.section1>.con-rg>div>.img-layer{transform:scale(1);-webkit-transform:scale(1);opacity:1;filter:alpha(opacity=100);}
.section1>.con-lf>a:hover em{width:100%;}


                                        <p><a href="{$val.link_season}" target="_blank" title="{$val.seasonName}">{$val.seasonName}</a></p>
                                        <p>
                                            {if count($val.aAttribute)>1}
                                                {$val.iCategory_text}
                                            {else}
                                                <a target="_blank" href="{$val.link_category}" title="{$val.iCategory_text}">{$val.iCategory_text}</a>
                                                <a target="_blank" href="{$val.link_subcategory}" title="{$val.iSubcategory_text}">{$val.iSubcategory_text}</a>

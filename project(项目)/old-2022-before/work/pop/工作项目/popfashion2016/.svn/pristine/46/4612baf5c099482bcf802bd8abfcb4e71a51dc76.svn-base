.head_li>li.returnLi .forUl .forLi:hover span{color:#d8b056 !important;}
.head_li>li.returnLi .forUl label{position: absolute;top: -40px;right: 12px;font-size: 12px;color: #fff;cursor: pointer;}

.head_li>li.returnLi .forUl .switch em{background-position:  -21px -3px;}
.head_li>li.returnLi .forUl .switch:hover em{background-position:  -19px -17px;}
.head_li>li.returnLi .forUl .signOut em{background-position:  -36px -3px;}
.head_li>li.returnLi .forUl .signOut:hover em{background-position:  -36px -17px;}
.head_li>li.returnLi .forUl .myStudio em{background-position: -63px 0;}
.head_li>li.returnLi .forUl .myStudio:hover em{background-position: -63px -16px;}
.head_li>li.returnLi .forUl .myFavorites em{background-position: -83px 0;}
.head_li>li.returnLi .forUl .myFavorites:hover em{background-position: -83px -16px;}
.head_li>li.returnLi .forUl .myInterested em{background-position: 0 -32px;}
.head_li>li.returnLi .forUl .myInterested:hover em{background-position: -19px -32px;}
.head_li>li.returnLi .forUl .myRecommend em{background-position: -38px -32px;}

html,body,.view{height: 100%;overflow: auto;}
a,button,i.close{-webkit-tap-highlight-color:rgba(0,0,0,0);}
.content{padding-top: 0;height: 100%;overflow: auto;}
.content.on{background: url(../../images/virtual/share/bg.png) no-repeat center bottom/cover;}

.loading-div{width: 50px;height: 50px;left: 50%;top: 50%;margin-left: -25px;margin-top: -25px;}
.banner{height: 260px;color: #fff;background: url(../../images/virtual/share/banner.png) no-repeat center/cover;}
.banner .title{font-size:54px;font-weight:600;height:75px;line-height:75px;padding-top: 60px;}
.banner p{font-size:30px;line-height:42px;margin-top: 17px;}
.banner span{font-size:14px;line-height:20px;display: block;text-align: right;margin-top: 28px;}
.vir-list{margin-top: 40px;margin-bottom: 40px;/* margin-left: -37.5px; */position: relative;}
.vir-list>li{position: absolute;float: left;width:210px;/* height:276px; */border-radius:5px;border:1px solid #eee;overflow: hidden;/* margin-left: 37.5px;margin-bottom: 48px; */box-sizing: border-box;font-size: 0;cursor: pointer;}
.vir-list>li>img{width: 100%;display: inline-block;vertical-align: middle;}
.invitation-code-box{display: none;position: fixed;top: 50%;left: 50%;margin-left: -200px;margin-top: -128px;width:400px;height:256px;background:#fff;box-shadow:0px 0px 17px 0px rgba(0,0,0,0.1);border-radius:5px;padding: 40px 30px 30px 30px;box-sizing: border-box;}
.invitation-code-box>.title{font-size:24px;font-weight:bold;color:#333;line-height:33px;margin-bottom: 30px;text-align: center;}
.invitation-code-box .ipt{width:340px;height:40px;line-height:40px;background:#fff;border-radius:5px;border:1px solid #d8d8d8;position: relative;white-space: nowrap;overflow: hidden;}
.invitation-code-box .ipt>label{display: inline-block;height: 100%;font-size:14px;color: #333;padding-left: 10px;}
.invitation-code-box .ipt>input{width: 270px;height: 100%;border: none;padding-right: 10px;box-sizing: border-box;}
.invitation-code-box .ipt>.msg{display: none;position: absolute;left: 0;bottom: -27px;font-size:12px;color: #C82F2F;line-height:17px;}
.invitation-code-box>.btn-box{text-align: center;}
.invitation-code-box>.btn-box>button{width:120px;height:40px;background:#1c232b;border-radius:5px;margin-top: 43px;font-size:14px;color: #fff;}

/* 无效链接 */
.header{display: none;position: fixed;top: 0;left: 0;right: 0;height:80px;line-height:80px;background:#fff;box-shadow:0px 2px 5px 0px rgba(0,0,0,0.08);font-size:20px;color: #333;text-align: left;padding-left: 40px;}
.header>span{margin-left: 60px;position: relative;top: -2px;}
.empty-ctn{display: none;position: fixed;left: 50%;top: 280px;margin-left: -100px;width: 200px;}
.empty-ctn>img{display: block;width: 180px;height: 158px;margin: 0 auto 40px;}
.empty-ctn>.title{font-size:16px;color: #666;margin-bottom: 20px;}
.empty-ctn>.msg{font-size:14px;color: #999;line-height:22px;}
.empty-ctn>.msg>p:first-child{margin-bottom: 5px;}

.content.empty-box{background: none;}
.content.empty-box>.banner{display: none;}
.content.empty-box>.header{display: block;}
.content.empty-box>.empty-ctn{display: block;}

/* 查看大图 */
.preview-box{display: none;position: fixed;top: 0;left: 0;right: 0;bottom: 0;overflow: auto;z-index: 501;}
.preview-box>.ctn{position: absolute;top: 0;left: 50%;margin: 100px 0 100px -400px;width:800px;height:1050px;}
.preview-box>.ctn>.probar{display: none;}
.preview-box>.ctn>.close{position: absolute;top: -70px;right: -80px;width: 40px;height: 40px;cursor: pointer;background: url(../../images/virtual/share/icon_close.png) no-repeat center / cover;}
.preview-box>.ctn>.prev{position: absolute;top: 50%;left: -150px;margin-top: -15px;width: 30px;height: 56px;cursor: pointer;background: url(../../images/virtual/share/icon_left.png) no-repeat center / cover;}
.preview-box>.ctn>.next{position: absolute;top: 50%;right: -150px;margin-top: -15px;width: 30px;height: 56px;cursor: pointer;background: url(../../images/virtual/share/icon_right.png) no-repeat center / cover;}
.preview-box>.ctn>img{display: block;width: 100%;}

/* H5 */
.h5-footer{display: none;height: 3.5rem;background: #1C232B;text-align: center;padding: 0.5rem 0.75rem;box-sizing: border-box;color: #fff;}
.h5-footer>.title{font-size: 0.46rem;font-weight:600;line-height: 0.65rem;margin-bottom: 0.23rem;}
.h5-footer>.info{font-size: 0.24rem;line-height: 0.33rem;margin-bottom: 0.38rem;}
.h5-footer>.btn{ width:6rem; height:0.9rem;line-height:0.9rem; background:#f5576c; border-radius:0.45rem;font-size: 0.36rem;font-weight:bold;color: #fff;}

@media screen and (max-width: 750px){
    .view{min-width: auto;}
    .section {width: 100%;}
    .content.on{background: url(../../images/virtual/share/bg.png) no-repeat center bottom / auto 50%;}
    .banner{height: 3rem;background: url(../../images/virtual/share/banner_h5.png) no-repeat center/cover;}
    .banner>div{display: none;}
    .vir-list{margin: 0.3rem 0.3rem 0.6rem;}
    .vir-list>li{width: 2.1rem;border-radius: 0.05rem;}
    /* 邀请码 */
    .invitation-code-box{width: 5.7rem;height: 4.9rem;margin-left: -2.85rem;margin-top: -2.45rem;padding: 0.7rem 0.5rem 0.4rem;border-radius: 0.05rem;}
    .invitation-code-box>.title{font-size: 0.36rem;line-height: 0.5rem;margin-bottom: 0.44rem;}
    .invitation-code-box .ipt{width: 4.7rem;height: 0.8rem;line-height: 0.8rem;border-radius: 0.05rem;display: flex;}
    .invitation-code-box .ipt>label{font-size: 0.28rem;padding-left: 0.2rem;color: #999;}
    .invitation-code-box .ipt>input{width: 3.35rem;padding-right: 0.2rem;}
    .invitation-code-box .ipt>.msg{bottom: -0.5rem;font-size: 0.24rem;line-height: 0.33rem;}
    .invitation-code-box>.btn-box>button{ width: 2.6rem; height: 0.9rem;line-height: 0.9rem; border-radius: 0.05rem;font-size: 0.36rem;margin-top: 1.12rem;}
    /* 无效链接 */
    .header{min-width: auto;height: 1.2rem;line-height: 1.2rem;padding-left: 0;text-align: center;}
    .header>a{position: absolute;left: 0.3rem;top: 0.4rem;width: 0.4rem;height: 0.4rem;background: url(../../images/virtual/icon_close.png) no-repeat center / cover;}
    .header>a>img{display: none;}
    .header>span{margin-left: 0;top: 0;font-size: 0.36rem;}
    .content.empty-box{background: #F5F5F5;}
    .content.empty-box>.section{display: none;}
    .content.empty-box .h5-footer,.h5-footer{display: block;}
    .content.on .h5-footer{display: none;}
    .empty-ctn{position: initial;height: calc(100vh - 1.2rem - 3.5rem);min-height: 8.64rem;width: 3.44rem;margin: 1.2rem auto 0;padding-top: 2.1rem;box-sizing: border-box;}
    .empty-ctn>img{width: 2.26rem;height: 1.98rem;margin-bottom: 0.35rem;}
    .empty-ctn>.title{font-size: 0.28rem;margin-bottom: 0.35rem;}
    .empty-ctn>.msg{font-size: 0.24rem;line-height: 0.33rem;}
    .empty-ctn>.msg>p:first-child{margin-bottom: 0.08rem;}
    /* 查看大图 */
    .preview-box{background: #000;}
    .preview-box>.ctn{left: 0;width: 7.5rem;height: 8.6rem;margin: 1.3rem auto 1.3rem;}
    .preview-box>.ctn>.probar{display: block;}
    .preview-box>.ctn>.close{top: -1rem;left: 0.3rem;width: 0.4rem;height: 0.4rem;}
    .preview-box>.ctn>.probar{position: absolute;top: -1rem;font-size: 0.32rem;color: #fff;width: 100%;text-align: center;}
    .preview-box>.ctn>.prev,.preview-box>.ctn>.next{display: none;}
    .preview-box>.ctn>img{ position: relative; left: 0;}
}

.contact{background: #fff;}
.contact ul{overflow:hidden;}
.contact ul li{float:left;}
.contact ul li i{display: inline-block;width: 34px;height:30px;vertical-align: middle;}
.contact ul li.phone i{background: url(../images/tel2.jpg) no-repeat center center;margin-right: 3px;}
.contact ul li.inter i{background: url(../images/ylb.jpg) no-repeat center center;margin-right:3px;}

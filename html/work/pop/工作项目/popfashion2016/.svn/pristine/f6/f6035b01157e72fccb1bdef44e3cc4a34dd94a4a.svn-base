.anit-downlist1>.con-lf>div>div.downlist1>img{height:100%;opacity:1;filter:alpha(opacity=100);}
.anit-downlist2>.con-lf>div>div.downlist2>img{height:100%;opacity:1;filter:alpha(opacity=100);}
.anit-downlist2>.con-lf>div>div.downlist1>img{height:0;opacity:0;filter:alpha(opacity=0);}
.anit-downlist3>.con-lf>div>div.downlist3>img{height:100%;opacity:1;filter:alpha(opacity=100);}
.anit-downlist3>.con-lf>div>div.downlist2>img{height:0;opacity:0;filter:alpha(opacity=0);}
.anit-downlist4>.con-lf>div>div.downlist4>img{height:100%;opacity:1;filter:alpha(opacity=100);}
.anit-downlist4>.con-lf>div>div.downlist3>img{height:0;opacity:0;filter:alpha(opacity=0);}



.section2>.con-rg>a:hover em{width:100%;}

/* content2 */
.section-bottom{background:url(../images/bg2.jpg) no-repeat center top;width:100%;height:1227px;overflow: hidden;}

/* section3 */
.section3{padding-top:140px;padding-bottom:100px;overflow: hidden;}
.section3>.con-lf{width:480px;transform:translateY(120px);-webkit-transform:translateY(120px);opacity:0;filter:alpha(opacity=0);transition:all 0.5s ease;-webkit-transition:all 0.5s ease;}
.section3>.con-lf>h2{margin-top:35px;}
.section3>.con-lf>p{margin-left:54px;font-size:16px;color:#333;line-height:26px;margin-top:30px;width:335px;}

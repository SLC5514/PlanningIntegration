

body{background-color:#fff;}
.h5-header-box{display:none;}
.h5-footer{display:none;}
.h2-h5{display:none;}
/* banner */
.color-banner{background:url(../images/banner.jpg) no-repeat center top;width:100%;height:670px;text-align: center;overflow: hidden;position: relative;opacity:0;filter:alpha(opacity=0);transition:opacity 0.5s ease;-webkit-transition: opacity 0.5s ease;}
.color-banner>div{opacity:0;filter:alpha(opacity=0);transition:opacity 1s ease 0.5s;-webkit-transition: opacity 1s ease 0.5s;}
.color-banner>div>h1{font-size:60px;color:#fff;margin-top:140px;letter-spacing:5px;}
.color-banner>div>p{font-size:20px;color:#fff;line-height:36px;margin-top:50px;letter-spacing:1px;}
.color-banner>div>p.h5-title{display:none;}
.color-banner>div>a.h5-reg-btn{display:none;}
.color-banner>div>div.banner-nav{position: absolute;bottom:0;}
.banner-nav>span{width:1px;height:57px;background-color:#fff;display:inline-block;float: left;position: relative;top:53px;}
.banner-nav>a{float: left;width:299px;cursor: pointer;}
.banner-nav>a>p{margin-bottom:35px;font-size:26px;color:#fff;}
.banner-nav>a>p>span{width:26px;height:27px;display:inline-block;}
.banner-nav>a>p.num1>span{background: url(../images/1.png) no-repeat;}
.banner-nav>a>p.num2>span{background: url(../images/2.png) no-repeat;}
.banner-nav>a>p.num3>span{background: url(../images/3.png) no-repeat;}
.banner-nav>a>p.num4>span{background: url(../images/4.png) no-repeat;}
.banner-nav>a>span{display:inline-block;width:20px;height:2px;background-color:#fff;margin-bottom:31px;}
.banner-nav>a:hover p.num1>span{background: url(../images/1-h.png) no-repeat;}
.banner-nav>a:hover p.num2>span{background: url(../images/2-h.png) no-repeat;}
.banner-nav>a:hover p.num3>span{background: url(../images/3-h.png) no-repeat;}
.banner-nav>a:hover p.num4>span{background: url(../images/4-h.png) no-repeat;}
.banner-nav>a:hover p{color:#df376f;}
.banner-nav>a:hover span{background-color:#df376f;}
/* banner动画 */
.anit-banner{opacity:1;filter:alpha(opacity=100);}
.anit-banner>div{opacity:1;filter:alpha(opacity=100);}

/* content1 */
.section-top{background:url(../images/bg1.jpg) no-repeat;width:100%;height:1355px;overflow: hidden;}

/* section1 */
.section1{padding-top:145px;padding-bottom:100px;}
.section1>.con-lf{width:405px;margin-left:50px;transform:translateY(120px);-webkit-transform:translateY(120px);opacity:0;filter:alpha(opacity=0);transition:all 0.5s ease;-webkit-transition:all 0.5s ease;}
.section1>.con-lf>h2{margin-top:55px;}
.section1>.con-lf>p{margin-left:50px;font-size:16px;color:#333;line-height:26px;margin-top:25px;}
.section1>.con-lf>a{width:198px;height:40px;background-color:#505050;font-size:18px;color:#fff;display:inline-block;margin-left:50px;line-height:40px;text-align:center;margin-top:30px;position: relative;}
.section1>.con-lf>a>em{width:0;height:100%;position: absolute;left:0;top:0;background-color:#55b7df;transition:all 0.3s ease;}
.section1>.con-lf>a>span{position: relative;z-index:1;}
.section1>.con-lf>a>span>i{background:url(../images/arrow.png) no-repeat;width:21px;height:5px;display:inline-block;vertical-align:middle;margin-left:10px;margin-top:-2px;}
.section1>.con-rg{background:url(../images/pic_bg.png) no-repeat;width:599px;height:380px;box-shadow:0 0 10px rgba(120,120,120,0.2);transform:translateY(120px);-webkit-transform:translateY(120px);opacity:0;filter:alpha(opacity=0);transition:all 0.5s ease;-webkit-transition:all 0.5s ease;}
.section1>.con-rg>h3{height:50px;line-height:50px;font-size:20px;color:#666;margin-left:30px;}
.section1>.con-rg>ul{margin-top:50px;}
.section1>.con-rg>ul>li{float: left;width:160px;height:230px;background-color:#fafafa;border-radius:4px;margin-left:30px;text-align:center;opacity:0;filter:alpha(opacity=0);-webkit-transform:scale(0);transform:scale(0);}
.section1>.con-rg>ul>li.li1{transition:all 1s ease 0.5s;-webkit-transition: all 1s ease 0.5s;}
.section1>.con-rg>ul>li.li2{transition:all 1s ease 10s;-webkit-transition: all 1s ease 1s;}
.section1>.con-rg>ul>li.li3{transition:opacity 1s ease 1.5s;-webkit-transition: all 1s ease 1.5s;}
.section1>.con-rg>ul>li>img{margin-top:30px;}
.section1>.con-rg>ul>li>p{margin-top:30px;font-size:14px;color:#666;}
/* section1动画 */
.anit-section1>.con-lf{opacity:1;filter:alpha(opacity=100);transform:translateY(0);-webkit-transform:translateY(0);}
.anit-section1>.con-rg{opacity:1;filter:alpha(opacity=100);transform:translateY(0);-webkit-transform:translateY(0);}
.anit-section1>.con-rg>ul>li{opacity:1;filter:alpha(opacity=100);-webkit-transform:scale(1);transform:scale(1);}
.section1>.con-lf>a:hover em{width:100%;}


<script id="T_DownloadType" type="text/html">
<thead>
	<tr>
		<th width="25%">格式</th>
		<th width="25%">尺寸</th>
		<th width="25%">文件大小</th>
		<th width="25%">点击下载</th>
	</tr>
</thead>
<tbody {{if downloadType}} data-downtype='{{downloadType}}'{{/if}}>		
	{{if !downloadType}}
	<tr>
		<td>.jpg</td>
		<td id="imgShape">尺寸</td>
		<td id="imgSize">文件大小</td>
		<td><a class="download_btn" href="javascript:void(0);" data-bp="" title="下载">下载</a></td>
	</tr>
	{{/if}}
</tbody>
</script>
<script id="T_Download" type="text/html">
    <a href="javascript:void(0);" class="top_bg grey downbtn {{if visit}}grey{{else}}pow_down{{/if}} {{if !download}}pow{{/if}}" id="J_DownNumContanier" data-path="" data-column="" data-id="" title="下载"><span id="downtxt">下载</span></a>
</script>

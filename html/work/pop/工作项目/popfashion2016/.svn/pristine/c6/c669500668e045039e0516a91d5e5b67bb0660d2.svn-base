.contact ul li.inter a:hover{color:#33cccc;}
.contact ul li.phone{margin:0 38px;margin-top:5px;}
.contact ul li.inter{margin-top:6px;}
.group a.radio img{position: absolute;top:266px;right:32px;}
.group a.radio{cursor: pointer;}
.service p{position: relative;}
.service .uliaobao{font-family: "微软雅黑";font-size: 21px;color:#333;right:0;bottom:5px;position: absolute;cursor: pointer;}
.service .uliaobao:hover{color:#37bab6;}
.service .uliaobao i{display: inline-block;vertical-align: middle;width: 30px;height: 32px;background: url(../images/ylb.jpg) no-repeat center center;margin-right:4px;}
.radio_d{width: 100%;height:100%;background: #000;display: none;}
.radio_d .radio_opa{width: 100%;height:100%;background: #000;position: fixed;top:0;right:0;opacity: 0.6;filter: alpha(opacity=60);}
.radio_d p{position: fixed;border-radius: 4px;left:50%;margin-left:-355px;top:200px;z-index: 100;background: url(../images/radio3.png) no-repeat;width: 710px;height: 353px;}
object{width: 569px;height: 321px;display: inline-block;margin:6px 6px 6px 69px;}
.radio_d p i{display: inline-block;width: 42px;height: 42px;background: url(../images/close.png) no-repeat;position: absolute;top:-40px;right:24px;cursor: pointer;}
                            {if $res.live_status == 1}
							<a href="{$res.sLiveUrl}" target="_blank">立即预约<i></i></a>
                            {elseif $res.live_status == 2}
                            <a href="{$res.sLiveUrl}" target="_blank">回看解读<i></i></a>
                            {elseif $res.live_status == 3}
                            <a href="{$res.sLiveUrl}" target="_blank">立即收看<i></i></a>
                            {/if}
						</div>
                        {/if}
					</div>
				</div>
				{if !empty($tryOutList)}

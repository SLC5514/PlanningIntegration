.fl{float:left;}
.fr{float:right;}
.style-width{width:1200px;margin:0 auto;}
.styles-inside-header{position:fixed;width:100%;height:79px;left:0;top:0;z-index:99;}
.first-level-nav{height:49px;background:#fff;width:100%;border-bottom: 1px solid #eee;}
.style-nav-left{margin-left:25px;}
.style-nav-left>div{float:left;height:49px;position:relative;cursor:pointer;}
.style-nav-left>div>a{line-height:49px;display:inline-block;color:#1C232B;font-size:14px;font-weight:bold;padding: 0 15px;}
.style-nav-left>div>a>em{background:url(../../../images/lists/styles-list/s_icon.png) no-repeat -39px 0;width:9px;height:5px;display:inline-block;vertical-align:middle;margin-top:-2px;margin-left:8px;}
.style-nav-left>div.style-brand>a{font-size:12px;color:#fff;cursor:default;}
/* .style-nav-left>div:hover{background:#29333c;} */
.style-nav-left>div.style-brand:hover{background:none;}
.style-nav-left>div>.nav-downlist{width:100%;background:#fff;position:absolute;left:0;top:38px;z-index:10;display:none;border-radius: 5px;box-shadow:0px -1px 5px 0px rgba(0,0,0,0.08);padding-bottom: 10px;}
.style-nav-left>div>.nav-downlist>a{font-size:12px;color:#666;display:block;text-align: center;margin-top: 10px;line-height: 16px;}
.style-nav-left>div>.nav-downlist>a:hover{color:#1c232b;}

.style-nav-right .user-contain{height: 49px;line-height: 49px;}
.style-nav-right .login-success-section{top: 49px;}
/* .style-nav-right>ul>li{float:left;padding:0 5px;height:38px;position:relative;margin-right:10px;}
.style-nav-right>ul>li>a{line-height:38px;display:inline-block;cursor:pointer;color:#fff;font-size:14px;}
.style-nav-right>ul>li.returnLi>a{color:#999;}
.style-nav-right>ul>li:hover{background:#13181e;}
.style-nav-right>ul>li>a:hover{color:#d8b056;} */
/*登录注册*/
.style-nav-right>ul>li.login_li{margin-right:0;}
.style-nav-right>ul>li.white-line{margin-right:0;background:#fff;}
/*申请试用*/
.style-nav-right>ul>li.apply-try a{background:url(../../../images/vi_icon.png?2018101108) no-repeat 0 -110px;width:92px;height:28px;display:inline-block;line-height:28px;padding-left:38px;margin-top:6px;color:#000;font-size:14px;}
.style-nav-right>ul>li.apply-try:hover a{background-position:0 -68px;}

/*全部导航按钮*/
.style-nav-right>ul>li.all-nav-li{margin-right:0;padding:0;width:40px;height:38px;background:#d8b056;cursor:pointer;}
.style-nav-right>ul>li.all-nav-li>span{background:url(../../../images/rotate1.png) no-repeat;display:inline-block;width:20px;height:16px;margin:12px 0 0 10px;transition:0.4s;}
.style-nav-right>ul>li.all-nav-li.fs>span{background:url(../../../images/rotate2.png) no-repeat;width:20px;height:20px;transform:rotate(-180deg);}
/*我的工作室*/
.welcome_name em{background:url(../../../images/vi_icon.png) no-repeat -47px -22px;display:inline-block;vertical-align:middle;width:19px;height:19px;margin-right:5px;margin-top:-2px;}
.welcome_name:hover em{background-position:-105px -45px;}

/*我的收藏*/
.collect_li em{background:url(../../../images/vi_icon.png) no-repeat -77px -21px;display:inline-block;vertical-align:middle;width:16px;height:16px;margin-right:5px;margin-top:-2px;}
.collect_li:hover em{background-position:-133px -44px;}
/*消息*/
.information_li span.ix{background:url(../../../images/vi_icon.png) no-repeat -130px -21px;display:inline-block;width:6px;height:6px;position:absolute;top:12px;right:0;}

.style-nav-right>ul>li.fg-line{width:1px;height:14px;background:#61656b;margin-top:14px;padding:0;}

/*下拉导航*/
.dropdown-nav{top:38px;}


.dp-level-nav{background:#fff;height:44px;position:relative;box-shadow:0px 2px 5px 0px rgba(0,0,0,0.08);}
.dp-level-nav .category-content{margin-left:20px;height:44px;margin-right:30px;}
.dp-level-nav .dp-category{float:left;padding:0 20px;line-height:44px;position:relative;}
.dp-level-nav .dp-category>a{font-size:14px;color:#666;display:block;cursor:pointer;}
.dp-level-nav .dp-category>a:hover{color:#1C232B;}
.dp-level-nav .dp-category>a.selected{color:#1C232B;font-weight: bold;}
.dp-level-nav .category-more-btn{width:30px;height:30px;background:#d2d2d2;color:#333;font-family:"simsun";border-radius:4px;line-height:30px;text-align:center;font-weight:bold;cursor:pointer;position:absolute;right: 0;top:5px;display:none;}
.dp-level-nav .category-more-prev-btn{left:0;right:auto;}
.dp-level-nav .category-more-btn:hover{background:#d8b056;color:#fff;}
.dp-category>.dp-category-doenlist{position:absolute;left:0;top:44px;width:100%;background:#fff;box-shadow: 0px -1px 5px 0px rgba(0,0,0,0.08);border-radius: 5px; display:none;padding-bottom: 10px;}
.dp-category>.dp-category-doenlist>a{text-align: center; display:block;color:#666;font-size:12px;cursor:pointer;margin-top: 10px;line-height: 16px;}
.dp-category>.dp-category-doenlist>a:hover{color:#1C232B;}

/*未登录提示条*/
.rightc_.wraning,.rightc_brand{width:100%;}
.wran{margin-left:0;background:url(../../../images/s_1200_h.png) no-repeat;height:45px; line-height: 45px;margin-top: 10px;padding:0 10px;width:1180px;}
.w_brand{margin-left:20px;border: 1px solid #ffcc33;background-color: #ffffcc;margin-top: 10px;padding:15px 6px 5px;}
.w_brand .brand_name{font-size:14px;color:#000;}
.rightc_brand div.fl{width:660px;}
.rightc_brand div.fl a{margin-right:5px;display:inline-block;margin-bottom:10px;font-weight:bold;}
.rightc_brand div.fl a:hover{color:#d8b056;text-decoration:underline;}
.rightc-wraning p{ background-position:-148px -257px; padding-left: 30px; font-size: 12px;letter-spacing:-1px;}
.rightc-wraning .yellow{color: #ff6633;text-decoration:underline;}
.rightc-wraning .wran_tel em{font-style:normal;color:#ff6633;font-size:12px;font-weight:bold;}
.rightc-wraning a.yellow:hover{text-decoration: underline;}
.rightc-wraning .neiyelistRight{position:relative;font-size:12px;}
.rightc-wraning .neiyelistRight span.fl{margin-right:8px;font-weight:bold;display:inline-block;}
.rightc-wraning .qq{background-position:7px -316px; background-color: #ff6633; width:60px;text-align: center;height: 30px;line-height:30px;color: #fff; padding-left:20px; margin-top:7px;*margin-top:0;border-radius: 3px;display:inline-block;}
.rightc-wraning .apply_try{font-size:12px;color:#000;}
.rightc-wraning .apply_try i{background:url(../../../images/neiyeList.png) no-repeat -33px -607px;width:23px;height:23px;display:inline-block;vertical-align: middle;margin-right:3px;margin-top:-3px;}
.tour_cus{background:url(../../../images/s_1200.png) no-repeat;}
.tour_cus p{background-position:-148px -214px; }
.tour_cus .wran_tel{display:inline-block;}
.tour_cus .yellow{color:#d8b056;}
.tour_cus .wran_tel em{font-style:normal;color:#d8b056;font-size:12px;font-weight:bold;}
.tour_cus .qq{background-color:#d8b056;}
.tour_cus .apply_try i{background-position:0 -607px;}

.style-inside-imglist{margin-top:100px;margin-bottom:80px;}
.style-inside-imglist ul{margin-left:-15px;margin-top:20px;min-height:726px;}
.style-inside-imglist ul>li{width:187px;height:242px;float:left;margin-left:15px;}
.style-inside-imglist ul>li>.img-div{width:165px;height:220px;padding:10px;border:1px solid #f9f9f9;position:relative;border-bottom:none;}
.style-inside-imglist ul>li>.img-div>a{width:100%;height:100%;display:inline-block;overflow:hidden;}
.style-inside-imglist ul>li>.img-div>a>img{max-width:165px;max-height:220px;}
.style-inside-imglist ul>li>.img-div .Dtime{color:#666;display:none;width:165px;height:28px;line-height:28px;text-align:left;font-size:12px;background:url(../../../images/opa_fff_80.png) repeat;text-align:center;position:absolute;left:10px;bottom:10px;}
.style-inside-imglist ul>li>.img-div .Dtime em{background:url(../../../images/neiyeList.png) no-repeat -37px 0;height:12px;width:12px;display: inline-block;vertical-align:middle;margin-right:5px;margin-top:-2px;}
.style-inside-imglist ul>li>.img-div .style-inside-position{position:absolute;left:-1px;top:235px;border:1px solid #fff;border-top:none;width:165px;padding:0 10px;display:none;z-index:10;background:#fff;}
.style-inside-position>a{color:#333;font-size:12px;margin-right:5px;line-height:25px;padding-left:5px;word-break:break-all;}
.style-inside-position>a:first-child{padding-left:0;}
.style-inside-position >a+a{border-left:1px solid #e5e5e5;}
.style-inside-imglist ul>li>.img-div .collect-download{position:absolute;top:20px;right:20px;font-size:0;display:none;}
.collect-download>a{background:url(../../../images/lists/styles-list/s_icon.png) no-repeat 0 0;display:inline-block;width:30px;height:30px;cursor:pointer;}
.collect-download>.collect-btn{background-position:0 -34px;}
.collect-download>.collect-btn.cur{background-position:-39px -34px;}
.collect-download>.download-btn{background-position:0 -70px;margin-left:10px;}
.collect-download>.collect-btn:hover{background-position:-81px -70px;}
.collect-download>.download-btn:hover{background-position:-39px -70px;}
.style-inside-imglist ul>li:hover .img-div{border-color:#eee;background:#fff;}
.style-inside-imglist ul>li:hover .style-inside-position{border-color:#eee;}
.style-inside-imglist ul>li .style-inside-position>a:hover{color:#d8b056;}
.style-inside-imglist ul>li:hover .Dtime{display:block;}
.style-inside-imglist ul>li:hover .collect-download{display:block;}
.scroll-load{text-align:center;padding:10px 0 30px;display:none;}
.rightc_bottom{display:none;}
.rightc_bottom .pages{margin-top:25px;}
/*无搜索内容*/
.stylelist-search-null{width:300px;margin:0 auto;text-align:center;padding:100px 0;}
.stylelist-search-null>p{font-size:18px;line-height:30px;color:#666;margin-top:20px;}

/*水印*/
.shuiy{background:url(../../../images/just-vip-new.png) no-repeat center center;opacity:.8;width:165px;height:220px;position:absolute;top:10px;left:10px;z-index:1;}

/* 登录注册引导 */
.login-box{position: fixed;height: 34px;font-size: 16px; line-height: 34px;padding: 30px 0;text-align: center;z-index: 62;left: 50%;width: 760px;margin-left: -380px;  top: 300px;background: #fff;border-radius: 3px;box-shadow: 0 2px 10px #ccc; }
.login-box>p{display: inline-block;vertical-align: middle; font-size: 18px; color: #333;margin-right: 50px;}
.login-box>a{display: inline-block;vertical-align: middle; height: 34px;padding: 0 30px; color: #fff;border-radius: 3px;}
.login-box>.login-link{background: #333;margin-right: 17px;}
.login-box>.red-rg{background: #d8b056;}
.login-box>.login-link:hover{background: #d8b056;}
.login-box>.red-rg:hover{background: #f4cd69;}








/*普通用户显示样式*/
.general-user-status .rightc-wraning,.general-user-status .dp-level-nav{display: none;}
.general-user-status.styles-inside-header{height: 38px;}
.general-user-status.style-inside-imglist{margin-top: 70px;}

.style-inside-imglist .general-user-status-list{margin-left:-20px;margin-top:20px;min-height: auto;height: 222px;overflow: hidden;}
.style-inside-imglist .general-user-status-list>li{position: relative;width:167px;height:222px;float:left;margin-left:20px;border:1px solid #f9f9f9;}
.style-inside-imglist .general-user-status-list>li>img{max-width:165px;max-height:220px;display: block;}
.style-inside-imglist .general-user-status-list>li>div{z-index: 10;position: absolute;left: 0px;top: 0px;width: 100%;height: 100%;background:linear-gradient(to top, rgba(255,255,255,0.8), rgba(255,255,255,0.3));}

.style-inside-imglist .login-info{padding-top: 180px;margin-bottom: 200px;text-align: center;}
.style-inside-imglist .login-info>h3{font-size: 20px;margin-bottom: 40px;text-align: center;color: #333;font-weight: bold;}
.style-inside-imglist .login-info>a{display: inline-block;width: 180px;height: 45px;line-height: 45px;font-size: 16px;border-radius: 3px;background-color: #d8b056;color: #fff;}
.style-inside-imglist .login-info>a.login-link{width: 178px;height: 43px;line-height: 43px;margin-right: 30px;border:1px solid #333;background-color: #fff;color: #333;}
.style-inside-imglist .login-info>div{margin-top: 58px;padding-top: 50px;border-top: 1px solid #ddd;font-size: 16px;}
.style-inside-imglist .login-info>div>span{display: inline-block;color: #f4523b;}
.style-inside-imglist .login-info>div>a{display: inline-block;width: 105px;padding-left: 15px;height: 30px;line-height: 30px;margin-left: 10px;color: #d8b056;border: 1px solid #999999;border-radius: 15px;background-image: url(../../../images/user/qq-icon1.png);background-repeat: no-repeat;background-position: 15px center;}




@media screen and (min-width: 1500px) {
	.style-width{width:1500px;}
	.style-inside-imglist ul{margin-left:0;}
	.style-inside-imglist ul>li{margin-left:0;}
	.style-inside-imglist .general-user-status-list{margin-left: -20px;}
	.style-inside-imglist .general-user-status-list>li{margin-left: 20px;}
	.wran{background:url(../../../images/s_1500_h.png) no-repeat;width:1460px;padding:0 20px;}
	.tour_cus{background:url(../../../images/s_1500.png) no-repeat;width:1460px;}
}
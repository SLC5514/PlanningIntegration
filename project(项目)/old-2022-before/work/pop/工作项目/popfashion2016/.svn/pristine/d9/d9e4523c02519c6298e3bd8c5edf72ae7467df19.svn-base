.top-main .top-list .list-btn{width:200px;height:50px;line-height: 50px;background:#1C232B;border-radius:25px;text-align: center;margin: 0 auto;font-size:16px;
font-weight:400;color:#fff;cursor: pointer;border:1px solid rgba(238,238,238,1);}
.top-main .top-list .list-btn i{display: inline-block;width: 14px;height: 14px;background: url(../../images/top/list-btn-hover.png);vertical-align: -1px;margin-right: 5px;}


body,html{height: 100%;}
#canvas-content{width: 100%;height: 900px;position: relative;min-width: 1200px;}
canvas{z-index: -1;min-width: 1200px;}
#shape {
  position: fixed;
  left: 0;
  top: 0;
  width: 100px;
  height: 100px;
  background: #e5c8a5;
  visibility: hidden;
  transition: all 0.4s ease-in-out;
}
#shape.count0 {
  background: #e5c8a5;
}
#shape.count1 {
  background: #e6b133;
}
#shape.count2 {
  background: #fad2d9;
}
#shape.count3 {
  background: #c397cc;
}

#scroll-nav-contain {
  position: relative;
  top: 0;
  right: 0;
  left: 0;
  z-index: 1;
  height: 100%;
  
  overflow: hidden;
}


#title-list {
  width: 56%;
  height: 100%; 
  overflow: initial;
}

#title-list .swiper-slide {
  position: relative;
  display: flex;
  justify-content: center;align-items: center;
  transition: all ease 1s;
}
#title-list .swiper-slide a {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);  
  width:100%;
  text-align: center;
  height: 9.25vw;
  line-height: 9.25vw;
  border:1px solid #fff;
  border-right:none;
  box-sizing: border-box;
}

#title-list .swiper-slide a strong{
  transition: all 0.5s ease 0s;
  color: #fff;
  font-size:4.5vw;
  position: relative;
  left: 0;
}


#title-list .swiper-slide:last-child a{
  border-right:1px solid #fff;
}

#title-list .swiper-slide a>span{
  transition: all 0.5s ease 0s;
  background: url(../images/arrow.png) no-repeat; 
  background-size: cover;
  width: 4vw;height: 2.5vw;
  display: inline-block;
  position: absolute;
  right: 3.5vw;
  bottom: 1.5vw;
  opacity: 0;
}

#title-list .swiper-slide a:hover strong{
  font-size: 3.6vw;
  left:-5vw;
}

#title-list .swiper-slide a:hover span{
  opacity: 1;
  filter: alpha(opacity=100);
}

#title-list a::before {
  content: attr(data-i);
  position: absolute;
  top: -40px;
  left: 30px;
  font-size: 1.4rem;
  font-size: 16px;
  opacity: 0.5;
  filter: alpha(opacity=50);
  height: 20px;
  line-height: 20px;
  color: #fff;
}

#thumbs{
  position: absolute;
  bottom: 9vw;
  z-index: 1;
  height: 3vw;;
  width: 58%;
  padding:0 21%;
}
#thumbs .btn-list {
  position: absolute;
  left:0;
  right:0;
  top:0;
  display: flex;
  justify-content: center;
}
.btn-list:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
  font-size: 0;
  line-height: 0;
}
.btn-list > div {
  float: left;
  opacity: 0.5;
  filter: alpha(opacity=50);
  transition: opacity 0.4s ease;
  height: 29px;
  cursor: pointer;
  margin-right: 50px !important;
  
}
.btn-list > div.btn1{
  background: url(../images/aw1920.png) no-repeat;
  width: 195px;
  background-size: 100% 100%;
}

.btn-list > div.btn2{
  background: url(../images/hot2019.png) no-repeat;
  width:192px;
  background-size: 100% 100%;
}

.btn-list > div.btn3{
  background: url(../images/sh2019.png) no-repeat;
  width: 189px;
  background-size: 100% 100%;
}

.btn-list > div.btn4{
  background: url(../images/showroom.png) no-repeat;
  width: 244px;
  background-size: 100% 100%;
}

.btn-list > div.swiper-slide-thumb-active {
  opacity: 1;
  filter: alpha(opacity=100);
}
.btn-list > div:hover{
  opacity: 1;
  filter: alpha(opacity=100);
}

.switch-btn-box{position: absolute;right: 10%;bottom: 3vw;height: 50px;width: 130px;}
.switch-btn-box>div{
  background: rgba(223,187,150,0.2);
  width: 50px;height: 50px; 
  border-radius: 30px; 
  text-align: center;line-height: 50px;
  top:0;
  transition: all 0.5s ease; 
}
.switch-btn-box .swiper-button-next{
  right: 0;
}
.switch-btn-box .swiper-button-next>i{
  background: url(../images/rg.png) no-repeat 0 15px;
  width: 11px;height: 50px;
  display: inline-block;
}
.switch-btn-box .swiper-button-prev{
  left: 0;
}
.switch-btn-box .swiper-button-prev>i{
  background: url(../images/lf.png) no-repeat 0 15px;
  width: 11px;height: 50px;
  display: inline-block;
}
.switch-btn-box>div:hover{
  top:-5px;
}

.lines-container {
  display: flex;
  justify-content: space-between;
  position: absolute;
  top: 0;
  right: 5%;
  bottom: 0;
  left: 5%;
  z-index: 0;
  height: 100%;
}
.line {
  width: 1px;
  height: 100%;
  background: rgba(255, 255, 255, 0.08);
}

@media screen and (min-width :1500px){
  .btn-list > div{
    margin-right: 10px !important;
  }
  .btn-list > div.btn1{
    background-size: contain;
  }
  
  .btn-list > div.btn2{
    background-size: contain;
  }
  
  .btn-list > div.btn3{
    background-size: contain;
  }
  
  .btn-list > div.btn4{
    background-size: contain;
  }
}



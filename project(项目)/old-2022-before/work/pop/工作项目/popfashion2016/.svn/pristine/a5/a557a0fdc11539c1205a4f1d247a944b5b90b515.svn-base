{block name="self"}
{literal}
<script>
	$(function(){
      var broswer = getBroswer();
      if (broswer.broswer == 'IE' && parseInt(broswer.version) == 9) {
        $.each($('.js-search-section>div'), function(i, v) {
          $(v).addClass('set'+(i+1));
        })
      }

    	//返回顶部
    	$('.js-scrollt').on('click',function(){
    		$('html,body').animate({
    			'scrollTop':'0px'
    		},400);
    	});
    	function getBroswer() {
        var sys = {};
        var ua = navigator.userAgent.toLowerCase();
        var s;
        (s = ua.match(/edge\/([\d.]+)/)) ? sys.edge = s[1] :
        (s = ua.match(/rv:([\d.]+)\) like gecko/)) ? sys.ie = s[1] :
        (s = ua.match(/msie ([\d.]+)/)) ? sys.ie = s[1] :
        (s = ua.match(/firefox\/([\d.]+)/)) ? sys.firefox = s[1] :
        (s = ua.match(/chrome\/([\d.]+)/)) ? sys.chrome = s[1] :
        (s = ua.match(/opera.([\d.]+)/)) ? sys.opera = s[1] :
        (s = ua.match(/version\/([\d.]+).*safari/)) ? sys.safari = s[1] : 0;

        if (sys.edge) return { broswer : "Edge", version : sys.edge };
        if (sys.ie) return { broswer : "IE", version : sys.ie };
        if (sys.firefox) return { broswer : "Firefox", version : sys.firefox };
        if (sys.chrome) return { broswer : "Chrome", version : sys.chrome };
        if (sys.opera) return { broswer : "Opera", version : sys.opera };
        if (sys.safari) return { broswer : "Safari", version : sys.safari };
        
        return { broswer : "", version : "0" };
      }
    	
    	var scroll_timer = null;
    	//根据页面滚动高度执行动画
    	setTimeout(function(){
    		scrollAni();
    	},1000);
    	
    	function scrollAni(){
    		
    		var w_t = $(window).scrollTop(), _i=0;
	    	if(w_t > 200){
				$('.js-scrollt').fadeIn(50);
			}else{
				$('.js-scrollt').fadeOut(50);
			}
			clearTimeout(scroll_timer);
			scroll_timer = setTimeout(function(){
				if(w_t < 400){
					_i = 0;
				}else if(w_t >= 400 && w_t < 1000){
					_i = 1;
				}else if(w_t >= 1000 && w_t < 2500){
					_i = 2;
				}else if(w_t >= 2500 && w_t < 3600){
					_i = 3;
				}else if(w_t >= 3600 && w_t < 4800){
					_i = 4;
				}else if(w_t >=4800 && w_t < 5800){
					_i = 5;
				}else if(w_t >=5800 && w_t < 6800){
					_i = 6;
				}else if(w_t >=6800 && w_t < 8000){
					_i = 7;
				} else {
          _i = 8;
        }
				aniSet(_i);
				
			},240);
			
    	}
    	
    	//执行动画
    	function aniSet(i){
    		i = i||0;
    		var _ele = $('.js-search-section>div').eq(i);    		
    		var ani_class = 'set'+(i+1);
    		_ele.addClass(ani_class);
    		
    		clearTimeout(scroll_timer);
    	}
    	
    	
    	
    	
    	//页面滚动
    	$(window).on('scroll',function(){
    		scrollAni();
    	});

    	// 轮播
    		var index = 0;
			var timer = null;
			var $imgObj = $('.js-image>li');
			var $pointObj = $('.jiaobiao>span');
			var pointObjLen = $pointObj.length;
			$imgObj.eq(0).show();

      timer = setInterval(carousel, 5000);
			// 轮播	
			function carousel() {
				clearInterval(timer);
				index++;
				if (index >= pointObjLen) {
					index = 0;
				}
				$imgObj.eq(index).stop(true,true).fadeIn(800).siblings().stop(true,true).fadeOut(800);
				$pointObj.eq(index).addClass('on').siblings().removeClass('on');
        timer = setInterval(carousel, 5000);
			}
			
			function leftcarousel() {
				clearInterval(timer);
				if (index == 0) {
					index = pointObjLen;
				}
				index--;
				$imgObj.eq(index).stop(true,true).fadeIn(800).siblings().stop(true,true).fadeOut(800);
				$pointObj.eq(index).addClass('on').siblings().removeClass('on');
        timer = setInterval(carousel, 5000);
			}

			$(".js-l-btn").on("click", function(){
				leftcarousel();
			});
			$(".js-r-btn").on("click", function(){
				carousel();
			});
			
			$pointObj.on('mouseenter mouseleave', function(e){
				clearInterval(timer);
				handerCarousel(this, e);
			});
      $imgObj.hover(function() {
        clearInterval(timer);
      }, function() {
        timer = setInterval(carousel, 5000);
      })

			function handerCarousel(obj, e) {
				var e = e || window.event;
				if (e.type == 'mouseenter') {
					clearInterval(timer);
					index = $(obj).index();
					$imgObj.eq(index).stop(true,true).fadeIn(800).siblings().stop(true,true).fadeOut(800);
					$pointObj.eq(index).addClass('on').siblings().removeClass('on');
				} else if (e.type == 'mouseleave') {
					timer = setInterval(carousel, 5000);
				}
			}

			// 滚动定位
			function sideBarLeft(){
				var sidebar_nav = $(".js-nav-scroll");
				var lis = $(".js-nav-scroll li.lis");
				var indexdiv = $(".indexdiv");
				var offsetTop = $(".js-scroll-top").offset().top;
	    		lis.on("click", function() {
	    			var index = $(this).index();
	    			var divhei = indexdiv.eq(index).offset().top;
	    			$("html,body").animate({scrollTop:divhei},300);
	    			$(this).addClass("cur").siblings().removeClass("cur");
	    		});
	    		if($(window).width()<=1200){
                    sidebar_nav.css({'position':'absolute','top':offsetTop +70,'left':0,'margin-left':0});
                }else{
                    sidebar_nav.css({'position':'absolute','top':offsetTop +70,'margin-left':"-680px"});
                }  
	    		$(window).scroll(function(){
		            var scrollTop = $(window).scrollTop();
		            if(scrollTop >=offsetTop-40){
		                if($(window).width()<=1200){
		                    sidebar_nav.css({'position':'fixed','top':'40px','left':0,'margin-left':0});
		                }else{
		                    sidebar_nav.css({'position':'fixed','top':'40px','margin-left':"-680px"});
		                } 
		            }else{
		                if($(window).width()<=1200){
		                    sidebar_nav.css({'position':'absolute','top':offsetTop +70,'left':0,'margin-left':0});
		                }else{
		                    sidebar_nav.css({'position':'absolute','top':offsetTop +70,'margin-left':"-680px"});
		                }    
		            }
		            for(var i=0;i<indexdiv.length;i++){
		                var divHei =indexdiv.eq(i).offset().top;
		                if(scrollTop > divHei-40){
		                    lis.eq(i).addClass('cur').siblings('li').removeClass('cur');
		                }else{
		                    lis.eq(i).removeClass('cur')
		                }
		            }
		        });
			}
			sideBarLeft();
			$(window).resize(function() {
				sideBarLeft();
			});
    });

</script>
{/literal}

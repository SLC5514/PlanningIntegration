{extends file="layout.html"}
{block name="header"}
{/block}
{block name="selfcss"}
    <link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/pop_layer.css?{STATIC_CHANGE_TIME}">
    <link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/new-detail/tbook-detail.css?{STATIC_CHANGE_TIME}">
	<link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/perfect-scrollbar.css?{STATIC_CHANGE_TIME}">
    <link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/lists/video-list-inside.css?{STATIC_CHANGE_TIME}">
{/block}
{block name="other"}
<div id="loadingBg"><div class="noPic"></div></div>
{/block}
{block name="main"}
<input id="encrypt" type="hidden" data-sign="{$sign}" data-timestamp="{$timeStamp}" data-token="{$token}">
<input type="hidden" id="param" data-url="/collect/setcollect/" data-col="{$columnId}" data-bra="{$data.brand_tid}" data-reg=""  data-id="{$id}" data-t="{$ct}">
<div class="video-detail-section">
    <div class="video-detail-header clearfix">
        <div class="web-logo js-web-logo fl">
            <a href="/" target="_blank">
                <img src="{STATIC_URL2}/global/images/lists/video_list/pop_logo.png"/>
            </a>
        </div>
        {if $columnId == 71}
        {if $data.brand_tid}
        <div class="video-seleted-labels-section fl">
            <a class="brand-selected-labels js-brand-selected" data-bra="{$data.brand_tid}">{$data.brand_name}</a>
            <span class="split-line"></span>
            <div class="season-seleted-labels-box js-swiper-season-main">
                <div class="season-seleted-labels-content js-season-seleted-labels js-swiper-child">
                    {if !empty($filterData.iSeason)}
                    {foreach $filterData.iSeason as $id => $item}
                        <a {if $id == $filter.sea} class="select-on" {/if} data-sea="{$id}">{$item}</a>
                    {/foreach}
                    {/if}
                </div>
                <a class="labels-switch labels-switch-prev js-switch-btn js-swiper-switch-prev"></a>
                <a class="labels-switch labels-switch-next js-switch-btn js-swiper-switch-next"></a>
                <div class="b-nav-mask"></div>
            </div>
        </div>
        {/if}
        {/if}
    </div>
    <div class="video-detail-infor {if empty($data.detail) && empty($videoData.video_id)}video-detail-infor-blank{/if}">
        <div class="con_width clearfix">
            <div class="video-detail-infor-lf fl">
                <div class="video-detail-infor-main">
                    <h1>{$theme}</h1>
                    <p class="video-labels">
                        {if $data.seasonName}
                          <span>{$data.seasonName}</span>
                        {/if}
                        {if $data.regionName}
                        <span><em></em>{$data.regionName}</span>
                        {/if}
                        {if $data.brand_name}
                        <em></em><span>{$data.brand_name}</span>
                        {/if}
                        {if $data.iCategoryName}
                        <span><em></em>{$data.iCategoryName}</span>
                        {/if}
                    </p>
                    {if $P_UserType!=4}
                    {if $bIsCollect == 1}
                    <a class="collect-status" id="collect" href="javascript:void(0);" data-iscollected="{$bIsCollect}" data-col="{$columnId}" data-tab="{$ct}" data-id="{$mainID}" data-type="8" data-para="mid={$mainID}&mtab={$mtab}" title="已收藏"><i></i><span>已收藏</span></a>
                    {else}
                    <a class="collect-status nzt" id="collect" href="javascript:void(0);" data-iscollected="{$bIsCollect}" data-col="{$columnId}" data-tab="{$ct}" data-id="{$mainID}" data-type="8" data-para="mid={$mainID}&mtab={$mtab}" title="收藏"><i></i><span>收藏</span></a>
                    {/if}
                    {/if}


                    {if !empty($data.detail)}
                    <!--广告大片有描述-->
                    <div class="insprition-intro">
                        {$data.detail}
                    </div>
                    <!--/广告大片有描述-->
                    {/if}
                </div>
            </div>
            {if !empty($videoData.video_id)}
            <div class="video-detail-infor-rg fr">
                <div class="video-detail-switch-main">
                    <div class="switch-video-btn js-switch-video-btn">
                        <a data-videoId="{$videoData.video_id}" style="display: none;" class="v-on">发布会视频</a>
                    </div>
                    <div class="play-video-box">
                        <div class="play-video-img">
                            <div class="close-video-positiion"></div>
                            <div id="runwayPlayer"></div>
                        </div>
                    </div>
                </div>
            </div>
            {/if}
        </div>
    </div>
    {if count($insideAllData) > 1}
    <div class="video-detail-more-section">
        <div class="contentW">
            <div class="video-detail-more-box js-swiper-runway-main">
                <div class="video-detail-lists-box js-swiper-child">
                    {foreach $insideAllData as $item}
                    <a href="{$item.inside_url}"  {if $item.id == $filter.id} class="select-on" {/if}>{$item.title}</a>
                    {/foreach}
                </div>
                <a class="detail-more-switch detail-more-prev js-switch-btn js-swiper-switch-prev"></a>
                <a class="detail-more-switch detail-more-next js-switch-btn js-swiper-switch-next"></a>
                <div class="nav-mask"></div>
            </div>
        </div>
    </div>
    {/if}
    <div class="video-lists-section">
        <div class="contentW">
            <div class="neiye-infor-bottom">
                <div class="listNav fl">
                    <a href="javascript:void(0);" class="on" title="Lookbook列表">Lookbook列表</a>
                </div>
                <div class="list-down-page fr">
                    <div class="page-box fl clearfix" id="s_result">
                        <p class="findstyle">共找到 <a title="{$totalCount}">{$totalCount}</a>页合辑图片</p>
                        <!--分页--页码开始-->
                        {$simplePageHtml}
                        <!--分页--页码结束-->
                    </div>
                    <span class="fl"></span>
                    {if $sBuyingLinks}
                    <a class="buy_fr fl" href="{$sBuyingLinks}" target="_blank" title="立即购买">立即购买</a>
                    {/if}
                    {if $downLoadUrl neq ''}
                    <a class="download fl" href="{$downLoadUrl}" title="立即打包下载">立即打包下载</a>
                    {else}
                    <a class="download no-download fl" href="javascript:void(0);" title="暂不支持打包下载">暂不支持打包下载</a>
                    {/if}

                </div>
            </div>
            <div class="video-lists-content runways_neiye" id="main">
                <ul class="picbox lazyload">
                    {foreach $list as $key => $val}
                    <li>
                        <a class="img_click" href="/details/bookpresscon/t_{$val.realName}-id_{$val.id}-col_{$val.columnId}-index_{$val.index}/" data-total="{if $viewMode eq 'fastView'} 3 {else} {$totalCount} {/if}" data-index="{$val.index}" data-t="{$val.realName}" data-id="{if isset($val.id)}{$val.id}{else}{$id}{/if}" data-col="{$val.columnId}" data-bp="/" data-sp="{$val.smallPicPath}" title="查看详情">
                            {if $viewMode eq 'fastView' && $page eq 1 && $key gt 2}
                            <div {if $shadeFlag} class="shuiy" {/if}></div>
                            {/if}
                            {if !empty($val.smallPicPath)}
                            <img  src="{STATIC_URL2}/global/images/loading/reference150-150.gif" data-original="{$val.smallPicPath}" alt="pic">
                            {else}
                            <img src="{STATIC_URL1}/global/images/loading/referenceMR150.jpg" alt="pic">
                            {/if}
                        </a>
                        <!--<p>待定义</p>-->
                    </li>
                    {/foreach}
                </ul>
                <div class="rightc_bottom">
                    <!--分页--页码开始-->
                    {$pageHtml}
                    <!--分页--页码结束-->
                </div>
            </div>
        </div>
    </div>
</div>
<div  class="pop_layer_page pop_layer_page1" id="pop_layer">
    {include file="bookpresscon_layer_template.html"}
</div>
{/block}
{block name="self"}
<script type="text/javascript" src="https://player.youku.com/jsapi"></script>
<script type="text/javascript" src="{STATIC_URL2}/global/js/lib/swiper/swiper.min.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL3}/global/js/secret/crypto-js/crypto-js.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL3}/global/js/secret/jsencrypt.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL2}/global/js/fashion/index.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL2}/global/js/fashion/global.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL2}/global/js/fashion/bookl.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL3}/global/js/common/template.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL2}/global/js/common/imagesloaded.pkgd.min.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL2}/global/js/fashion/aggregation.js?{STATIC_CHANGE_TIME}"></script>
<script type="text/javascript" src="{STATIC_URL3}/global/js/fashion/util.collect.js?{STATIC_CHANGE_TIME}"></script>

.home-nav-list{float: left;margin-bottom: 150px}
.home-nav-list>li{float: left;position: relative;margin-right: 20px;text-align: center;}
.home-nav-list>li>a{cursor: pointer;display: block;width: 96px;padding: 10px 0 30px;font-size: 16px;border: 1px solid rgba(255,255,255,0.5);color: #fff;background-image: url(../../images/home/icon3.png);background-repeat: no-repeat;background-position: 65px 34px;}
.home-nav-list>li.now-choice>a{color: #d8b056;border-color: #d8b056;background-image: url(../../images/home/icon2.png);background-position: 65px 41px}

.home-nav-list>li>a.home-nav-two{background-position: 55px 34px;}
.home-nav-list>li.now-choice>a.home-nav-two{background-position: 55px 41px;}


.home-nav-list>li>span{cursor: default;display: block;width: 96px;padding: 10px 0 30px;font-size: 16px;border: 1px solid rgba(255,255,255,0.5);color: #fff;background-image: url(../../images/home/icon3.png);background-repeat: no-repeat;background-position: 65px 34px;}
.home-nav-list>li.now-choice>span{color: #d8b056;border-color: #d8b056;background-image: url(../../images/home/icon2.png);background-position: 65px 41px}
/*.home-nav-list>li>span:hover +ul{display: block;}*/
.home-nav-list>li>ul{z-index: 10;position: absolute;left: 0;top: 70px;width: 100%;display: none;color: #fff;}
.home-nav-list>li>ul>li{font-size: 14px;margin-bottom: 1px;height: 36px;line-height: 36px;background-color: #d8b056;cursor: pointer;}
.home-nav-list>li>ul>li>a{display: block;color: #fff;}
.home-nav-list>li>ul>li:hover{background-color: #aa7c12}
.nav-btn{display: inline-block;width: 120px;padding: 8px 15px 13px 50px;font-size: 16px;text-align: right;color: #fff;background-image: url(../../images/home/icon4.png);background-repeat: no-repeat;background-position: 10px center;background-color: #d8b056;
	transform:translateX(180px);
	transition:transform 0.8s;
}


.canvas-div{height: 400px;position: relative;}
.canvas-div>canvas{width: 100%;height: 100%;display: block;}
#line-canvas{z-index: 2;position: absolute;left: 0;top: 0;width: 100%;}
.wordcloud{z-index: 5;position: absolute;left: 50%;top: 10%;width: 1000px;height: 80%;margin-left: -600px;}
.wordcloud>a{z-index: 10;position: absolute;display: inline-block;letter-spacing: 0;line-height: 1em;cursor: pointer;
	transition: top 0.5s;
}
.wordcloud>a:hover{color: #d8b056 !important;filter: alpha(opacity=100)!important;}

.tab-div{width: 533px;height: 457px;margin-bottom: 120px;position: relative;float: left;overflow: hidden;}
.tab-div>ul>li{height: 100%;background-color: rgba(255,255,255,0.5);}
.tab-div>ul>li>a{position: relative;display: block;width: 100%;height: 100%;}
.tab-div>ul>li>a>img{width: 100%;height: 100%;
	transition:all 0.5s;
}
.tab-div>ul>li>a>div{z-index: 10;position: absolute;left: 0;top: 0;width: 100%;height: 100%;background-color: #d8b056;opacity: 0;filter: alpha(opacity=0);
	transition:all 0.4s;
}
.tab-div>ul>li>a>p{z-index: 12;position: absolute;left: 0;top: 50%;width: 100%;margin-top: -20px;font-size: 16px;line-height: 30px;text-align: center;color: #fff;opacity: 0;filter: alpha(opacity=0);
	transition:all 0.4s;
}
/*.tab-div>ul>li>a:hover>div{opacity: 0.9;filter: alpha(opacity=90)}*/
/*.tab-div>ul>li>a:hover>p{opacity: 1;filter: alpha(opacity=100)}*/
.tab-div>ul>li>a:hover>img{transform:scale(1.05,1.05);}

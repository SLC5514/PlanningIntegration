.container{width: 100%;height:100%;}
.banner{width: 100%;height:600px;background: url(../images/bg.jpg) no-repeat center center;position: relative;z-index:1;}

.banner .bannerCenter{position:relative;width: 1006px;height:280px;margin:0 auto;padding-top:356px;background: url(../images/link.png) no-repeat center center;}
.banner .bannerCenter .Theme{display: inline-block;width: 170px;height: 51px;background: #000;filter: alpha(opacity=0);position: absolute;top:404px;opacity: 0;}
.banner .bannerCenter .colorTheme{left:311px;}
.banner .bannerCenter .pciTheme{right:297px;}

.content{width: 100%;height: 700px;}
.content1{background: url(../images/content1.jpg) no-repeat center center;}
.content .con_box{width: 1200px;margin:0 auto;position: relative;height:700px;}
.content1 .box{overflow: hidden;width: 390px;position: absolute;top:218px;left:40px;}
.content1 .con_box{background: url(../images/box1.jpg) no-repeat center center;}
.content2 .con_box{background: url(../images/box2.jpg) no-repeat center center;}
.content .box .smallBox{width: 392px;font-family: "微软雅黑";font-size: 14px;padding-top:30px;color:#fff;font-weight: 100;}
.content1 .box .smallBox{color:#000;padding-bottom: 49px;}
.content2 .box .smallBox{padding-bottom: 44px;}
.fl{float:left;}
.fr{float:right;}
.content .box dt{margin-bottom: 5px;text-align: center;}
.content .box dl{display: block;margin-bottom: 10px;}
.content .box dl a{font-family: "微软雅黑";font-size: 16px;color:#000;width:114px;height:30px;line-height:30px;text-align:center;background: #fff;display: inline-block;}
.content2 .box dl a{background: #000;color:#fff;width: 150px;height: 30px;}
.content .box dl a:hover{background: #dd7633;color:#fff;}
.content2{background: url(../images/content2.jpg) no-repeat center center;}
.content2 .box{overflow: hidden;width: 390px;position: absolute;top:176px;right:40px;}
.content1 .box dl.dl01{margin-left:34px;}
.content2 .box dl.dl01{margin-left:17px;}
.content1 .box dl.dl02{margin-right:53px;}
.content2 .box dl.dl02{margin-right:29px;}
.p1>.ani3>a>span{display: inline-block;color: #fff;*display: inline;*zoom: 1;width: 26px;height: 26px;background: url(../images/icon.png?2019070904) -44px -28px no-repeat;margin-bottom: 4px;}

.p1>.ani3>div{color: #fff;}
.p1>.ani3>div>.text1{font-size: 26px;line-height: 28px;letter-spacing: 1px;}
.p1>.ani3>div>.text2{color: #fff;font-size: 24px;line-height: 50px;font-weight: bold;margin-top: 8px;width:338px;height:50px;background:url(../images/telbg.png?2019070904) no-repeat;font-family:"Arial";margin-left:15px;margin-right: 15px;}
.p1>.ani3>div>.text3{font-size: 27px;margin-top: 2px;letter-spacing: 3px;}

.p1>.ani3>img{width: 120px;height: 120px;}


/*动画*/
.set1>h2{opacity: 1;top: 0px;}
.set1>.ani1,.set1>.ani2{left: 0px;opacity: 1;}
.set1>.ani3{top: 0px;opacity: 1;}


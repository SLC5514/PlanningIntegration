{
  "name": "pop-client",
  "productName": "POP客户端",
  "version": "2.0.3",
  "description": "POP客户端",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make64": "electron-forge make --platform=win32 --arch=x64",
    "make32": "electron-forge make --platform=win32 --arch=ia32",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "POP"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "POP客户端",
        "appBundleId": "popClientAppId",
        "appCopyright": "Copyright© 2004-至今 逸尚创展（上海）科技有限公司.",
        "icon": "./favicon",
        "platform": [
          "win32",
          "darwin"
        ],
        "ignore": [
          ".gitignore",
          "yarn.lock",
          "README.md",
          "pic.png",
          "backup"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "platforms": [
            "win32"
          ],
          "config": {
            "/* certificateFile */": "./POP.pfx",
            "/* certificatePassword */": 123456
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "icon": "./favicon.icns",
            "background": "",
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-zip"
        }
      ]
    }
  },
  "dependencies": {
    "ajax": "^0.0.4",
    "electron-squirrel-startup": "^1.0.0",
    "getmac": "^5.20.0",
    "iconv-lite": "^0.6.3",
    "jquery": "^3.6.0",
    "lowdb": "1.0.0",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-dmg": "^6.0.0-beta.61",
    "@electron-forge/maker-pkg": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-snap": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "15.3.1"
  }
}

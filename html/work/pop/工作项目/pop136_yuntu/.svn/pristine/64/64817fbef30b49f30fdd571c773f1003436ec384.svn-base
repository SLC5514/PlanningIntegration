.report-video-section>.close{position: absolute;top: 0;right: -38px;width: 38px;height: 38px;cursor: pointer;background: url(../../images/report/report-video-close.png) no-repeat center;background-size: cover;}
.report-video-section>div{width: 100%;height: 100%;}

/* 侧栏导航 */
.nav-right{position: fixed; z-index: 96;top: 0;right: 0;display: none;background: #1D252D;width: 299px;height: 100%;}
.nav-right>.right-cont{/*background: #f9f9f9;*/height: 100%; overflow-x: hidden;overflow-y: auto;*position: relative;}
/* .nav-right>.right-cont>div{background: #fff;} */
.nav-right>.nav-control{transition: 0.2s;width: 24px;left: -24px; cursor: pointer; height: 36px; position: absolute;z-index: 99;top: 50%;background: url(../../images/report/nav-control1.png) no-repeat center;}
.nav-right>.nav-control:hover{width: 55px;left: -55px;background: url(../../images/report/nav-shou.png) no-repeat center; }

/*右边导航*/
.nav-right>.right-cont{/*background: #f9f9f9;*/height: 100%; overflow-x: hidden;overflow-y: auto;*position: relative;}
/* .nav-right>.right-cont>div{background: #fff;} */
.nav-right>.nav-control{transition: 0.2s;width: 24px;left: -24px; cursor: pointer; height: 36px; position: absolute;z-index: 99;top: 50%;background: url(../../images/report/nav-control1.png) no-repeat center;}
.nav-right>.nav-control:hover{width: 55px;left: -55px;background: url(../../images/report/nav-shou.png) no-repeat center; }

.right-top{padding:24px 0 24px 0;}
.right-top+.right-top{border-top:1px solid #12181F;padding-left: 19px;}
.right-top>h3{line-height: 20px;font-size: 16px;font-weight: bold; color: #fff;padding: 0 19px 8px 0;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;}
.right-top>.report-msg{cursor: pointer;word-break:break-all;/*text-align: justify;*/position: relative;left: 0;top: 0; font-size: 14px;line-height: 18px;color: #999;padding-right: 19px;}
.right-top>.cursor-words{cursor: text;}
.right-top>.report-msg>div>span{color: #d8b056;}
.right-top>.report-msg>.report-messages{display: none; position: absolute;z-index: 100;font-size: 14px;color: #999;background: #1D252D;border-radius: 3px;left: -10px;top: -8px;width: 300px;padding: 8px 10px;line-height: 18px;}
.right-top>.msg-i{font-size: 14px;color: #333;height: 20px;line-height: 20px;}
.right-top>.msg-i>i{float: left;width: 20px;height: 20px;margin:4px 15px 0 -18px;}
.right-top>.msg-i>p{float: left;height: 100%}
.right-top>.msg-i1>i{background: url(../../images/report/report-icons.png) no-repeat -83px -10px;}
.right-top>.msg-i2>i{background: url(../../images/report/report-icons.png) no-repeat -84px -35px;}
.right-top>.msg-i3>i{background: url(../../images/report/report-icons.png) no-repeat -81px -64px;}
.right-top>.msg-i4>i{background: url(../../images/report/icon_labels.png) no-repeat center / 100%;width: 13px;height: 16px;}
.right-top>.msg-i4>ul{width: 242px;}
.right-top>.msg-i4{height: auto;width: 280px;}
.right-top>.msg-i4 li{float: left;height: 24px;line-height: 24px;margin: 0 10px 9px 0; text-align: center;}
.right-top>.msg-i4 li>a{font-size: 12px; white-space: nowrap;display: block;height: 100%; padding: 0 10px;border-radius: 12px;color: #fff;background: #7D8185;}
.right-top>.msg-i4 li>a:hover{background: #d8b056;}

.see-nav{padding-top:1px;/*border-bottom:1px solid #eee;*/border-top:1px solid #12181F;font-size: 14px;color: #999;line-height: 30px;}
.see-nav>.see-title{font-weight: bold;padding: 9px 19px;color: #fff;}
.see-nav>.see-title>i{display: inline-block;*display: inline;*zoom: 1;vertical-align: middle;width: 14px;height: 16px; margin: 0 15px 0 -18px;background: url(../../images/report/icon_mulu.png) no-repeat center / 100%;}
.see-nav>ul{padding-bottom: 1px;}
.see-nav>ul>li{margin-bottom: 8px;}
.see-nav>ul>li>a{color: #999; display: block;padding: 7px 0 7px 0;/*height: 100%;*/cursor: pointer;}
.see-nav>ul>li>a:hover{color: #ab7b10;}
.see-nav>ul>li>a>i{float: left;width: 20px;height: 20px;margin: 5px 4px 0 -4px;background: url(../../images/report/report-icons2.png) no-repeat -62px -287px;}
.see-nav>ul>li>a:hover>i{background: #eee;background: url(../../images/report/report-icons2.png) no-repeat -90px -287px;}
.see-nav>ul>li>a>span{float: left;width: 228px;line-height: 20px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.see-nav>ul>.this-item>a>i{background: #eee;background: url(../../images/report/report-icons2.png) no-repeat -90px -287px;}
.see-nav>ul>.this-item>a{color: #ab7b10;}

.cntsub-null{padding-bottom: 1px;}
.cntsub-null>div{color: #333; display: block;padding: 7px 0 7px 19px;margin-bottom: 8px;}
.cntsub-null>div>i{float: left;width: 20px;height: 20px;margin: 0 4px 0 -4px;background: url(../../images/report/report-icons.png) no-repeat -62px -287px;}
.cntsub-null>div>span{float: left;width: 228px;line-height: 20px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}

.shop-serve{padding: 1px 0px;font-size: 14px;color: #333;line-height: 30px;}
.shop-serve>.serve-title{font-weight: bold;padding: 9px 19px;}
.shop-serve>.serve-title>i{display: inline-block;*display: inline;*zoom: 1;vertical-align: middle;width: 20px;height: 20px;margin-right: 4px;background: url(../../images/report/report-icons.png) no-repeat -145px -11px;}
.shop-serve>ul>li>a{display: block; border-bottom:1px solid #eee;padding: 9px 19px;color: #333;}
.shop-serve>ul>li>a:hover{background: #eee;}
.shop-serve>ul>li>a>.shop-serve-msg>div{float: left;}
.shop-serve>ul>li>a>.shop-serve-msg>.shop-img{width: 48px;height: 48px;}
.shop-serve>ul>li>a>.shop-serve-msg>.shop-img>img{display: block; width: 100%;height: 100%;}
.shop-serve>ul>li>a>.shop-serve-msg>.shop-title{width: 186px;padding-left: 8px; line-height: 20px;font-size: 14px;color: #333;}
.shop-serve>ul>li>a>.shop-serve-msg>.shop-title>ul{padding-top: 5px;display: none;}
.shop-serve>ul>li>a>.shop-serve-msg>.shop-title>.disc-show{display: block;}

.shop-serve>ul>li>a>.shop-serve-msg>.shop-title>ul>li{height: 20px;line-height: 20px;color: #999;font-size: 12px;}
.shop-serve>ul>li>a>.shop-serve-msg>.shop-title>ul>li>i{display: inline-block;*display: inline;*zoom: 1;vertical-align: middle;width: 20px;height: 20px;margin-right: 4px;}
.shop-serve>ul>li>a>.shop-serve-msg>.shop-title>ul>.shop-detail1>i{background: url(../../images/report/report-icons.png) no-repeat -80px -132px;}
.shop-serve>ul>li>a>.shop-serve-msg>.shop-title>ul>.shop-detail2>i{background: url(../../images/report/report-icons.png) no-repeat -80px -151px;}
.shop-serve>ul>li>a>.shop-serve-msg>.shop-title>ul>.shop-detail3>i{background: url(../../images/report/report-icons.png) no-repeat -78px -171px;}

/*小屏幕右边导航*/
.nav-right-min{display: none; position: fixed; z-index: 97;top: 50%; right: 0;margin-top: -190px; background: #1D252D;width: 142px;height: 376px;border:1px solid #1D252D;border-right: 0 none;}
.right-cont-min{height: 100%; overflow-x: hidden;overflow-y: auto;}
.right-cont-min .see-nav>ul>li>a{padding:  5px 0px 5px 13px;}
.right-cont-min .see-nav>ul>li>a>p{float: left;overflow: hidden;word-break:break-all; width: 86px;line-height: 20px;max-height: 40px;}
.right-cont-min .see-nav>ul>li>a>i{margin-top: 0;}
.nav-right-min>.nav-control{transition: 0.2s;width: 24px;left: -24px;cursor: pointer; height: 36px; position: absolute;z-index: 100;top: 50%;background: url(../../images/report/nav-control3.png) no-repeat center;}
.nav-right-min>.nav-control:hover{ width: 55px;left: -55px;background: url(../../images/report/nav-control2.png) no-repeat center; }
.nav-right-min>.nav-control>.nav-control-disc{cursor: default; display: none; text-align: center; position: absolute;right: 48px;top: -80px; z-index: 101;width: 221px;height: 196px;background: url(../../images/report/control-disc.png) no-repeat center;}
.nav-right-min>.nav-control>.nav-control-disc>i{margin: 46px 0 20px; display: inline-block;*display: inline;*zoom: 1;width: 18px;height: 26px;background: url(../../images/report/report-icons.png) no-repeat -115px -36px;}
.nav-right-min>.nav-control>.nav-control-disc>p{font-size: 12px;color: #000;}
.nav-right-min>.nav-control>.nav-control-disc>div{cursor: pointer; font-size: 14px; width: 56px;color: #d8b056;line-height: 22px;cursor: pointer;margin: 23px auto 0;}

/* 报告成册 */
.nav-right{top: 60px;bottom: 0;box-sizing: border-box;width: 380px;height: auto;padding: 60px 32px 0;}
.nav-right>.right-cont{width: 100%;height: 94%;box-sizing: border-box;}
.right-tab{max-height: 50px;overflow: hidden;margin-bottom: 14px;user-select: none;-moz-user-select: none;-webkit-user-select: none;-ms-user-select: none;}
.right-tab>li{width: 50%;height:36px;line-height: 36px;text-align: center;font-size:14px;color: #fff;box-sizing: border-box;cursor: pointer;}
.right-tab>li>i{display: inline-block;margin-right: 4px;vertical-align: middle;position: relative;top: -1px;}
.right-tab>li.right-tab-book>i{background: url(../../images/report/tab-book.png) no-repeat;background-size: cover;width: 16px;height: 15px;}
.right-tab>li.right-tab-text>i{background: url(../../images/report/tab-text.png) no-repeat;background-size: 100%;width: 16px;height: 16px;}
.right-tab>li.active{line-height: 32px;border-top: 2px solid #333;}
.right-tab.nobook{margin-bottom: 0;}
.right-tab.nobook>li{width: 100%;height: 50px;line-height: 50px;border-bottom: 1px solid #12181F;text-align: left;border-top: 2px solid #fff;}

/* 小屏 */
.nav-right-min .right-cont-min{height: 87%;}
.nav-right-min .see-nav{border-top: 0;}
.nav-right-min .see-nav>.see-title>i{margin-left: -9px;}
.nav-right-min .right-tab{max-height: 36px;}
.nav-right-min .right-tab>li{width: 36px;}
.nav-right-min .right-tab>li>i{margin-right: 0;}
.nav-right-min .right-tab>li>span{display: block;}
.nav-right-min .right-tab>li.active{width: 106px;}
.nav-right-min .right-tab>li.active>i{margin-right: 5px;margin-top: 4px;}
.nav-right-min .right-tab>li.active>span{display: inline-block;}
.nav-right-min .right-tab.nobook{margin-bottom: 14px;}
.nav-right-min .right-tab.nobook>li{width: 100%;height:36px;line-height:32px;border-bottom: 0;text-align: center;padding-left: 0;border-top: 2px solid #000;}
.nav-right-min .right-tab.nobook>li>i{margin-right: 5px;margin-top: 4px;}
.nav-right-min .right-tab.nobook>li>span{display: inline-block;}

/* 1350屏幕微调 */
@media screen and (max-width:1350px){
	.nav-right-min{width:110px;}
    .right-cont-min .see-nav>ul>li>a{padding: 5px 0px 5px 8px;}
    /* 报告成册 */
    .nav-right-min .right-tab{max-height: 30px;margin-bottom: 10px;}
    .nav-right-min .right-tab.nobook{margin-bottom: 10px;}
    .nav-right-min .right-tab>li{width: 30px;height: 30px;line-height: 30px;}
    .nav-right-min .right-tab>li.active{width: 80px;white-space: nowrap;font-size: 12px;}
    .nav-right-min .right-tab>li.active>i{margin-right: 3px;}
    .nav-right-min .right-tab.nobook>li{width: 100%;height: 30px;line-height: 30px;}
    .nav-right-min .right-tab>li.right-tab-book>i{width: 16px;height: 15px;}
    .nav-right-min .right-tab>li.right-tab-text>i{width: 14px;height: 16px;}
    .nav-right-min .right-cont-min .see-nav>ul>li>a>p{width: 64px;font-size: 12px;}
    .see-nav>.see-title{font-size: 12px;}
}

@media screen and (min-width:1500px){
    /* 报告视频 */
    .report-video-section{width: 1100px;height: 620px;margin-left: -550px;margin-top: -310px;}
    .report-video-section>.close{width: 60px;height: 60px;right: -60px;}
}
.advertisement-small>a{float: left;position: relative;display: block;width: 278px;height: 214px;margin-left: 30px;margin-bottom: 29px;text-align: center;overflow: hidden;}
.advertisement-small>a>img{display: block;width: 100%;height: 100%;background-color: rgba(255,255,255,0.5);
	transition:all 0.5s;
}
.advertisement-small>a>div{z-index: 10;position: absolute;left: 0;top: 0;width: 100%;height: 100%;background-color: #d8b056;opacity: 0;filter: alpha(opacity=0);
	transition:all 0.4s;
}
.advertisement-small>a>p{z-index: 12;position: absolute;left: 5%;top: 50%;width: 90%;font-size: 14px;line-height: 24px;color: #fff;opacity: 0;filter: alpha(opacity=0);
	/*transition:all;*/
}


.advertisement-small>a:hover>div{opacity: 0.9;filter: alpha(opacity=90)}
.advertisement-small>a:hover>p{opacity: 1;filter: alpha(opacity=100)}
.advertisement-small>a:hover>img{transform:scale(1.1,1.1);}



                <div class="trend_box box">
                    {if !empty($lists.1.data)}
                    <div class="new_trend clearfix" style="padding-top:0;">
                        {foreach $lists.1.data as $item}
                            {if $item.sign == 1}
                            <!--主报告-->
                            <div class="trend_item js-analysis-main">
                                <!--分数-->
                                <a href="{$item.detailUrl}" data-score="{$item.score}" target="_blank" class="trend_bg">
                                    <img src="{$item.cover}">
                                </a>
                                <div class="trend_box">
                                    <div class="trend_text">
                                        <a href="{$item.detailUrl}" target="_blank" class="clearfix">
                                            <p class="ay-time clearfix">
                                                <span class="fl">{$item.publish_time|date_format:'%Y-%m-%d'}</span>
                                                <span class="fr">浏览（{$item.iViewCount}）</span>
                                            </p>
                                            <p class="ay-title">{$item.major_title}</p>
                                        </a>
                                        <p class="ay-img-description js-ay-img-description">{$item.major_desc}</p>

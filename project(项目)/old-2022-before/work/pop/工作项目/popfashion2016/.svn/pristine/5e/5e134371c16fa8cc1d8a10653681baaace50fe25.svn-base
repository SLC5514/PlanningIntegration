    .section2>.con-lf>ul.big-ul>li>p{font-size:0.6rem;margin-top:0.75rem;}

    /* content2 */
    .section-bottom{background:none;height:auto;}
    .section3{width:94%;padding:2rem 3% 3rem;background:url(../images/bg3-h5.jpg) no-repeat;height:auto;background-size:100%;}

{extends file="layout.html"}

{block name="selfcss"}
<link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/pop_layer.css?{STATIC_CHANGE_TIME}">
<link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/per.css?{STATIC_CHANGE_TIME}">
<script type="text/javascript" src="{STATIC_URL1}/global/js/common/template.js"></script>
{/block}
<!-- header start -->
{block name="header"}

{/block}
<!-- header end -->
{block name="main"}
<div class="cont_search">
    <div class="division"></div>
    {include file="../common/search_head.html"}
    <div class="search-main">
        <div class="search_head js-search_head clearfix">
            <a data-name="col" data-id="all" data-num="{$allTotal.all}" class="head_item js-jump {if $iColumnPid=='all'}action{/if}">全部<span>{$allTotal.all}</span></a>
            <a data-name="col" data-id="4" data-num="{$allTotal.4}" class="head_item js-jump {if $iColumnPid=='4'}action{/if}">款式<span>{$allTotal.4}</span></a>
            <a data-name="col" data-id="1" data-num="{$allTotal.1}" class="head_item js-jump {if $iColumnPid=='1'}action{/if}">趋势解读<span>{$allTotal.1}</span></a>
            <a data-name="col" data-id="2" data-num="{$allTotal.2}" class="head_item js-jump {if $iColumnPid=='2'}action{/if}">流行分析<span>{$allTotal.2}</span></a>
            <a data-name="col" data-id="82" data-num="{$allTotal.82}" class="head_item js-jump {if $iColumnPid=='82' || $iColumnPid=='120'}action{/if}">图案<span>{$allTotal.82}</span></a>
            <a data-name="col" data-id="3" data-num="{$allTotal.3}" class="head_item js-jump {if $iColumnPid=='3'}action{/if}">T台<span>{$allTotal.3}</span></a>
            <a data-name="col" data-id="71" data-num="{$allTotal.71}" class="head_item js-jump {if $iColumnPid=='71'}action{/if}">广告大片<span>{$allTotal.71}</span></a>
        </div>
        <div class="select_items_box js_select_items_box clearfix">
            <span class="aspan_load">
                <img src="{STATIC_URL2}/global/images/5-121204193934-50.gif">
                    &nbsp筛选项加载中，请稍等...
            </span>
        </div>
    </div>
    {if $spellcheck}
        <div class="con_width search-tips js-search-tips">你要找的是不是：
            {foreach $spellcheck as $word}
                <span class="sel">{$word}</span>
            {/foreach}
        </div>
    {/if}
    <div class="con_width serach_box">
        <div class="all_search_main">
            {if $iColumnPid=="4"}
            <!-- 款式 -->
                <div class="style_box box js-box-style">
                    {if !empty($lists.4.data)}
                    <div class="style_list clearfix" style="padding-top:0;">
                        {foreach $lists.4.data as $item}
                        <div data-url="{$item.detailUrl}" class="style_item js-item">
                            <a href="{$item.detailUrl}" class="img_box" target="_blank">
                                <img src="{$item.cover}">
                                <div class="time"></div>
                                <span>{$item.publish_time}</span>
                                <div class="shuiyin"></div>
                            </a>
                            <div class="js-page-text">
                                <div class="label js-label">
                                    {foreach from=$item.labels key=key  item=val name=loop}
                                    <a href="{$val.lLink}" target="_blank">{$val.name}</a>{if !$smarty.foreach.loop.last}<s></s>{/if}
                                    {/foreach}
                                </div>
                            </div>
                        </div>
                        {/foreach}
                    </div>
                    {if !empty($lists.4.more_url)}
                    <a href="{$lists.4.more_url}" class="more-btn"><span>查看更多<i></i></span></a>
                    {/if}
                    {else}

.top-main{min-height: 800px;background-color: #F7F7F7;}
.top-main .top-head{height: 420px;background: url(../../images/top/data-banner.png) center no-repeat;}
.top-main .top-head .head-title{font-size:60px;font-weight:500;color:#fff;line-height:75px;letter-spacing:1px;text-shadow:0px 2px 8px rgba(234,86,101,0.65);padding-top: 60px;text-align: center;}
.top-main .top-head .head-describe{font-size:42px;font-weight:500;color:#fff;line-height:75px;letter-spacing:1px;text-shadow:0px 2px 8px rgba(234,86,101,0.67);padding-top: 10px;text-align: center;}
.top-main .top-head .head-btn{width:200px;height:50px;line-height: 50px;text-align: center;font-size:22px;font-weight:400;background:#fff;box-shadow:0px 2px 7px 0px rgba(0,0,0,0.15);border-radius:25px;margin: 30px auto 0;color: #EB5C71;cursor: pointer;}
.top-main .top-head .head-btn img{width: 20px;height: 20px;display: inline-block;margin-right: 5px;vertical-align: -2px;}
.top-main .top-describe{height: 682px;background: #fff;padding-top: 40px;}
.top-main .top-describe .describe-main{width: 1200px;margin: 0 auto;}
.top-main .top-describe .describe-main .describe-title{width: 435px;height: 141px;background: url(../../images/top/title-bg.png);margin: 0 auto 40px;font-size:36px;font-weight:bold;color:#1C232B;line-height:141px;letter-spacing:3px;text-align: center;}
.top-main .top-describe .describe-main span{font-size:18px;font-weight:400;color:#4F5964;line-height:30px;letter-spacing:2px;}
.top-main .top-describe .describe-main .explain{margin: 65px auto;}
.top-main .top-describe .describe-main .explain .explain-item{width: 33%;float: left;text-align: center;}
.top-main .top-describe .describe-main .explain .explain-item img{display:block;margin: 0 auto;}
.top-main .top-describe .describe-main .explain .explain-item span{font-size:34px;font-weight:600;color:#4F5964;line-height:48px;letter-spacing:1px;margin: 20px auto 5px;display: block;}
.top-main .top-describe .describe-main .explain .explain-item label{display:block;margin: 0 auto;font-size:18px;font-weight:400;color:#4F5964;line-height:25px;letter-spacing:1px;}
.top-main .top-list{width: 1200px;margin: 0 auto;padding-top: 50px;}
.top-main .top-list .list-head{margin-bottom: 50px;}
.top-main .top-list .list-head span{margin-right: 20px;width:106px;height:44px;border-radius:22px;font-size:22px;font-weight:400;color:#2B2B2B;line-height:44px;text-align: center;display: inline-block;cursor: pointer;}
.top-main .top-list .list-head span.action{color: #fff;background-color: #1C232B;}
.top-main .top-list .list-head span:hover{background-color: #4F5964;color: #fff;}

.top-main .top-list .list{width: 1200px;}
.top-main .top-list .list .list-item{position: relative;width: 210px;height: 280px;margin: 0 37px 50px 0;float: left;}
.top-main .top-list .list .list-item:nth-child(5n){margin-right: 0;}

.anit-section2>.con-lf>ul.small-ul>li.gender-li,.anit-section2>.con-lf>ul.small-ul>li.single-li,.anit-section2>.con-lf>ul.small-ul>li.brand-li{opacity:1;filter:alpha(opacity=100);-webkit-transform:scale(1);transform:scale(1);}

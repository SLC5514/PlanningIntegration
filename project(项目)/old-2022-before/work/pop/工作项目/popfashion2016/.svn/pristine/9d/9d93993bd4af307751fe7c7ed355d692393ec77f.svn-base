{extends file="layout.html"}

{block name="selfcss"}
<link rel="stylesheet" type="text/css" href="{STATIC_URL2}/global/css/home/fashion-home.css?{STATIC_CHANGE_TIME}">
<style>
    .ui-helper-hidden-accessible{
        display: none;
    }
</style>
{/block}
{block name="main"}
<div class="content">
    <!-- 首页banner -->
    {if !empty($banner)}

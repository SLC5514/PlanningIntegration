            <div class="con_width">
                <div class="cont_section_left fl">
                    <div class="cont_title">
                        <h1>{if $allData.sGenderName || $allData.sIndustryName}{$allData.sGenderName} {$allData.sIndustryName}{else}全部{/if}频道</h1>
                        <div class="cont_smalltitle">
                            <img src="{STATIC_URL3}/global/images/channel.png"/>
                        </div>
                    </div>
                    <div class="cont_search" id="jiaocha_search">
                        <input type="text" id ="jiaocha_text" value="时尚资讯，一网打尽！"/>
                        <span class="search_icon" id="go_search11"></span>
                    </div>
                    <div class="hot_search">
                        <ul>
						{foreach $allData.hotWord as $key => $val}
							<li class="{if $key eq 0}first_border{/if} key_search"><a href="javascript:void(0);" title="{$val}"><span>{$val}</span></a></li>
						{/foreach}
                        </ul>
                    </div>
                    <div class="cont_pic_num">
                            <span><i></i>资料总数：<em>{$allData.count.all}</em></span>
                    </div>
                </div>
                <div class="cont_section_right fl">
                    <div class="contain fl">
                        <a {if count($AdData_Nb) > 1}class="lunbbtn_left" {/if} href="javascript:void(0);"></a>
                        <a {if count($AdData_Nb) > 1}class="lunbbtn_right" {/if} href="javascript:void(0);"></a>
                        <ul>
                            {if count($AdData_Nb)}
                            {foreach $AdData_Nb as $val }

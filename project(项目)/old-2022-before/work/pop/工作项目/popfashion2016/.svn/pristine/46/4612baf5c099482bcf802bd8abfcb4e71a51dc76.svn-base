/*搜索*/
.head_li>.total_search{width: 127px;padding: 0;}
.search-box{position:absolute;top: 0;right:0;height:40px;width: 127px;background:#1c232b;padding: 0 10px;box-sizing: border-box;}
.total_search:hover .search-box{background:#13181e;}
.search-box input[type="button"]{background:url(../images/vi_icon.png?2019081301) no-repeat 10px -22px;width:27px;height:16px;position:absolute;right:8px;top:12px;cursor:pointer; border:none;}
.search-box input[type="text"]{width: 110px;height:20px;border:0 none;border-bottom:1px solid #ccc;background:none;color:#fff;font-size:14px;margin-top:10px;padding-right:25px;box-sizing: border-box;}
.search-box input[type="text"]:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: #fff; opacity:1; 
}

.search-box input[type="text"]::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: #fff;opacity:1;
}

.search-box input[type="text"]:-ms-input-placeholder{
    color: #fff;opacity:1;
}

.search-box input[type="text"]::-webkit-input-placeholder{
    color: #fff;opacity:1;
}
.search-box.cur input[type="text"]:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: #999; opacity:1; 
}

.search-box.cur input[type="text"]::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: #999;opacity:1;
}

.search-box.cur input[type="text"]:-ms-input-placeholder{
    color: #999;opacity:1;
}

.search-box.cur input[type="text"]::-webkit-input-placeholder{
    color: #999;opacity:1;
}  
.search_listDown{position:absolute;top:37px;left:0;}
.search_listDown .menu,
.search_listDown .hot-menu{background:#fff;position:relative;overflow:hidden;z-index:110;top:0;left:0;width:317px;box-shadow: 0px 0px 15px 0px rgba(0, 0, 0, 0.09);border-radius: 5px;overflow: hidden;}
.search_listDown .ui-autocomplete,
.search_listDown .menu-hot-list{z-index:12;/* max-height:174px; */overflow:hidden;position:relative;left:0!important;top:0!important;display:none;}
.search_listDown .menu-hot-list{padding-bottom: 15px;/* max-height:none; */}
.search_listDown .menu-hot-list .label,
.search_listDown .menu-hot-list li{color:#333;font-size:14px;height:32px;line-height:32px;padding:0 20px;overflow:hidden;margin: 2px 0;}
.search_listDown .menu-hot-list .label{color:#a2a2a2;font-size:16px;margin: 6px 0;}
.search_listDown .menu-hot-list .label .operat{font-size:14px;cursor: pointer;}
.search_listDown .menu-hot-list .label .operat:hover{color: #333;}
.search_listDown .menu-hot-list .his-clear{display: none;width: 12px;height: 12px;background: url(../images/i-close-12x-a2.png) no-repeat 100% 100%;position: relative;top: 1px;}
.search_listDown .ui-autocomplete li,
.search_listDown .menu-hot-list .ui-menu-item{color:#333;font-size:14px;height:32px;line-height:32px;padding:0 20px;overflow:hidden;cursor:pointer;}
.search_listDown .menu-hot-list .history-list .ui-menu-item{white-space: nowrap;text-overflow: ellipsis;padding-right: 35px;position: relative;}
.search_listDown .menu-hot-list .history-list .ui-menu-item .fr{position: absolute;right: 20px;top: 0;}
.search_listDown .menu-hot-list .hot-list>.loading{text-align: center;color: #a2a2a2;}
.search_listDown .menu-hot-list .hot-list li .serial{display: inline-block;background-color: #f1f1f1;color: #a2a2a2;width: 24px;height: 24px;border-radius: 2px;font-size: 14px;vertical-align: middle;position: relative;top: -2px;margin-right: 8px;text-align: center;line-height: 24px;font-style: normal;}
.search_listDown .menu-hot-list .hot-list li:nth-child(1) .serial{background-color: rgba(223, 60, 114, 0.1);color: rgb(223, 60, 114);}
.search_listDown .menu-hot-list .hot-list li:nth-child(2) .serial{background-color: rgba(76, 84, 254, 0.15);color: rgb(76, 84, 254);}
.search_listDown .menu-hot-list .hot-list li:nth-child(3) .serial{background-color: rgba(55, 188, 168, 0.1);color: rgb(55, 188, 168);}
.search_listDown .menu-hot-list .hot-list li:nth-child(1) .serial::before{content: '1';}
.search_listDown .menu-hot-list .hot-list li:nth-child(2) .serial::before{content: '2';}
.search_listDown .menu-hot-list .hot-list li:nth-child(3) .serial::before{content: '3';}
.search_listDown .menu-hot-list .hot-list li:nth-child(4) .serial::before{content: '4';}
.search_listDown .menu-hot-list .hot-list li:nth-child(5) .serial::before{content: '5';}
.search_listDown .menu-hot-list .hot-list li:nth-child(6) .serial::before{content: '6';}
.search_listDown .menu-hot-list .hot-list li:nth-child(7) .serial::before{content: '7';}
.search_listDown .menu-hot-list .hot-list li:nth-child(8) .serial::before{content: '8';}
.search_listDown .menu-hot-list .hot-list li:nth-child(9) .serial::before{content: '9';}
.search_listDown .menu-hot-list .hot-list li:nth-child(10) .serial::before{content: '10';}

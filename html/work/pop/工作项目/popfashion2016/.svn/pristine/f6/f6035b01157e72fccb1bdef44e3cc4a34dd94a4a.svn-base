.anit-setion-add>div>.con-lf{opacity:1;filter:alpha(opacity=100);transform:translateY(0);-webkit-transform:translateY(0);}
.anit-setion-add>div>.con-rg{opacity:1;filter:alpha(opacity=100);transform:translateY(0);-webkit-transform:translateY(0);}

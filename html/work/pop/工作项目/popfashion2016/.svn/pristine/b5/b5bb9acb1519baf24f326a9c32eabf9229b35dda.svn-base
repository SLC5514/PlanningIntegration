*{padding: 0;margin: 0;}
body{font-size: 18px;}
.mian{width: 100%;background-color: #fff;}
.head{width: 100%;height: 17.35rem;background: url(../images/h5-head-bg.png) no-repeat;background-size:100%;text-align: center;}
.head>span{display: block;font-family: "PingFangSC","微软雅黑", "Verdana", "Geneva", "sans-serif";}
.head>.headSpan{font-size: 1.858rem;padding-top: 9.3rem;font-weight: bold;}
.head>.headSpan2{font-size: 1.7rem;margin: .25rem 0;font-weight: bold;}
.head>.headSpan3{font-size: 1.125rem;}
.head>.headSpan3>i{width: 1.5rem;border-bottom: 2px solid #666;display: inline-block;margin: 0 .575rem;vertical-align: middle;}
.topList{width: 100%;}
.topList>.bnt{background: url(../images/h5-btn.png);height: 1.75rem;width: 7.65rem;margin: 2.5rem auto 2rem;text-align: center;line-height: 1.75rem;font-size: .65rem;background-size: 100% 100%;position: relative;color: #fff;}
.topList>.bnt>.selectTime{position: absolute;top: 1.75rem;left: 0;width: 7.25rem;border: 1px solid #1c232b; border-top:none;z-index: 1;display: none;}
.topList>.bnt>.selectTime>span{display: block;text-align: center;line-height: 1.75rem;height: 1.75rem;width: 100%;  background-image: -moz-linear-gradient( 9deg, rgb(130,163,251) 0%, rgb(165,160,245) 60%, rgb(200,156,238) 100%, rgb(58,161,214) 100%, rgb(200,156,238) 100%);
    background-image: -webkit-linear-gradient( 9deg, rgb(130,163,251) 0%, rgb(165,160,245) 60%, rgb(200,156,238) 100%, rgb(58,161,214) 100%, rgb(200,156,238) 100%);
    background-image: -ms-linear-gradient( 9deg, rgb(130,163,251) 0%, rgb(165,160,245) 60%, rgb(200,156,238) 100%, rgb(58,161,214) 100%, rgb(200,156,238) 100%);}
.topList>.bnt>.selectTime>span:hover{background-image: url(../images/span-bg.png);background-size:100% 100%;}
.topList>.bnt>i{width: .625rem;height: .625rem;background: url(../images/title-icon.png);margin-left: .375rem;display: inline-block;vertical-align: middle;background-size: 100% 100%;}
.topList>ul{margin: 0 auto;clear:both;overflow:hidden;width: 18rem;}
.topList>ul li{width: 8.25rem;height: 8.25rem;float: left;list-style: none;margin: .375rem;position: relative;cursor: pointer;}
.topList>ul li>img{width: 100%;}
.topList>ul li>span{display: inline-block;position:absolute;top: 10px;right: 15px;width: 2.1rem;height: 2.35rem;}
.topList>ul li>.topIcon1{background: url(../images/top-01.png);background-size: 100% 100%;}
.topList>ul li>.topIcon2{background: url(../images/top-02.png);background-size: 100% 100%;}
.topList>ul li>.topIcon3{background: url(../images/top-03.png);background-size: 100% 100%;}
.topList>ul li>.topIcon4{background: url(../images/top-04.png);background-size: 100% 100%;}
.bottomBg{width: 100%;height: 2.8rem;background: url(../images/h5-bottom.png) no-repeat;background-size:100%;}


@media screen and (min-width: 1200px){
    .head{height: 493px;background: url(../images/head-bg.png) no-repeat center;}
    .head>.headSpan{font-size: 72px;padding-top: 106px;margin-right: 600px;}
    .head>.headSpan2{font-size: 65px;margin: 10px 0;margin-right: 600px;}
    .head>.headSpan3{font-size: 37px;margin-right: 600px;}
    .head>.headSpan3>i{width: 80px;border-bottom: 2px solid #666;display: inline-block;margin: 0 20px;vertical-align: middle;}
    .topList>.bnt{background: url(../images/btn.png);height: 54px;width: 266px;margin: 100px auto 60px;line-height: 54px;font-size: 18px;}
    .topList>.bnt>.selectTime{position: absolute;top: 54px;left: 0;width: 251px;}
    .topList>.bnt>.selectTime>span{line-height: 52px;height: 52px;width: 100%;}
    .topList>.bnt>i{width: 22px;height: 22px;margin-left: 10px;margin-top: -2px;}
    .topList>ul{width: 1200px;}
    .topList>ul li{width: 270px;height: 270px;margin: 15px;}
    .topList>ul li>span{width: 80px;height: 93px;}
    .bottomBg{height: 227px;background: url(../images/bottom.png) no-repeat center;}
}
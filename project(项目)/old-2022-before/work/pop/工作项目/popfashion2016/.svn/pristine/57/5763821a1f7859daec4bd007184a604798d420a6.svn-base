.book-buy a{text-align: center; display: block;width: 92px;height: 100%;color: #fff;}
.book-buy>.buy-btn>a>i{vertical-align: middle; width: 20px;height: 20px;margin-right:6px;display: inline-block;*zoom: 1;*display: inline;background:url(../../images/new-detail/report-icons.png) no-repeat -181px -77px;}
.book-buy>.list-back>a>i{vertical-align: middle; width: 20px;height: 20px;margin-right:6px;display: inline-block;*zoom: 1;*display: inline;background:url(../../images/new-detail/report-icons.png) no-repeat -182px -176px;}
.book-buy>div:hover{background: #f4cb69;}

/*快速预览购买*/

.review-contain{height: 749px;background: #151515;}
.review-msg{width: 1200px;margin: 0 auto;position: relative;padding-top: 90px;height: 618px;}

.review-msg>img{display: block;height: 250px; width: auto;margin: 0 auto;}
.review-msg>.review-cont{width: 1200px;height: 260px;position: relative; margin: 50px auto 0;}
.review-msg>.review-cont>.review-list{width: 1052px;position: relative; margin: 0 auto;overflow: hidden;padding: 15px 15px 20px;}
.review-msg>.review-cont>.left-btn{cursor: pointer; position: absolute;z-index: 2;left: 0;top: 93px; width: 40px;height: 40px;background: url(../../../common/images/re-left.png) no-repeat right;}
.review-msg>.review-cont>.left-btn:hover{background: url(../../../common/images/re-left-hv.png) no-repeat right;}
.review-msg>.review-cont>.right-btn{cursor: pointer;position: absolute;z-index: 2;right: 0;top: 93px;width: 40px;height: 40px;background: url(../../../common/images/re-right.png) no-repeat right;}
.review-msg>.review-cont>.right-btn:hover{background: url(../../../common/images/re-right-hv.png) no-repeat right;}

.review-msg ul{text-align: center;font-size: 0;width: 7500px;margin-left: 0;}
.review-msg ul>li{float: left; margin-right: 30px;width: 240px;
			transition: all 0.5s ease ;-webkit-transition: all 0.5s ease ;
        	transform:scale(1);-webkit-transform:scale(1);-moz-transform:scale(1);-o-transform:scale(1);-ms-transform:scale(1);		
		}
.review-msg ul>li:hover{ transform:scale(1.1);-webkit-transform:scale(1.1);-moz-transform:scale(1.1);-o-transform:scale(1.1);-ms-transform:scale(1.1);	}		
		
.review-msg ul>li>a{display: block;height: 260px;width: 100%; color: #fff;font-size: 14px;}
.review-msg ul>li>a img{width: 100%;vertical-align: middle;transition: all 0.3s ease ;-webkit-transition: all 0.5s ease ;}
.img-c{width: 240px;height: 200px;margin: 0 auto;}
.review-msg ul>li>a>p{padding-top: 20px;font-size: 14px;height: 40px;overflow: hidden;}
.review-msg ul>li>a>div{position: relative;}

        var url = location.href;
        var genPattern = /gen_\d+-?/;
        var indPattern = /ind_\d+-?/;
        if (genPattern.test(url)) {
            url = url.replace(genPattern, '');
        }
        if (indPattern.test(url)) {
            url = url.replace(indPattern, '');
        }
        url = url.replace(/\/\/\?/, '/?');
        return url;
    }

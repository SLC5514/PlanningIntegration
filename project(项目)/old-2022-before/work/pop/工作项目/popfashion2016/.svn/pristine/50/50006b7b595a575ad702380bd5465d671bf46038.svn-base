	<!--底部-->
	<div class="detail-footer js-detail-footer max-search2">
		<div class="clearfix">
			<div class="footer-cont">
				<div  class="book-buy clearfix">
					{if $isF5}
					<div class="list-back js-list-back"><a href="javascript:void(0);" title="全部内容" target="_blank"><i></i>全部内容</a></div>
					{/if}
					{if $isBook}
					<div class="buy-btn" id="J_BookBuy"></div>
					{/if}
				</div>
				<!--图片控制器-->
				
				<div class="pic-control-box js-pic-control">
					<div class="pic-control">
						<div class="bp-set js-bp-set">
							<div class="clearfix">
								<div class="disc-icon">
									<p style="padding: 8px 0 12px;margin-top: -2px;">

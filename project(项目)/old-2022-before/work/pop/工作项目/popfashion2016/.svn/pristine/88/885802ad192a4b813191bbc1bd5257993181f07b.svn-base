.p1>.ani3>a,.p1>.ani3>div,.p1>.ani3>img{display: inline-block;color: #fff;*display: inline;*zoom: 1;vertical-align: middle;}
.p1>.ani3>a{width: 120px;height: 85px;padding-top: 35px; font-size: 16px;line-height: 20px;background: url(../images/t3.png?2019070904) center no-repeat;}

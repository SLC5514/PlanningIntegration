.search-section>div{position: relative;overflow: hidden;}

.p1{height: 510px;padding-top: 90px;}
.p1>h2{font-weight: normal; position: relative;top: -160px;opacity: 0;transition: all 0.6s linear;-webkit-transition: all 0.6s linear;-ms-transition: all 0.6s linear;-moz-transition: all 0.6s linear; height: 50px;line-height: 50px; font-size:28px; margin:0 auto; width: 615px;text-align: center;color: #fff;border-bottom: 1px solid #fff;border-top: 1px solid #fff;letter-spacing: 1px;}
.p1>.ani1{position: relative;left: -80%;opacity: 0;transition: all 0.6s linear 0.5s;-webkit-transition: all 0.6s linear 0.5s;-ms-transition: all 0.6s linear 0.5s; width: 620px;height: 170px;background: url(../images/t1.png?2019070904) center top no-repeat;margin: 19px auto 12px;}
.p1>.ani2{position: relative;letter-spacing: 1px; left: 80%;opacity: 0;transition: all 0.6s linear 0.5s;-webkit-transition: all 0.6s linear 0.5s;-ms-transition: all 0.6s linear 0.5s; width: 609px;height: 50px;background: url(../images/t2.png?2019070904) center top no-repeat;margin: 0px auto; margin-bottom:18px;text-align: center;font-size: 24px;line-height: 50px;color: #fff;}

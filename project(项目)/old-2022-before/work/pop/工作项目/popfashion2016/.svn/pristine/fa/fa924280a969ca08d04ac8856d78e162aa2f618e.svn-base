			{/if}
		    {if isset($categorys) && !empty($categorys)}
			<li class="pli">
				<span class="aspan">单品<i class="nav-icon"></i></span>
				<div class="showbox short-sum">
					<div class="dateHeight contentHolder">
						<div class="content">
							{foreach $categorys as $key=>$category}
							<a class="clickme {if $key != 0}borderTop{/if}" href="{$category.link}" title="{$category.name}">{$category.name}</a>
							{/foreach}
						</div>
					</div>
				</div>
			</li>
			{/if}

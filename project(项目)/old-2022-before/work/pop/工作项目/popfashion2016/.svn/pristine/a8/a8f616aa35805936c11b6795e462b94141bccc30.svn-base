.color-click-button>em{background:url(../images/lists/color.png) no-repeat -49px 0;width:10px;height:6px;display:inline-block;vertical-align:middle;margin-left:10px;margin-top:-2px;}
.show-up-hover{background:#d8b056;}
.show-up-hover .color-click-button{background:url(../images/lists/color-hover.png) no-repeat;color:#ab7b10;}
.show-up-hover .color-click-button>i{background-position:-23px 0;}
.show-up-hover .color-click-button>em{background-position:-68px 0;}
.show-up .color-click-button>em{background-position:-49px -13px;}
.show-up.show-up-hover .color-click-button>em{background-position:-68px -13px;}
